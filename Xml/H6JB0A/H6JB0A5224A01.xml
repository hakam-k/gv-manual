<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "H6JB0A5224A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
A fogaskerék-kapcsolat szétcsúszik
</title>
<diag type = "condition">
<condition>
<ptxt>
A fogaskerék-kapcsolat szétcsúszik
</ptxt>
</condition>
<ps>
<ptxt>
Kopott kapcsolóvilla-tengely
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki.
</ptxt>
</action>
<ps>
<ptxt>
Kopott kapcsolóvilla vagy szinkronizálóhüvely
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki.
</ptxt>
</action>
<ps>
<ptxt>
Gyenge vagy sérült rögzítőrugó
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki.
</ptxt>
</action>
<ps>
<ptxt>
A behajtótengelyen, az előtéttengelyen vagy a főtengelyen lévő csapágyak elhasználódtak
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki.
</ptxt>
</action>
<ps>
<ptxt>
Kopott leélezett fog a karmantyún vagy a fogaskeréken
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a karmantyút és a fogaskereket.
</ptxt>
</action>
<ps>
<ptxt>
Hiányzó vagy kiakadt rögzítőgyűrű(k)
</ptxt>
</ps>
<action>
<ptxt>
Szerelje fel, vagy cserélje ki.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

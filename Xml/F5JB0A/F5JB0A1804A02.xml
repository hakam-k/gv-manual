<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "F5JB0A1804A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Economie de carburant ou performances du moteur faibles
</title>
<diag type = "condition">
<condition>
<ptxt>
Economie de carburant ou performances du moteur faibles
</ptxt>
</condition>
<ps>
<ptxt>
Avance à l&apos;allumage incorrecte
</ptxt>
</ps>
<action>
<ptxt>
Vérifier les détecteurs concernés et la plaque du détecteur CKP.
</ptxt>
</action>
<ps>
<ptxt>
Bougie(s) d&apos;allumage ou câble(s) haute tension défectueux (pour le moteur M16)
</ptxt>
</ps>
<action>
<ptxt>
Régler, nettoyer ou remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Bobine d&apos;allumage complète défectueuse
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Détecteur CKP ou plaque du détecteur CKP défectueux
</ptxt>
</ps>
<action>
<ptxt>
Nettoyer, serrer ou remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Détecteur CMP ou dent du rotor du détecteur de l&apos;arbre à cames défectueux
</ptxt>
</ps>
<action>
<ptxt>
Nettoyer, serrer ou remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Détecteur de cliquetis défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
<ps>
<ptxt>
ECM défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

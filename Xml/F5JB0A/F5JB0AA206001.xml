<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "10B">
<servcat sctnbr = "10" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "F5JB0AA206001" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Dépose et repose du BCM
</title>
<servinfosub id = "F5JB0AA206001001">
<title>
Dépose
</title>
<s1>
<ptxt>
Débrancher le câble négatif de la batterie.
</ptxt>
</s1>
<s1>
<ptxt>
Déposer le cache (1) d&#8217;orifice de colonne de direction.
</ptxt>
</s1>
<s1>
<ptxt>
Débrancher les connecteurs du BCM.
</ptxt>
</s1>
<s1>
<ptxt>
Déposer le BCM (2).
</ptxt>
<figure>
<graphic graphicname = "I5JB0AA20022.swf" width = "88.000mm" height = "83.258mm" ver = "01" gw = "165" gh = "219"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "F5JB0AA206001002">
<title>
Repose
</title>
<ptxt>
Procéder dans l&apos;ordre inverse de la dépose en tenant compte des points suivants.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Brancher les connecteurs fermement jusqu&apos;à ce qu&apos;il s&apos;engagent.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "2">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1326009" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
ECT (Engine Coolant Temperature) Sensor Removal and Installation
</title>
<servinfosub id = "S8T40A1326009001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable from battery.
</ptxt>
</s1>
<s1>
<ptxt>
Drain coolant. 
<intxreftitleoff1 refid = "S8T40A1626004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect connector from ECT sensor.
</ptxt>
</s1>
<s1>
<ptxt>
Remove ECT sensor (1).
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1326009002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1326010">ECT (Engine Coolant Temperature) Sensor Inspection:J24B</intxrefpoff>
</ptxt>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Check O-ring for damage and replace if necessary.
</ptxt>
</item>
<item>
<ptxt>
Tighten ECT sensor to specified torque.
</ptxt>
<torque>
<torqueitem id = "T1C213">
<t-part>
ECT sensor
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
12
</t-value1>
<t-value2>
1.2
</t-value2>
<t-value3>
9.0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I8T401132012.swf" width = "88.000mm" height = "45.511mm" ver = "01" gw = "143" gh = "112"/>
</figure>
</item>
<item>
<ptxt>
Connect connector to ECT sensor.
</ptxt>
</item>
<item>
<ptxt>
Refill cooling system. 
<intxreftitleoff1 refid = "S8T40A1626005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
<item>
<ptxt>
Check for coolant leakage. 
<intxreftitleoff1 refid = "S8T40A1626003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

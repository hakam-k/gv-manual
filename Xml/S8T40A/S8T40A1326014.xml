<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "2">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1326014" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
CMP (Camshaft Position) Sensor Removal and Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1326028">CMP (Camshaft Position) Sensor On-Vehicle Inspection:J24B</intxrefpoff>
</ptxt>
<servinfosub id = "S8T40A1326014001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable from battery.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect connector from CMP sensor.
</ptxt>
</s1>
<s1>
<ptxt>
Remove CMP sensor from cylinder head.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1326014002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1326015">CMP (Camshaft Position) Sensor Inspection:J24B</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Apply engine oil to O-ring of CMP sensor (1).
</ptxt>
</item>
<item>
<ptxt>
Tighten CMP sensor bolt to specified torque.
</ptxt>
</item>
</list1>
<torque>
<torqueitem id = "T1C216">
<t-part>
CMP sensor bolt 
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
11
</t-value1>
<t-value2>
1.1
</t-value2>
<t-value3>
8.5
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I8T401132017.swf" width = "88.000mm" height = "54.683mm" ver = "01" gw = "155" gh = "138"/>
</figure>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "4">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1446004" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Air Cleaner Assembly Removal and Installation
</title>
<servinfosub id = "S8T40A1446004001" page = "2">
<title>
Removal
</title>
<s1 page = "2">
<ptxt>
Disconnect negative cable from battery.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Disconnect MAF sensor connector (1) and remove MAF sensor (2).
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Disconnect air cleaner outlet hose (3) from air cleaner assembly (4).
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Remove air cleaner assembly.
</ptxt>
<figure>
<graphic graphicname = "I9T401141005.swf" width = "88.000mm" height = "51.156mm" ver = "01" gw = "183" gh = "128"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1446004002" page = "2">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure for installation noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Tighten MAF sensor bolt to specified torque.
</ptxt>
</item>
</list1>
<torque>
<torqueitem id = "T1D484">
<t-part>
MAF sensor bolt
</t-part>
<t-value1>
3
</t-value1>
<t-value2>
0.3
</t-value2>
<t-value3>
2.5
</t-value3>
</torqueitem>
</torque>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

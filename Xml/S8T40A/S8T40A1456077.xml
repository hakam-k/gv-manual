<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "5">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1456077" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Vacuum Pump Removal and Installation
</title>
<servinfosub id = "S8T40A1456077001" page = "2">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative (&#8211;) cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove engine cover.
</ptxt>
</s1>
<s1>
<ptxt>
Remove air intake pipe referring to 
<intxref refid = "S8T40A1456074">Air intake Pipe Removal and Installation:F9Q</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect vacuum hose (1) from vacuum pump (2).
</ptxt>
</s1>
<s1>
<ptxt>
Remove vacuum pump from cylinder head.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B120002.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "161" gh = "127"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1456077002" page = "2">
<title>
Installation
</title>
<s1>
<ptxt>
Install vacuum pump (1) to cylinder head with new gasket.
</ptxt>
<torque>
<torqueitem id = "T1D5126">
<t-part>
Vacuum pump bolt
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
23
</t-value1>
<t-value2>
2.3
</t-value2>
<t-value3>
17.0
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Connect vacuum hose (2) to vacuum pump.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B120003.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "161" gh = "127"/>
</figure>
</s1>
<s1>
<ptxt>
Install air intake pipe referring to 
<intxref refid = "S8T40A1456074">Air intake Pipe Removal and Installation:F9Q</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Install engine cover.
</ptxt>
</s1>
<s1>
<ptxt>
Connect negative (&#8211;) cable at battery.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

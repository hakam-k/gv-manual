<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3B">
<servcat sctnbr = "03" scnbr = "02">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A3214A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Oil leakage
</title>
<diag type = "condition">
<condition>
<ptxt>
Oil leakage
</ptxt>
</condition>
<ps>
<ptxt>
Clogged breather plug
</ptxt>
</ps>
<action>
<ptxt>
Clean.
</ptxt>
</action>
<ps>
<ptxt>
Worn or damaged oil seal
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
<ps>
<ptxt>
Excessive oil
</ptxt>
</ps>
<action>
<ptxt>
Adjust oil level referring to 
<intxref refid = "S8T40A3216001">Front Differential Oil Change:Front</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Loose differential carrier bolts
</ptxt>
</ps>
<action>
<ptxt>
Replace or retighten.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

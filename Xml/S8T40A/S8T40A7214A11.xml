<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A7214A11" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
A loud rattle is heard at low engine rpm
</title>
<servinfosub id = "S8T40A7214011001">
<title>
Abnormal Noise from Compressor
</title>
<diag type = "condition">
<condition>
<ptxt>
A loud rattle is heard at low engine rpm
</ptxt>
</condition>
<ps>
<ptxt>
Loose compressor armature plate bolt
</ptxt>
</ps>
<action>
<paragroup>
<ptxt>
Retighten armature plate bolt.
</ptxt>
<ptxt>
Replace compressor if it was operated in this condition for a long time.
</ptxt>
</paragroup>
</action>
</diag>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

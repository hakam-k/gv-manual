<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "2">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1826001" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Spark Plug Removal and Installation
</title>
<servinfosub id = "S8T40A1826001001">
<title>
Removal
</title>
<s1>
<ptxt>
Remove ignition coil assemblies. 
<intxreftitleoff1 refid = "S8T40A1826003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Remove spark plugs from cylinder head.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1826001002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1826002">Spark Plug Inspection:J24B</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Tighten spark plugs to specified torque.
</ptxt>
</item>
</list1>
<torque>
<torqueitem id = "T1H204">
<t-part>
Spark plug
</t-part>
<t-value1>
25
</t-value1>
<t-value2>
2.5
</t-value2>
<t-value3>
18.5
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Install ignition coil assemblies. 
<intxreftitleoff1 refid = "S8T40A1826003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

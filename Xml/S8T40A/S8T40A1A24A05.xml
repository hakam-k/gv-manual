<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1J">
<servcat sctnbr = "01" scnbr = "10">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A1A24A05" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Noisy generator
</title>
<ptxt>
Trouble in charging system will show up as one or more of the following conditions.
</ptxt>
<servinfosub id = "S8T40A1A24002005">
<title>
Noise
</title>
<diag type = "condition">
<condition>
<ptxt>
Noisy generator
</ptxt>
</condition>
<ps>
<ptxt>
Mounting bolts loose
</ptxt>
</ps>
<action>
<ptxt>
Tighten mounting bolts. 
</ptxt>
</action>
<ps>
<ptxt>
Bearings dirty, worn or faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair or replace generator. 
<intxreftitleoff1 refid = "S8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Rotor, diode or stator faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair or replace generator. 
<intxreftitleoff1 refid = "S8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

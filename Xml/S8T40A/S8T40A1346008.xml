<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "4">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1346008" svcinfotypenbr = "08" svcinfoqualnbr = "03" page = "2" supplyoff = "1" modify = "0">
<title>
Glow Plug Inspection
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1346007">Glow Plug Removal and Installation:F9Q</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Disconnect negative (&#8211;) cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove injector cover referring to 
<intxref refid = "S8T40A1456048">Cylinder Head Cover Removal and Installation:F9Q</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect glow plug wires.
</ptxt>
</s1>
<s1>
<ptxt>
Check resistance between glow plug and engine ground.
</ptxt>
<ptxt>
If resistance is out of specification, replace glow plug.
</ptxt>
<spec>
<title>
Glow plug resistance
</title>
<ptxt>
Approx. 0.6 &#937;
</ptxt>
</spec>
<figure>
<graphic graphicname = "I4SN0A132006.swf" width = "88.000mm" height = "49.349mm" ver = "01" gw = "129" gh = "123"/>
</figure>
</s1>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1E">
<servcat sctnbr = "01" scnbr = "05">
<configtype typenbr = "4">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1546005" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Oil Pump Removal and Installation
</title>
<servinfosub id = "S8T40A1546005001" page = "2">
<title>
Removal
</title>
<s1 page = "2">
<ptxt>
Remove engine assembly from vehicle. 
<intxreftitleoff1 refid = "S8T40A1446012"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Remove cylinder head cover. 
<intxreftitleoff1 refid = "S8T40A1446010"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Remove generator bracket. 
<intxreftitleoff1 refid = "S8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Remove timing chain cover. 
<intxreftitleoff1 refid = "S8T40A1446014"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Remove 1st timing chain, timing chain lower guide and crankshaft timing sprocket. 
<intxreftitleoff1 refid = "S8T40A1446051"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Remove oil pump (1).
</ptxt>
<figure>
<graphic graphicname = "I8T401151024.swf" width = "88.000mm" height = "67.976mm" ver = "01" gw = "183" gh = "176"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1546005002" page = "2">
<title>
Installation
</title>
<s1 page = "2">
<ptxt>
Install oil pump (1).
</ptxt>
<torque>
<torqueitem id = "T1E446">
<t-part>
Oil pump bolt
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
23
</t-value1>
<t-value2>
2.3
</t-value2>
<t-value3>
17.0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I8T401151025.swf" width = "88.000mm" height = "66.452mm" ver = "01" gw = "206" gh = "171"/>
</figure>
</s1>
<s1 page = "2">
<ptxt>
Install 1st timing chain. 
<intxreftitleoff1 refid = "S8T40A1446051"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Install timing chain cover. 
<intxreftitleoff1 refid = "S8T40A1446014"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Install generator bracket. 
<intxreftitleoff1 refid = "S8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Install OCV. 
<intxreftitleoff1 refid = "S8T40A1446047"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Install P&#x002F;S pump bracket. 
<intxreftitleoff1 refid = "S8T40A6306020"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Install cylinder head cover. 
<intxreftitleoff1 refid = "S8T40A1446010"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Install accessory drive belt. 
<intxreftitleoff1 refid = "S8T40A1A26004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Install engine assembly from vehicle. 
<intxreftitleoff1 refid = "S8T40A1446012"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

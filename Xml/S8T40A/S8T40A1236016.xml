<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1B">
<servcat sctnbr = "01" scnbr = "02">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1236016" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
EVAP Canister Removal and Installation
</title>
<servinfosub id = "S8T40A1236016001">
<title>
Removal
</title>
<s1>
<ptxt>
Hoist vehicle.
</ptxt>
</s1>
<s1>
<ptxt>
Remove EVAP canister (1) and its bracket (2) from vehicle.
</ptxt>
<figure page = "2">
<graphic graphicname = "I8T401121007.swf" width = "88.000mm" height = "52.214mm" ver = "01" gw = "168" gh = "131"/>
</figure>
</s1>
<s1>
<ptxt>
Remove suction hose and purge hose from EVAP canister.
</ptxt>
</s1>
<s1>
<ptxt>
Remove EVAP canister from bracket.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1236016002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure.
</ptxt>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

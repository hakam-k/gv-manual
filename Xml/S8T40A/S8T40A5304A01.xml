<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A5304A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Slipping
</title>
<diag type = "condition">
<condition>
<ptxt>
Slipping
</ptxt>
</condition>
<ps>
<ptxt>
Improper clutch pedal free travel
</ptxt>
</ps>
<action>
<ptxt>
Bleed air or replace master cylinder.
</ptxt>
</action>
<ps>
<ptxt>
Worn or oily clutch disc facing
</ptxt>
</ps>
<action>
<ptxt>
Replace disc.
</ptxt>
</action>
<ps>
<ptxt>
Warped disc, pressure plate or flywheel surface
</ptxt>
</ps>
<action>
<ptxt>
Replace disc, clutch cover or flywheel.
</ptxt>
</action>
<ps>
<ptxt>
Weakened diaphragm spring
</ptxt>
</ps>
<action>
<ptxt>
Replace clutch cover.
</ptxt>
</action>
<ps>
<ptxt>
Master cylinder piston or seal cup not returning
</ptxt>
</ps>
<action>
<ptxt>
Repair master cylinder.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

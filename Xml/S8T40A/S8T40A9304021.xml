<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A9304021" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Remote Audio Control Switch Symptom Diagnosis (If Equipped)
</title>
<diag type = "condition" id = "S8T40A9304021001">
<condition>
<ptxt>
Audio system is operative, but remote control switch does not control audio system
</ptxt>
</condition>
<ps>
<ptxt>
Remote audio control switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check remote audio control switch referring to 
<intxref refid = "S8T40A9306026">Remote Audio Control Switch Inspection (If Equipped)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Contact coil faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace contact coil.
</ptxt>
</action>
<ps>
<ptxt>
Wiring and&#x002F;or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Radio assembly faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace radio assembly.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

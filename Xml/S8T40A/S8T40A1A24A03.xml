<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1J">
<servcat sctnbr = "01" scnbr = "10">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A1A24A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Undercharged battery
</title>
<ptxt>
Trouble in charging system will show up as one or more of the following conditions.
</ptxt>
<servinfosub id = "S8T40A1A24002003">
<title>
Battery Charging Failure
</title>
<diag type = "condition">
<condition>
<ptxt>
Undercharged battery
</ptxt>
</condition>
<ps>
<ptxt>
Accessory drive belt loose or worn
</ptxt>
</ps>
<action>
<ptxt>
Replace accessory drive belt. 
<intxreftitleoff1 refid = "S8T40A1A26004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Battery terminals loose or corroded 
</ptxt>
</ps>
<action>
<ptxt>
Tighten battery terminals or replace battery. 
<intxreftitleoff1 refid = "S8T40A1A26002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Rectifier, rotor or IC regulator faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair or replace generator. 
<intxreftitleoff1 refid = "S8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

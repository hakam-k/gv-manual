<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "4">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1346001" svcinfotypenbr = "08" svcinfoqualnbr = "03" page = "2" supplyoff = "1" modify = "0">
<title>
Idle Speed Inspection
</title>
<s1>
<ptxt>
Shift transmission into Neutral.
</ptxt>
</s1>
<s1>
<ptxt>
Start engine and warm it up to normal operating temperature.
</ptxt>
</s1>
<s1>
<ptxt>
Turn all electrical loads OFF.
</ptxt>
</s1>
<s1>
<ptxt>
Using SUZUKI scan tool, verify that idle speed is within specification.
</ptxt>
<spec-sdm>
<sdm-title>
Engine idle speed
</sdm-title>
<sdm1>
<sdm-condition>
</sdm-condition>
<sdm-value>
750 &#8211; 850 rpm
</sdm-value>
</sdm1>
</spec-sdm>
</s1>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

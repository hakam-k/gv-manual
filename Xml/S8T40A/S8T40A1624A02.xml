<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A1624A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Engine overheats (Radiator fan does not operate)
</title>
<diag type = "condition">
<condition>
<ptxt>
Engine overheats (Radiator fan does not operate)
</ptxt>
</condition>
<ps>
<ptxt>
Fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Check fuse. 
</ptxt>
</action>
<ps>
<ptxt>
Radiator cooling fan relay faulty
</ptxt>
</ps>
<action>
<ptxt>
Check radiator cooling fan relay. 
<intxreftitleoff1 refid = "S8T40A1626021"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
ECT sensor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check ECT sensor. 
<intxreftitleoff1 refid = "S8T40A1326010"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Radiator cooling fan motor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check radiator cooling fan motor. 
<intxreftitleoff1 refid = "S8T40A1626009"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair as necessary.
</ptxt>
</action>
<ps>
<ptxt>
ECM faulty
</ptxt>
</ps>
<action>
<ptxt>
Check ECM and its circuit. 
<intxreftitleoff1 refid = "S8T40A1124063"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

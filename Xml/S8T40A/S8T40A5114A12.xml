<diag type = "condition">
<condition>
<ptxt>
Excessive &#8220;N&#8221; &#8594; &#8220;D&#8221; or &#8220;N&#8221; &#8594; &#8220;R&#8221; time lag
</ptxt>
</condition>
<ps>
<ptxt>

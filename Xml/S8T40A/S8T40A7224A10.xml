<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A7224A10" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
A loud noise is heard at a certain rpm, disproportionately to engine revolution
</title>
<servinfosub id = "S8T40A7224012001">
<title>
Abnormal Noise from Compressor
</title>
<diag type = "condition">
<condition>
<ptxt>
A loud noise is heard at a certain rpm, disproportionately to engine revolution
</ptxt>
</condition>
<ps>
<ptxt>
Loose or faulty accessory (A&#x002F;C compressor) drive belt
</ptxt>
</ps>
<action>
<ptxt>
Replace drive belt and&#x002F;or belt tensioner as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Loose compressor mounting bolts
</ptxt>
</ps>
<action>
<ptxt>
Retighten mounting bolts.
</ptxt>
</action>
</diag>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

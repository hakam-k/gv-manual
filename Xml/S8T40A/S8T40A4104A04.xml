<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "4A">
<servcat sctnbr = "04" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A4104A04" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Excessive pedal travel
</title>
<diag type = "condition">
<condition>
<ptxt>
Excessive pedal travel
</ptxt>
<ptxt>
(Pedal stroke too large)
</ptxt>
</condition>
<ps>
<ptxt>
Partial brake system failure
</ptxt>
</ps>
<action>
<ptxt>
Check brake systems and repair as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Leaking brake fluid 
</ptxt>
</ps>
<action>
<ptxt>
Locate leaking point and repair. Then air bleed.
</ptxt>
</action>
<ps>
<ptxt>
Air in system (pedal soft &#x002F; spongy)
</ptxt>
</ps>
<action>
<ptxt>
Bleed system.
</ptxt>
</action>
<ps>
<ptxt>
Rear brake system not adjusted (malfunctioning auto adjusting mechanism) (rear drum brake model)
</ptxt>
</ps>
<action>
<ptxt>
Repair auto adjusting mechanism. Adjust rear brakes.
</ptxt>
</action>
<ps>
<ptxt>
Bent brake shoes (rear drum brake model)
</ptxt>
</ps>
<action>
<ptxt>
Replace brake shoes.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

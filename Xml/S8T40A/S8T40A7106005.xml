<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7A">
<servcat sctnbr = "07" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A7106005" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Blower Motor Removal and Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A7106001">Blower Unit and Heater Unit Components</intxrefpoff>
</ptxt>
<servinfosub id = "S8T40A7106005001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative (&#8211;) cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Disable air bag system. 
<intxreftitleoff1 refid = "S8T40A8206001"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect blower motor lead wire (2) at coupler.
</ptxt>
</s1>
<s1>
<ptxt>
Remove blower motor (1) from HVAC unit.
</ptxt>
</s1>
<figure>
<graphic graphicname = "I9T401710001.swf" width = "88.000mm" height = "46.922mm" ver = "03" gw = "182" gh = "116"/>
</figure>
</servinfosub>
<servinfosub id = "S8T40A7106005002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A7106006">Blower Motor Inspection</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Reverse removal procedure for installation.
</ptxt>
</s1>
<s1>
<ptxt>
Enable air bag system. 
<intxreftitleoff1 refid = "S8T40A8206002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

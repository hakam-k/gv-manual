<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1836001" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Spark Plug Removal and Installation
</title>
<servinfosub id = "S8T40A1836001001">
<title>
Removal
</title>
<s1>
<ptxt>
Remove ignition coil assemblies. 
<intxreftitleoff1 refid = "S8T40A1836003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Remove spark plugs.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1836001002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1836002">Spark Plug Inspection:N32A</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Install spark plugs and tighten them to specified torque.
</ptxt>
<spec-sdm>
<sdm-title>
Spark plug type
</sdm-title>
<sdm1>
<sdm-condition>
ACDelco
</sdm-condition>
<sdm-value>
41-990 (platinum)
</sdm-value>
</sdm1>
</spec-sdm>
<torque>
<torqueitem id = "T1H306">
<t-part>
Spark plug
</t-part>
<t-value1>
18
</t-value1>
<t-value2>
1.8
</t-value2>
<t-value3>
13.5
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Install ignition coil assemblies. 
<intxreftitleoff1 refid = "S8T40A1836003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1336009" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Engine Coolant Temperature (ECT) Sensor Removal and Installation
</title>
<servinfosub id = "S8T40A1336009001" page = "2">
<title>
Removal
</title>
<s1 page = "2">
<ptxt>
Disconnect negative cable from battery.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Drain coolant. 
<intxreftitleoff1 refid = "S8T40A1636004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Remove engine cover.
</ptxt>
</s1>
<s1>
<ptxt>
Remove oil level gauge and its guide.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Disconnect ECT sensor connector (1).
</ptxt>
<figure page = "2">
<graphic graphicname = "I8T401131012.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "170" gh = "124"/>
</figure>
</s1>
<s1 page = "2">
<ptxt>
Remove ECT sensor from cylinder head.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1336009002" page = "2">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Tighten new ECT sensor to specified torque.
</ptxt>
<torque>
<torqueitem id = "T1C321">
<t-part>
ECT sensor
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
22
</t-value1>
<t-value2>
2.2
</t-value2>
<t-value3>
16.5
</t-value3>
</torqueitem>
</torque>
<figure page = "2">
<graphic graphicname = "I8T401131013.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "172" gh = "124"/>
</figure>
</item>
<item>
<ptxt>
Refill cooling system. 
<intxreftitleoff1 refid = "S8T40A1636017"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
<item>
<ptxt>
Check for coolant leakage from ECT sensor with engine running.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

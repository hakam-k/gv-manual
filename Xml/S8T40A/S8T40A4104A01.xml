<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "4A">
<servcat sctnbr = "04" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A4104A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Not enough braking force
</title>
<diag type = "condition">
<condition>
<ptxt>
Not enough braking force
</ptxt>
</condition>
<ps>
<ptxt>
Brake fluid leakage from brake lines
</ptxt>
</ps>
<action>
<ptxt>
Locate leaking point and repair.
</ptxt>
</action>
<ps>
<ptxt>
Brake disc or pads stained with fluid, grease or other oils
</ptxt>
</ps>
<action>
<ptxt>
Clean or replace.
</ptxt>
</action>
<ps>
<ptxt>
Overheated brakes
</ptxt>
</ps>
<action>
<ptxt>
Determine cause and repair.
</ptxt>
</action>
<ps>
<ptxt>
Poor contact of brake shoes on brake drum (rear drum brake model)
</ptxt>
</ps>
<action>
<ptxt>
Repair for proper contact.
</ptxt>
</action>
<ps>
<ptxt>
Brake shoes stained with fluid or wet with water (rear drum brake model)
</ptxt>
</ps>
<action>
<ptxt>
Clean or replace.
</ptxt>
</action>
<ps>
<ptxt>
Badly worn brake Pad
</ptxt>
</ps>
<action>
<ptxt>
Replace brake shoe.
</ptxt>
</action>
<ps>
<ptxt>
Defective wheel cylinders (rear drum brake model)
</ptxt>
</ps>
<action>
<ptxt>
Repair or replace.
</ptxt>
</action>
<ps>
<ptxt>
Malfunctioning caliper assembly
</ptxt>
</ps>
<action>
<ptxt>
Repair or replace.
</ptxt>
</action>
<ps>
<ptxt>
Air in system
</ptxt>
</ps>
<action>
<ptxt>
Bleed system.
</ptxt>
</action>
<ps>
<ptxt>
Malfunctioning ABS (ESP&#174;)
</ptxt>
</ps>
<action>
<ptxt>
Check system and replace as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Malfunctioning brake booster 
</ptxt>
</ps>
<action>
<ptxt>
Check system and replace as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Malfunctioning brake master cylinder 
</ptxt>
</ps>
<action>
<ptxt>
Check system and replace as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Negative pressure for brake booster is inadequate
</ptxt>
</ps>
<action>
<ptxt>
Check engine condition and vacuum hose.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

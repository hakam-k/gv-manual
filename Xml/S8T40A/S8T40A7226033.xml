<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "2">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A7226033" svcinfotypenbr = "08" svcinfoqualnbr = "03" page = "2" supplyoff = "1" modify = "0">
<title>
Compressor Assembly On-Vehicle Inspection
</title>
<s1>
<ptxt>
Install manifold gauge set (1) as shown in figure.
</ptxt>
</s1>
<s1>
<ptxt>
Close Hi (4) and Lo (5) side valves.
</ptxt>
<figure>
<graphic graphicname = "I5JB0A720059.swf" width = "88.000mm" height = "85.375mm" ver = "01" gw = "192" gh = "225"/>
<callout>
<table frame = "all" colsep = "1" rowsep = "1">
<tgroup cols = "2" colsep = "1" rowsep = "1">
<colspec colnum = "1" colname = "1" colwidth = "0.191in"/>
<colspec colnum = "2" colname = "2" colwidth = "1.911in"/>
<tbody>
<row rowsep = "1">
<entry colname = "1" border-top = "1" border-bottom = "1" border-left = "1" border-right = "0" align = "right">
<ptxt>
2.
</ptxt>
</entry>
<entry colname = "2" border-top = "1" border-bottom = "1" border-left = "0" border-right = "1">
<ptxt>
High pressure side (Delivery side hose)
</ptxt>
</entry>
</row>
<row rowsep = "0">
<entry colname = "1" border-top = "1" border-bottom = "1" border-left = "1" border-right = "0" align = "right">
<ptxt>
3.
</ptxt>
</entry>
<entry colname = "2" border-top = "1" border-bottom = "1" border-left = "0" border-right = "1">
<ptxt>
Low pressure side (Suction side pipe)
</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</callout>
</figure>
</s1>
<s1>
<ptxt>
Run engine at fast idle.
</ptxt>
</s1>
<s1>
<ptxt>
Check compressor for the following items. 
</ptxt>
<ptxt>
If defect is found, repair compressor.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
High pressure gauge reading is not low and low pressure gauge reading is not higher than normal.
</ptxt>
</item>
<item>
<ptxt>
Metallic sound
</ptxt>
</item>
<item>
<ptxt>
Leakage from compressor
</ptxt>
</item>
</list1>
</s1>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

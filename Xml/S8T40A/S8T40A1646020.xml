<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "4">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1646020" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Water Pump Removal and Installation
</title>
<servinfosub id = "S8T40A1646020001" page = "2">
<title>
Removal
</title>
<s1>
<ptxt>
Drain coolant referring to 
<intxref refid = "S8T40A1646004">Cooling System Draining:F9Q</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Remove timing belt referring to 
<intxref refid = "S8T40A1456051">Timing Belt and Belt Tensioner Removal and Installation:F9Q</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Remove water pump assembly (1).
</ptxt>
<figure>
<graphic graphicname = "I5JB0B160008.swf" width = "88.000mm" height = "54.175mm" ver = "01" gw = "162" gh = "137"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1646020002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1646017">Water Pump Inspection:F9Q</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Remove oil and dust from sealing surface of cylinder block and water pump.
</ptxt>
</s1>
<s1>
<ptxt>
Apply sealant to three bolts (1).
</ptxt>
<materials>
<materialitem name = "Loctite FRENETANCH&reg;" id = "M1F402" uid = "1083042972">
<m-part>
&#8220;A&#8221;
</m-part>
<m-desc>
Thread lock cement
</m-desc>
<m-number>
</m-number>
<m-name>
Loctite FRENETANCH&#174;
</m-name>
<ptxt>
</ptxt>
</materialitem>
</materials>
</s1>
<s1>
<ptxt>
Install water pump assembly (2) with new gasket.
</ptxt>
<torque>
<torqueitem id = "T1F409">
<t-part>
Water pump assembly bolt
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
10
</t-value1>
<t-value2>
1.0
</t-value2>
<t-value3>
7.5
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I5JB0B160009.swf" width = "88.000mm" height = "53.794mm" ver = "01" gw = "187" gh = "135"/>
</figure>
</s1>
<s1>
<ptxt>
Install timing belt referring to 
<intxref refid = "S8T40A1456051">Timing Belt and Belt Tensioner Removal and Installation:F9Q</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Refill coolant referring to 
<intxref refid = "S8T40A1646005">Cooling System Refilling:F9Q</intxref>
.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

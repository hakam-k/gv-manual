<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A9204A15" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Optical axes of both headlights do not change
</title>
<diag type = "condition">
<condition>
<ptxt>
Optical axes of both headlights do not change
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Headlight manual leveling switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check headlight manual leveling switch referring to 
<intxref refid = "S8T40A9206022">Headlight Manual Leveling Switch Inspection (If Equipped)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Supply voltage too low
</ptxt>
</ps>
<action>
<ptxt>
Check charging system referring to 
<intxref refid = "S8T40A1A24003">Generator Test:J24B and N32A</intxref>
 &#x002F; 
<intxref refid = "S8T40A1A14003">Generator Test (Undercharged Battery Check):M16A and J20A</intxref>
 or 
<intxref refid = "S8T40A1A34003">Generator Test (Undercharged Battery Check):F9Q</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

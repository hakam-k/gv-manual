<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9I">
<servcat sctnbr = "09" scnbr = "09">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A9904001" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Power Sliding (Sun) Roof System Symptom Diagnosis
</title>
<diag type = "condition" id = "S8T40A9904001001">
<condition>
<ptxt>
Sliding roof does not operate (sliding roof motor runs OK)
</ptxt>
</condition>
<ps>
<ptxt>
Foreign object stuck in sliding roof rail
</ptxt>
</ps>
<action>
<ptxt>
Remove or clean stuck object.
</ptxt>
</action>
<ps>
<ptxt>
Mis-installation of sliding roof rail
</ptxt>
</ps>
<action>
<ptxt>
Install correctly.
</ptxt>
</action>
<ps>
<ptxt>
Conflicts of sliding roof parts
</ptxt>
</ps>
<action>
<ptxt>
Fix conflicts.
</ptxt>
</action>
<ps>
<ptxt>
Mis-installation of sliding roof drive cable
</ptxt>
</ps>
<action>
<ptxt>
Install correctly.
</ptxt>
</action>
<condition>
<ptxt>
Sliding roof does not operate (sliding roof motor does not run)
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse to check for short.
</ptxt>
</action>
<ps>
<ptxt>
Sliding roof switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check sliding roof switch referring to 
<intxref refid = "S8T40A9906004">Sliding (Sun) Roof Switch Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Check circuit referring to 
<intxref refid = "S8T40A9906006">Sliding (Sun) Roof System Circuit and Motor Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Sliding roof motor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check sliding roof motor referring to 
<intxref refid = "S8T40A9906006">Sliding (Sun) Roof System Circuit and Motor Inspection</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1436051" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
OCV (Oil Control Valve) Removal and Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1436013">Timing Chain Cover Components:J24B</intxrefpoff>
</ptxt>
<servinfosub id = "S8T40A1436051001">
<title>
Removal
</title>
<ptxt>
Remove OCV (2) and O-ring (3) from timing chain cover (1).
</ptxt>
<figure>
<graphic graphicname = "I8T401142066.swf" width = "88.000mm" height = "38.456mm" ver = "01" gw = "164" gh = "92"/>
</figure>
</servinfosub>
<servinfosub id = "S8T40A1436051002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Install new O-ring.
</ptxt>
</item>
<item>
<ptxt>
Tighten OCV bolt to specified torque.
</ptxt>
<torque>
<torqueitem id = "T1D370">
<t-part>
OCV bolt
</t-part>
<t-value1>
11
</t-value1>
<t-value2>
1.1
</t-value2>
<t-value3>
8.5
</t-value3>
</torqueitem>
</torque>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

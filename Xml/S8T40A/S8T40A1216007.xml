<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1B">
<servcat sctnbr = "01" scnbr = "02">
<configtype typenbr = "1">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1216007" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
EGR Valve Removal and Installation
</title>
<servinfosub id = "S8T40A1216007001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove EGR pipe.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect EGR valve connector.
</ptxt>
</s1>
<s1>
<ptxt>
Remove EGR valve and gasket from cylinder head.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1216007002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1216008">EGR Valve Inspection:M16A and J20A</intxrefpoff>
</ptxt>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Clean mating surface of valve and cylinder head.
</ptxt>
</item>
<item>
<ptxt>
Use new gaskets.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "2">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1326018" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Knock Sensor Removal and Installation
</title>
<servinfosub id = "S8T40A1326018001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable from battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove intake manifold. 
<intxreftitleoff1 refid = "S8T40A1436008"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect knock sensor connector.
</ptxt>
</s1>
<s1>
<ptxt>
Remove knock sensor (1) from cylinder block.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1326018002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S8T40A1326027">Knock Sensor Inspection:J24B</intxrefpoff>
</ptxt>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Install knock sensor (1) as shown in figure.
</ptxt>
</item>
<item>
<ptxt>
Tighten knock sensor bolt (2) to specified torque.
</ptxt>
</item>
</list1>
<torque>
<torqueitem id = "T1C218">
<t-part>
Knock sensor bolt
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
25
</t-value1>
<t-value2>
2.5
</t-value2>
<t-value3>
18.5
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I8T401132021.swf" width = "88.000mm" height = "90.314mm" ver = "03" gw = "160" gh = "239"/>
<callout>
<table frame = "all" colsep = "1" rowsep = "1">
<tgroup cols = "2" colsep = "1" rowsep = "1">
<colspec colnum = "1" colname = "1" colwidth = "0.315in"/>
<colspec colnum = "2" colname = "2" colwidth = "0.583in"/>
<tbody>
<row rowsep = "0">
<entry colname = "1" border-top = "1" border-bottom = "1" border-left = "1" border-right = "0" align = "right">
<ptxt>
&#8220;a&#8221;:
</ptxt>
</entry>
<entry colname = "2" border-top = "1" border-bottom = "1" border-left = "0" border-right = "1" align = "left">
<ptxt>
0 &#8211; 90&#176;
</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</callout>
</figure>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

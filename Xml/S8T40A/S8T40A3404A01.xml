<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3D">
<servcat sctnbr = "03" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A3404A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Abnormal noise
</title>
<diag type = "condition">
<condition>
<ptxt>
Abnormal noise
</ptxt>
</condition>
<ps>
<ptxt>
Loose propeller shaft flange bolt and nut
</ptxt>
</ps>
<action>
<ptxt>
Tighten propeller shaft flange bolt and nut. 
<intxreftitleoff1 refid = "S8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Spider bearing worn out or stuck
</ptxt>
</ps>
<action>
<ptxt>
Replace. 
<intxreftitleoff1 refid = "S8T40A3406003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Wear spider
</ptxt>
</ps>
<action>
<ptxt>
Replace propeller shaft. 
<intxreftitleoff1 refid = "S8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A9304A13" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Sound volume does not vary according to changes of vehicle speed
</title>
<servinfosub id = "S8T40A9304020005">
<title>
Auto Volume Control System
</title>
<diag type = "condition">
<condition>
<ptxt>
Sound volume does not vary according to changes of vehicle speed
</ptxt>
</condition>
<ps>
<ptxt>
Auto volume control system is &#8220;OFF&#8221; mode
</ptxt>
</ps>
<action>
<ptxt>
Select auto volume control
</ptxt>
</action>
<ps>
<ptxt>
Vehicle speed signal faulty
</ptxt>
</ps>
<action>
<ptxt>
Check vehicle speed signal referring to 
<intxref refid = "S8T40A9306027">Vehicle Speed Signal Inspection (For Audio Unit) (If Equipped)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
<ps>
<ptxt>
BCM faulty
</ptxt>
</ps>
<action>
<ptxt>
Check input and output signal of BCM referring to 
<intxref refid = "S8T40AA204019">Inspection of BCM and Its Circuits</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Radio assembly faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace radio assembly.
</ptxt>
</action>
</diag>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "2">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A1426001" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Air Cleaner Assembly Removal and Installation
</title>
<servinfosub id = "S8T40A1426001001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect breather hose (1) from air cleaner outlet hose (2).
</ptxt>
</s1>
<s1>
<ptxt>
Remove air cleaner outlet hose (2).
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect MAF sensor connector (3).
</ptxt>
</s1>
<s1>
<ptxt>
Remove air cleaner case (4).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A142003.swf" width = "88.000mm" height = "46.922mm" ver = "01" gw = "154" gh = "116"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S8T40A1426001002">
<title>
Installation
</title>
<s1>
<ptxt>
Install air cleaner case (4).
</ptxt>
</s1>
<s1>
<ptxt>
Connect MAF sensor connector (3).
</ptxt>
</s1>
<s1>
<ptxt>
Install air cleaner outlet hose (2).
</ptxt>
</s1>
<s1>
<ptxt>
Connect breather hose (1) to air cleaner outlet hose (2).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A142003.swf" width = "88.000mm" height = "46.922mm" ver = "01" gw = "154" gh = "116"/>
</figure>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

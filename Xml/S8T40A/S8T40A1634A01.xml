<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "3">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A1634A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Engine overheats (Radiator fan operates) 
</title>
<diag type = "condition">
<condition>
<ptxt>
Engine overheats (Radiator fan operates) 
</ptxt>
</condition>
<ps>
<ptxt>
Loose or broken accessory drive belt
</ptxt>
</ps>
<action>
<ptxt>
Adjust or replace accessory drive belt. 
<intxreftitleoff1 refid = "S8T40A1A26008"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Not enough coolant
</ptxt>
</ps>
<action>
<ptxt>
Check coolant level and add as necessary. 
<intxreftitleoff1 refid = "S8T40A1636002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Faulty thermostat
</ptxt>
</ps>
<action>
<ptxt>
Replace thermostat assembly. 
<intxreftitleoff1 refid = "S8T40A1636008"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Faulty water pump
</ptxt>
</ps>
<action>
<ptxt>
Replace water pump. 
<intxreftitleoff1 refid = "S8T40A1636016"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Dirty or bent radiator fins
</ptxt>
</ps>
<action>
<ptxt>
Clean or repair. 
<intxreftitleoff1 refid = "S8T40A1636013"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Coolant leakage on cooling system
</ptxt>
</ps>
<action>
<ptxt>
Repair cooling system. 
<intxreftitleoff1 refid = "S8T40A1636003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Clogged radiator
</ptxt>
</ps>
<action>
<ptxt>
Check and replace radiator as necessary. 
<intxreftitleoff1 refid = "S8T40A1636014"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Faulty radiator cap
</ptxt>
</ps>
<action>
<ptxt>
Check radiator cap. 
<intxreftitleoff1 refid = "S8T40A1636003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Dragging brakes
</ptxt>
</ps>
<action>
<ptxt>
Adjust brake. 
<intxreftitleoff1 refid = "S8T40A4104002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Poorly charged battery
</ptxt>
</ps>
<action>
<ptxt>
Check and replace battery as necessary. 
<intxreftitleoff1 refid = "S8T40A1A24001"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Poor electricity generation by generator
</ptxt>
</ps>
<action>
<ptxt>
Check and repair generator as necessary. 
<intxreftitleoff1 refid = "S8T40A1A24002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
ECT sensor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check and replace ECT sensor as necessary. 
<intxreftitleoff1 refid = "S8T40A1336010"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Radiator cooling fan motor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check and replace radiator cooling fan motor as necessary. 
<intxreftitleoff1 refid = "S8T40A1636009"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
ECM faulty
</ptxt>
</ps>
<action>
<ptxt>
Check and replace ECM as necessary. 
<intxreftitleoff1 refid = "S8T40A1134063"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Too much additional electrical equipment
</ptxt>
</ps>
<action>
<ptxt>
Dismount necessary pieces of equipment.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A9204A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Only one low beam does not light up
</title>
<diag type = "condition">
<condition>
<ptxt>
Only one low beam does not light up
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Bulb faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace bulb.
</ptxt>
</action>
<ps>
<ptxt>
Igniter faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace igniter.
</ptxt>
</action>
<ps>
<ptxt>
Ballast faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace ballast.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or ground faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S8T40A9306028" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Center Speaker Removal and Installation (If Equipped) 
</title>
<servinfosub id = "S8T40A9306028001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative (&#8211;) cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove audio unit referring to 
<intxref refid = "S8T40A9306016">Audio Unit Removal and Installation</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Remove center ventilation louver garnish (1).
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect hazard warning switch connector.
</ptxt>
</s1>
<s1>
<ptxt>
Remove center speaker (2) and disconnect center speaker connector.
</ptxt>
<figure>
<graphic graphicname = "I8T40A930009.swf" width = "88.000mm" height = "62.445mm" ver = "01" gw = "238" gh = "160"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S8T40A9306028002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure.
</ptxt>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

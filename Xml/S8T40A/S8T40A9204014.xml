<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S8T40A9204014" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Rear Fog Light Symptom Diagnosis (If Equipped)
</title>
<diag type = "condition" id = "S8T40A9204014001">
<condition>
<ptxt>
Rear fog light do not light
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Bulb blown
</ptxt>
</ps>
<action>
<ptxt>
Replace bulb.
</ptxt>
</action>
<ps>
<ptxt>
Rear fog light switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check rear fog light switch referring to 
<intxref refid = "S8T40A9206024">Rear Fog Light Switch Inspection (If Equipped)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

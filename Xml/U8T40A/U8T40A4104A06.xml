<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "4A">
<servcat sctnbr = "04" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "U8T40A4104A06" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Pedaal pulseert
</title>
<diag type = "condition">
<condition>
<ptxt>
Pedaal pulseert
</ptxt>
<ptxt>
(Het rempedaal pulseert bij het indrukken)
</ptxt>
</condition>
<ps>
<ptxt>
Beschadigde of losse wiellagers
</ptxt>
</ps>
<action>
<ptxt>
Vervang de wielnaaf.
</ptxt>
</action>
<ps>
<ptxt>
Vervormde fusee van stuuras of achterwielophanging
</ptxt>
</ps>
<action>
<ptxt>
Vervang de fusee.
</ptxt>
</action>
<ps>
<ptxt>
Overmatige laterale schijfslingering
</ptxt>
</ps>
<action>
<ptxt>
Controleer volgens de aanwijzingen. Vervang of bewerk de schijf als ze niet binnen het voorgeschreven bereik valt.
</ptxt>
</action>
<ps>
<ptxt>
Achterwielremtrommel niet rond
</ptxt>
</ps>
<action>
<ptxt>
Controleer de slingering Repareer of vervang de trommel zoals vereist.
</ptxt>
</action>
<ps>
<ptxt>
Remzadel loopt vast
</ptxt>
</ps>
<action>
<ptxt>
Controleer de glijpen van het remzadel.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

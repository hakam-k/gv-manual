</torque>
<figure>
<graphic graphicname = "I8T401142120.swf" width = "88.000mm" height = "53.272mm" ver = "01" gw = "178" gh = "134"/>
</figure>
</s1>
<s1>
<ptxt>
Monteer de P&#x002F;S-pomp. 
<intxreftitleoff1 refid = "U8T40A6306022"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Monteer de waterpomp. 
<intxreftitleoff1 refid = "U8T40A1626015"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Monteer de dynamo. 
<intxreftitleoff1 refid = "U8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>

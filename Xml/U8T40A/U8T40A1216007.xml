<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1B">
<servcat sctnbr = "01" scnbr = "02">
<configtype typenbr = "1">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "U8T40A1216007" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Uit- en inbouw van de EGR-klep
</title>
<servinfosub id = "U8T40A1216007001">
<title>
Uitbouw
</title>
<s1>
<ptxt>
Maak de negatieve accukabel los.
</ptxt>
</s1>
<s1>
<ptxt>
Verwijder de EGR-leiding.
</ptxt>
</s1>
<s1>
<ptxt>
Maak de stekkerverbinding van de EGR-klep los.
</ptxt>
</s1>
<s1>
<ptxt>
Verwijder de EGR-klep en de pakking van de cilinderkop.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "U8T40A1216007002">
<title>
Inbouw
</title>
<ptxt>
<intxrefpoff refid = "U8T40A1216008">Onderzoek van de EGR-klep:M16A and J20A</intxrefpoff>
</ptxt>
<ptxt>
Keer de uitbouwprocedure om en neem daarbij de volgende punten in acht.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Reinig het pasvlak van de klep en de cilinderkop.
</ptxt>
</item>
<item>
<ptxt>
Gebruik nieuwe pakkingen.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "U8T40A5224A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Ruw schakelen
</title>
<diag type = "condition">
<condition>
<ptxt>
Ruw schakelen
</ptxt>
</condition>
<ps>
<ptxt>
Onjuiste vrije slag van het koppelingspedaal
</ptxt>
</ps>
<action>
<ptxt>
Vervang de pedaalarm en&#x002F;of de hoofdkoppelingscilinder.
</ptxt>
</action>
<ps>
<ptxt>
Vervormde of kapotte koppelingsplaat
</ptxt>
</ps>
<action>
<ptxt>
Vervangen.
</ptxt>
</action>
<ps>
<ptxt>
Beschadigde koppelingsdrukplaat
</ptxt>
</ps>
<action>
<ptxt>
Vervang het koppelingshuisdeksel.
</ptxt>
</action>
<ps>
<ptxt>
Lucht in het hydraulische systeem van de koppeling
</ptxt>
</ps>
<action>
<ptxt>
Ontlucht het systeem.
</ptxt>
</action>
<ps>
<ptxt>
Vloeistoflekkage uit de koppelingsvloeistofleiding
</ptxt>
</ps>
<action>
<ptxt>
Zoek en repareer het lek.
</ptxt>
</action>
<ps>
<ptxt>
Versleten symchromesh-ring
</ptxt>
</ps>
<action>
<ptxt>
Vervangen.
</ptxt>
</action>
<ps>
<ptxt>
Versleten afgeschuinde tand op mof of tandwiel
</ptxt>
</ps>
<action>
<ptxt>
Vervang de mof en het tandwiel.
</ptxt>
</action>
<ps>
<ptxt>
Vervormde schakelas
</ptxt>
</ps>
<action>
<ptxt>
Vervangen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

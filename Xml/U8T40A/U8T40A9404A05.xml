<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "U8T40A9404A05" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Storing van de koplampsproeier
</title>
<diag type = "condition">
<condition>
<ptxt>
Storing van de koplampsproeier
</ptxt>
</condition>
<ps>
<ptxt>
Verstopte sproeierslang of -kop
</ptxt>
</ps>
<action>
<ptxt>
Reinig of repareer de verstopte slang of kop
</ptxt>
</action>
<ps>
<ptxt>
Circuitzekering gesmolten
</ptxt>
</ps>
<action>
<ptxt>
Vervang de zekering en controleer op kortsluiting.
</ptxt>
</action>
<ps>
<ptxt>
Koplampsproeierschakelaar defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de koplampsproeierschakelaar volgens 
<intxref refid = "U8T40A9406014">Onderzoek van de koplampsproeierschakelaar (indien uitgerust)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Defecte combischakelaar (sproeierschakelaar)
</ptxt>
</ps>
<action>
<ptxt>
Controleer de sproeierschakelaar volgens 
<intxref refid = "U8T40A9406009">Onderzoek van de voorruitwisser- en -sproeierschakelaar</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Koplampsproeierpomp defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de koplampsproeierpomp volgens 
<intxref refid = "U8T40A9406003">Onderzoek van de ruitensproeierpomp</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Bedrading of massaverbinding defect
</ptxt>
</ps>
<action>
<ptxt>
Repareer het circuit.
</ptxt>
</action>
<ps>
<ptxt>
Regelapparaat van de koplampsproeier defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer het regelapparaat van de koplampsproeier volgens 
<intxref refid = "U8T40A9404005">Onderzoek van het regelapparaat van de koplampsproeier en het bijbehorende circuit (indien uitgerust)</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

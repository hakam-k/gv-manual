<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1E">
<servcat sctnbr = "01" scnbr = "05">
<configtype typenbr = "5">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "U8T40A1556006" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Uitbouw en inbouw van de warmtewisselaar
</title>
<servinfosub id = "U8T40A1556006001">
<title>
Uitbouw
</title>
<s1>
<ptxt>
Verwijder de oliefilter volgens 
<intxref refid = "U8T40A0206006">Verversen van de motorolie en vervangen van de filter</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Maak de waterslangen (1) los van de warmtewisselaar (2).
</ptxt>
</s1>
<s1>
<ptxt>
Verwijder de warmtewisselaar (2).
</ptxt>
<figure>
<graphic graphicname = "I5JB0B150041.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "179" gh = "124"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "U8T40A1556006002">
<title>
Inbouw
</title>
<s1>
<ptxt>
Monteer de warmtewisselaar (1) met een nieuwe pakking.
</ptxt>
<torque>
<torqueitem id = "T1E562">
<t-part>
Bout van de oliefiltervoet
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
15
</t-value1>
<t-value2>
1,5
</t-value2>
<t-value3>
11,0
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Sluit de waterslangen (2) aan op de warmtewisselaar.
</ptxt>
</s1>
<s1>
<ptxt>
Monteer de oliefilter volgens 
<intxref refid = "U8T40A0206006">Verversen van de motorolie en vervangen van de filter</intxref>
.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B150042.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "179" gh = "124"/>
</figure>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

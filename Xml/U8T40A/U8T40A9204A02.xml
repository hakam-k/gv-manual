<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "U8T40A9204A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Dimlicht brandt niet
</title>
<diag type = "condition">
<condition>
<ptxt>
Dimlicht brandt niet
</ptxt>
</condition>
<ps>
<ptxt>
Circuitzekeringen gesprongen
</ptxt>
</ps>
<action>
<ptxt>
Vervang de zekeringen en controleer op kortsluiting.
</ptxt>
</action>
<ps>
<ptxt>
Dimlichtrelais van de koplamp defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer het koplamprelais volgens 
<intxref refid = "U8T40A9206008">Onderzoek van het achterlichtrelais en koplamprelais</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Verlichtings- of dimlichtschakelaar defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de licht- en dimlichtschakelaar volgens 
<intxref refid = "U8T40A9206007">Onderzoek van de koplampschakelaar (in de lichtschakelaar)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Gloeilampen defect
</ptxt>
</ps>
<action>
<ptxt>
Vervang de gloeilampen.
</ptxt>
</action>
<ps>
<ptxt>
Ontstekers defect
</ptxt>
</ps>
<action>
<ptxt>
Vervang de ontstekers.
</ptxt>
</action>
<ps>
<ptxt>
Ballasten defect
</ptxt>
</ps>
<action>
<ptxt>
Vervang de ballasten.
</ptxt>
</action>
<ps>
<ptxt>
Bedrading of massaverbinding defect
</ptxt>
</ps>
<action>
<ptxt>
Repareer het circuit.
</ptxt>
</action>
<ps>
<ptxt>
BCM defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de werking van de BCM volgens 
<intxref refid = "U8T40AA204019">Onderzoek van de BCM en de bijbehorende circuits</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

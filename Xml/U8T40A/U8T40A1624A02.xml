<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "U8T40A1624A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Motor raakt oververhit (radiateurventilator werkt niet)
</title>
<diag type = "condition">
<condition>
<ptxt>
Motor raakt oververhit (radiateurventilator werkt niet)
</ptxt>
</condition>
<ps>
<ptxt>
Zekering gesmolten
</ptxt>
</ps>
<action>
<ptxt>
Controleer de zekering. 
</ptxt>
</action>
<ps>
<ptxt>
Radiateurkoelventilatorrelais defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer het radiateurkoelventilatorrelais. 
<intxreftitleoff1 refid = "U8T40A1626021"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
ECT-sensor defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de ECT-sensor. 
<intxreftitleoff1 refid = "U8T40A1326010"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Radiateurkoelventilatormotor defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de radiateurkoelventilatormotor. 
<intxreftitleoff1 refid = "U8T40A1626009"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Bedrading of massaverbinding defect
</ptxt>
</ps>
<action>
<ptxt>
Repareer zoals vereist.
</ptxt>
</action>
<ps>
<ptxt>
ECM defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de ECM en het bijbehorende circuit. 
<intxreftitleoff1 refid = "U8T40A1124063"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "2">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "U8T40A1326009" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Uit- en inbouw van de ECT-sensor (motorkoelvloeistoftemperatuursensor)
</title>
<servinfosub id = "U8T40A1326009001">
<title>
Uitbouw
</title>
<s1>
<ptxt>
Maak de negatieve kabel los van de accu.
</ptxt>
</s1>
<s1>
<ptxt>
Tap de koelvloeistof af. 
<intxreftitleoff1 refid = "U8T40A1626004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Maak de stekkerverbinding van de ECT-sensor los.
</ptxt>
</s1>
<s1>
<ptxt>
Verwijder de ECT-sensor (1).
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "U8T40A1326009002">
<title>
Inbouw
</title>
<ptxt>
<intxrefpoff refid = "U8T40A1326010">Onderzoek van de ECT-sensor (motorkoelvloeistoftemperatuursensor):J24B</intxrefpoff>
</ptxt>
<ptxt>
Keer de uitbouwprocedure om en neem daarbij de volgende punten in acht.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Controleer de O-ring op beschadiging en vernieuw de ring indien nodig.
</ptxt>
</item>
<item>
<ptxt>
Draai de ECT-sensor met het voorgeschreven koppel aan.
</ptxt>
<torque>
<torqueitem id = "T1C213">
<t-part>
ECT-sensor
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
12
</t-value1>
<t-value2>
1,2
</t-value2>
<t-value3>
9,0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I8T401132012.swf" width = "88.000mm" height = "45.511mm" ver = "01" gw = "143" gh = "112"/>
</figure>
</item>
<item>
<ptxt>
Sluit de stekkerverbinding op de ECT-sensor aan.
</ptxt>
</item>
<item>
<ptxt>
Vul het koelsysteem bij. 
<intxreftitleoff1 refid = "U8T40A1626005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
<item>
<ptxt>
Controleer op koelvloeistoflekkage. 
<intxreftitleoff1 refid = "U8T40A1626003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "U8T40A9404A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Ruitenwisserstoringen
</title>
<diag type = "condition">
<condition>
<ptxt>
Ruitenwisserstoringen
</ptxt>
</condition>
<ps>
<ptxt>
Circuitzekering gesmolten
</ptxt>
</ps>
<action>
<ptxt>
Vervang de zekering en controleer op kortsluiting.
</ptxt>
</action>
<ps>
<ptxt>
Defecte wissermotor
</ptxt>
</ps>
<action>
<ptxt>
Controleer de wissermotor volgens 
<intxref refid = "U8T40A9406005">Onderzoek van de voorruitwissermotor</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Defecte combischakelaar (ruitenwisserschakelaar)
</ptxt>
</ps>
<action>
<ptxt>
Controleer de ruitenwisserschakelaar volgens 
<intxref refid = "U8T40A9406009">Onderzoek van de voorruitwisser- en -sproeierschakelaar</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Bedrading of massaverbinding defect
</ptxt>
</ps>
<action>
<ptxt>
Repareer het circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

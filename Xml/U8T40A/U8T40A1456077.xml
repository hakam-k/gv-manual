<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "5">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "U8T40A1456077" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Uitbouw en inbouw van de vacuümpomp
</title>
<servinfosub id = "U8T40A1456077001" page = "2">
<title>
Uitbouw
</title>
<s1>
<ptxt>
Maak de negatieve (-) kabel los van de accu.
</ptxt>
</s1>
<s1>
<ptxt>
Verwijder de motorbedekking.
</ptxt>
</s1>
<s1>
<ptxt>
Verwijder de luchtinlaatleiding volgens 
<intxref refid = "U8T40A1456074">Uitbouw en inbouw van de luchtinlaatleiding:F9Q</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Maak de vacuümslang (1) los van de vacuümpomp (2).
</ptxt>
</s1>
<s1>
<ptxt>
Verwijder de vacuümpomp van de cilinderkop.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B120002.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "161" gh = "127"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "U8T40A1456077002" page = "2">
<title>
Inbouw
</title>
<s1>
<ptxt>
Monteer de vacuümpomp (1) op de cilinderkop met de nieuwe pakking.
</ptxt>
<torque>
<torqueitem id = "T1D5126">
<t-part>
Vacuümpompbout
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
23
</t-value1>
<t-value2>
2,3
</t-value2>
<t-value3>
17,0
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Sluit de vacuümslang (2) aan op de vacuümpomp.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B120003.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "161" gh = "127"/>
</figure>
</s1>
<s1>
<ptxt>
Monteer de luchtinlaatleiding volgens 
<intxref refid = "U8T40A1456074">Uitbouw en inbouw van de luchtinlaatleiding:F9Q</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Monteer de motorbedekking.
</ptxt>
</s1>
<s1>
<ptxt>
Sluit de negatieve kabel van de accu weer aan.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

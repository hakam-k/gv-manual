<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "U8T40A9404A04" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Ruitensproeierstoringen
</title>
<diag type = "condition">
<condition>
<ptxt>
Ruitensproeierstoringen
</ptxt>
</condition>
<ps>
<ptxt>
Verstopte sproeierslang of -kop
</ptxt>
</ps>
<action>
<ptxt>
Reinig of repareer de verstopte slang of kop
</ptxt>
</action>
<ps>
<ptxt>
Circuitzekering gesmolten
</ptxt>
</ps>
<action>
<ptxt>
Vervang de zekering en controleer op kortsluiting.
</ptxt>
</action>
<ps>
<ptxt>
Sproeiermotor defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de sproeiermotor volgens 
<intxref refid = "U8T40A9406003">Onderzoek van de ruitensproeierpomp</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Defecte combischakelaar (sproeierschakelaar)
</ptxt>
</ps>
<action>
<ptxt>
Controleer de sproeierschakelaar volgens 
<intxref refid = "U8T40A9406011">Onderzoek van de achterruitwisser- en -sproeierschakelaar</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Bedrading of massaverbinding defect
</ptxt>
</ps>
<action>
<ptxt>
Repareer het circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "U8T40A9204012" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Diagnose van de remlichtsymptomen
</title>
<diag type = "condition" id = "U8T40A9204012001">
<condition>
<ptxt>
Remlicht brandt niet
</ptxt>
</condition>
<ps>
<ptxt>
Gloeilamp(en) gesprongen
</ptxt>
</ps>
<action>
<ptxt>
Vervang de gloeilamp(en).
</ptxt>
</action>
<ps>
<ptxt>
Circuitzekering gesmolten
</ptxt>
</ps>
<action>
<ptxt>
Vervang de zekering en controleer op kortsluiting.
</ptxt>
</action>
<ps>
<ptxt>
Remlichtschakelaar defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer de remlichtschakelaar volgens 
<intxref refid = "U8T40A9206011">Onderzoek van de remlichtschakelaar</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Bedrading of massaverbinding defect
</ptxt>
</ps>
<action>
<ptxt>
Repareer het circuit.
</ptxt>
</action>
<condition>
<ptxt>
Remlicht blijft branden
</ptxt>
</condition>
<ps>
<ptxt>
Remlichtschakelaar defect
</ptxt>
</ps>
<action>
<ptxt>
Controleer of regel de remlichtschakelaar, zie 
<intxref refid = "U8T40A9206011">Onderzoek van de remlichtschakelaar</intxref>
 of 
<intxref refid = "U8T40A4106006">Bijstelling van remlichtschakelaar</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<graphic graphicname = "I5JB0A130029.swf" width = "88.000mm" height = "48.686mm" ver = "02" gw = "174" gh = "121"/>
</figure>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0E5304A04" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Kupplung vibriert
</title>
<diag type = "condition">
<condition>
<ptxt>
Kupplung vibriert
</ptxt>
</condition>
<ps>
<ptxt>
Kupplungsscheibenschlag oder mangelhafter Kontakt
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsscheibe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Torsionsfedern der Kupplungsscheibe ermüdet
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsscheibe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Nieten der Kupplungsscheibe locker
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsscheibe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Druckplatte oder Schwungrad verformt
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsdeckel bzw. Schwungrad austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Schrauben oder Muttern der Motortraglager locker
</ptxt>
</ps>
<action>
<ptxt>
Festziehen bzw. Traglager erneuern.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

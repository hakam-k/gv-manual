<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0E9204015" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "0" modify = "0">
<title>
Symptomdiagnose Beleuchtungsausschaltsystem (falls vorhanden)
</title>
<diag type = "condition" id = "G5JB0E9204015001">
<condition>
<ptxt>
Beleuchtungsausschaltsystem funktioniert nicht richtig
</ptxt>
</condition>
<ps>
<ptxt>
Schaltkreissicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Sicherung austauschen und Schaltkreis auf Kurzschluss untersuchen.
</ptxt>
</action>
<ps>
<ptxt>
Beleuchtungsausschalter defekt
</ptxt>
</ps>
<action>
<ptxt>
Den Beleuchtungsausschalter gemäß 
<intxref refid = "G5JB0E9206028">Prüfung Beleuchtungsausschalter (falls vorhanden)</intxref>
 prüfen.
</ptxt>
</action>
<ps>
<ptxt>
Kombinationsinstrument und&#x002F;oder Display (Uhr) defekt
</ptxt>
</ps>
<action>
<ptxt>
Kombinationsinstrument und&#x002F;oder Display (Uhr) austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Schaltkreis reparieren.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

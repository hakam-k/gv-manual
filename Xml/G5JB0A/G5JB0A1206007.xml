<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1B">
<servcat sctnbr = "01" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "G5JB0A1206007" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Aus- und Einbau des EGR-Ventils
</title>
<servinfosub id = "G5JB0A1206007001">
<title>
Ausbau
</title>
<s1>
<ptxt>
Massekabel von Batterie abklemmen.
</ptxt>
</s1>
<s1>
<ptxt>
Die EGR-Leitung abmontieren.
</ptxt>
</s1>
<s1>
<ptxt>
Den Steckverbinder des EGR-Ventils abklemmen.
</ptxt>
</s1>
<s1>
<ptxt>
EGR-Ventil und Dichtung vom Zylinderkopf abnehmen.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "G5JB0A1206007002">
<title>
Einbau
</title>
<ptxt>
<intxrefpoff refid = "G5JB0A1206008">Überprüfung des EGR-Ventils</intxrefpoff>
</ptxt>
<ptxt>
Zum Einbau den Ausbauvorgang unter Beachtung der folgenden Hinweise umkehren.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Die Passflächen von Ventil und Zylinderkopf reinigen.
</ptxt>
</item>
<item>
<ptxt>
Neue Dichtungen verwenden.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

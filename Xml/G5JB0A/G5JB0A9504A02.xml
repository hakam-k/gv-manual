<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9E">
<servcat sctnbr = "09" scnbr = "05">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0A9504A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Alle elektrischen Fensterheber funktionieren nicht
</title>
<diag type = "condition">
<condition>
<ptxt>
Alle elektrischen Fensterheber funktionieren nicht
</ptxt>
</condition>
<ps>
<ptxt>
Schaltkreissicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Sicherung austauschen und auf Kurzschluss prüfen.
</ptxt>
</action>
<ps>
<ptxt>
Hauptschalter der elektrischen Fensterheber defekt
</ptxt>
</ps>
<action>
<ptxt>
Hauptschalter der elektrischen Fensterheber überprüfen. Siehe dazu 
<intxref refid = "G5JB0A9506016">Überprüfung des Hauptschalters der elektrischen Fensterheber</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Zündschalter defekt
</ptxt>
</ps>
<action>
<ptxt>
Zündschalter überprüfen. Siehe dazu 
<intxref refid = "G5JB0A9306002">Überprüfung des Zündschalters</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Schaltkreis reparieren.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "6C">
<servcat sctnbr = "06" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0A6304A07" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Abnormales Geräusch
</title>
<diag type = "condition">
<condition>
<ptxt>
Abnormales Geräusch
</ptxt>
</condition>
<ps>
<ptxt>
Lufteintritt in das System aufgrund von Flüssigkeitsmangel
</ptxt>
</ps>
<action>
<ptxt>
Lenkungsflüssigkeit auffüllen und System entlüften.
</ptxt>
</action>
<ps>
<ptxt>
Eindringen von Luft in die Flüssigkeitsleitungen oder -schläuche
</ptxt>
</ps>
<action>
<ptxt>
Leitungen bzw. Schläuche austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Rutschender (lockerer) Antriebsriemen der Servolenkung
</ptxt>
</ps>
<action>
<ptxt>
Riemenspannung einstellen.
</ptxt>
</action>
<ps>
<ptxt>
Servolenkungs-Antriebsriemen verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Riemen austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Befestigungsschraube des Getriebegehäuses locker
</ptxt>
</ps>
<action>
<ptxt>
Die Schrauben festziehen.
</ptxt>
</action>
<ps>
<ptxt>
Gestänge oder Gelenke locker
</ptxt>
</ps>
<action>
<ptxt>
Nachziehen.
</ptxt>
</action>
<ps>
<ptxt>
Leitungen oder Schläuche scheuern an der Karosserie.
</ptxt>
</ps>
<action>
<ptxt>
Leitungen und Schläuche korrekt einbauen.
</ptxt>
</action>
<ps>
<ptxt>
Flügel der Servolenkungspumpe defekt
</ptxt>
</ps>
<action>
<ptxt>
Defekte Teile austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Defekt des Steuerventils im Lenkgetriebegehäuse
</ptxt>
</ps>
<action>
<ptxt>
Lenkgetriebegehäuse austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Lager der Servolenkungspumpenwelle defekt
</ptxt>
</ps>
<action>
<ptxt>
Lager austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0A9404A04" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Störung der Waschanlage
</title>
<diag type = "condition">
<condition>
<ptxt>
Störung der Waschanlage
</ptxt>
</condition>
<ps>
<ptxt>
Schlauch bzw. Düse der Waschanlage verstopft
</ptxt>
</ps>
<action>
<ptxt>
Schlauch bzw. Düse der Waschanlage reinigen oder reparieren.
</ptxt>
</action>
<ps>
<ptxt>
Schaltkreissicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Sicherung austauschen und auf Kurzschluss prüfen.
</ptxt>
</action>
<ps>
<ptxt>
Waschanlagenmotor defekt
</ptxt>
</ps>
<action>
<ptxt>
Waschanlagenmotor überprüfen. Siehe dazu 
<intxref refid = "G5JB0A9406003">Überprüfung der Waschanlagenpumpe</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Kombinationsschalter (Waschanlagenschalter) defekt
</ptxt>
</ps>
<action>
<ptxt>
Waschanlagenschalter überprüfen. Siehe dazu 
<intxref refid = "G5JB0A9406011">Überprüfung des Schalters der Heckscheibenwischer- und -waschanlage</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Schaltkreis reparieren.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

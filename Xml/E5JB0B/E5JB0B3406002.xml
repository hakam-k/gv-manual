</t-part>
<t-part1>
a
</t-part1>
<t-value1>
30
</t-value1>
<t-value2>
3,0
</t-value2>
<t-value3>
22,0
</t-value3>
</torqueitem>
<torqueitem id = "T3D002">
<t-part>
Tuerca de brida del eje de transmisión trasero
</t-part>
<t-part1>
b
</t-part1>
<t-value1>
85
</t-value1>
<t-value2>
8,5
</t-value2>
<t-value3>
61,5
</t-value3>
</torqueitem>
</torque>

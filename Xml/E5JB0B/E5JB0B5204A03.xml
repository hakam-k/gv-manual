<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0B5204A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Cambio duro
</title>
<diag type = "condition">
<condition>
<ptxt>
Cambio duro
</ptxt>
</condition>
<ps>
<ptxt>
Carrera libre del pedal del embrague, incorrecta
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el brazo del pedal y&#x002F;o el cilindro maestro del embrague.
</ptxt>
</action>
<ps>
<ptxt>
Disco de embrague deformado o roto
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Placa de presión del embrague dañada
</ptxt>
</ps>
<action>
<ptxt>
Cambie la tapa del embrague.
</ptxt>
</action>
<ps>
<ptxt>
Aire en el sistema hidráulico del embrague
</ptxt>
</ps>
<action>
<ptxt>
Purga de aire.
</ptxt>
</action>
<ps>
<ptxt>
Fuga de líquido de la tubería de líquido del embrague
</ptxt>
</ps>
<action>
<ptxt>
Ubique el lugar de la fuga y repare.
</ptxt>
</action>
<ps>
<ptxt>
Anillo sincronizador, desgastado
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Dientes biselados de manguito o engranaje, desgastados
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el manguito y el engranaje.
</ptxt>
</action>
<ps>
<ptxt>
Eje de cambio, deformado
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

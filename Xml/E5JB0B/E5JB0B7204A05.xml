<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0B7204A05" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
El aire fresco no sale sólo de forma intermitente
</title>
<diag type = "condition">
<condition>
<ptxt>
El aire fresco no sale sólo de forma intermitente
</ptxt>
</condition>
<ps>
<ptxt>
Conexión de cableado, defectuosa
</ptxt>
</ps>
<action>
<ptxt>
Repare según sea necesario.
</ptxt>
</action>
<ps>
<ptxt>
Válvula de expansión, defectuosa
</ptxt>
</ps>
<action>
<ptxt>
Revise la válvula de expansión consultando 
<intxref refid = "E5JB0B7206013">Inspección en el vehículo de la válvula de expansión</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Humedad excesiva en el sistema de A&#x002F;C
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el desecante y, a continuación, efectúe el vaciado y la carga consultando 
<intxref refid = "E5JB0B7206001">Procedimiento de carga de refrigerante del A&#x002F;C</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Embrague magnético, defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Revise el embrague magnético consultando 
<intxref refid = "E5JB0B7206039">Inspección del funcionamiento del embrague magnético</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Excesiva cantidad de refrigerante
</ptxt>
</ps>
<action>
<ptxt>
Revise la cantidad de refrigerante.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

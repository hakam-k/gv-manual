<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0B9304004" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "0" modify = "0">
<title>
Diagnóstico de síntomas del medidor de combustible
</title>
<diag type = "condition" id = "E5JB0B9304004001">
<condition>
<ptxt>
El medidor de combustible no funciona o funciona incorrectamente
</ptxt>
</condition>
<ps>
<ptxt>
Fusible de circuito, fundido
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el fusible y compruebe si hay cortocircuito.
</ptxt>
</action>
<ps>
<ptxt>
Sensor de nivel de combustible, defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Revise el sensor del nivel de combustible consultando 
<intxref refid = "E5JB0B9306005">Inspección del sensor de nivel del combustible</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Cableado o conexión a masa, defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Repare el circuito.
</ptxt>
</action>
<ps>
<ptxt>
Juego de instrumentos defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el juego de instrumentos.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0B5304A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Patinaje
</title>
<diag type = "condition">
<condition>
<ptxt>
Patinaje
</ptxt>
</condition>
<ps>
<ptxt>
Carrera libre del pedal del embrague incorrecta
</ptxt>
</ps>
<action>
<ptxt>
Purgue el aire o reemplace el cilindro maestro.
</ptxt>
</action>
<ps>
<ptxt>
Forro del disco de embrague desgastado o manchado de aceite.
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el disco.
</ptxt>
</action>
<ps>
<ptxt>
Superficie del disco, del plato de presión o del volante de inercia deformada
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el disco, la tapa del embrague o el volante.
</ptxt>
</action>
<ps>
<ptxt>
Muelle del diafragma debilitado
</ptxt>
</ps>
<action>
<ptxt>
Sustituya la tapa del embrague.
</ptxt>
</action>
<ps>
<ptxt>
El pistón de cilindro maestro o la copa del sello no retornan.
</ptxt>
</ps>
<action>
<ptxt>
Repare el cilindro maestro.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

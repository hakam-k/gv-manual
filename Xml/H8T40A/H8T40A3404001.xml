<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3D">
<servcat sctnbr = "03" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "H8T40A3404001" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Kardántengely hibajelenségeinek diagnosztizálása
</title>
<diag type = "condition" id = "H8T40A3404001001">
<condition>
<ptxt>
Rendellenes zaj
</ptxt>
</condition>
<ps>
<ptxt>
Laza a kardántengely karimás csavarja és anyája
</ptxt>
</ps>
<action>
<ptxt>
Húzza meg a kardántengely karimás csavarját és anyáját. 
<intxreftitleoff1 refid = "H8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
A kardánkereszt-csapágy elhasználódott vagy beragadt
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki. 
<intxreftitleoff1 refid = "H8T40A3406003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Kopott kardánkereszt
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a kardántengelyt. 
<intxreftitleoff1 refid = "H8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<condition>
<ptxt>
Rezgés
</ptxt>
</condition>
<ps>
<ptxt>
Deformált kardántengely
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki. 
<intxreftitleoff1 refid = "H8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

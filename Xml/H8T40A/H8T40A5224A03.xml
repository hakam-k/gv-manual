<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "H8T40A5224A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Nehéz kapcsolás
</title>
<diag type = "condition">
<condition>
<ptxt>
Nehéz kapcsolás
</ptxt>
</condition>
<ps>
<ptxt>
Nem megfelelő a tengelykapcsoló pedál holtjátéka
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a pedálkart és&#x002F;vagy a tengelykapcsoló-főhengert.
</ptxt>
</action>
<ps>
<ptxt>
Elgörbült vagy törött tengelykapcsoló-tárcsa
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki.
</ptxt>
</action>
<ps>
<ptxt>
Sérült a tengelykapcsoló nyomólapja
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a tengelykapcsoló-fedelet.
</ptxt>
</action>
<ps>
<ptxt>
A tengelykapcsoló hidraulikarendszere levegős
</ptxt>
</ps>
<action>
<ptxt>
Végezze el a légtelenítést.
</ptxt>
</action>
<ps>
<ptxt>
Folyadék szivárog a tengelykapcsoló-folyadék vezetékéből
</ptxt>
</ps>
<action>
<ptxt>
Keresse meg a szivárgás helyét és javítsa ki.
</ptxt>
</action>
<ps>
<ptxt>
Kopott szinkrongyűrű
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki.
</ptxt>
</action>
<ps>
<ptxt>
Kopott leélezett fog a karmantyún vagy a fogaskeréken
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a karmantyút és a fogaskereket.
</ptxt>
</action>
<ps>
<ptxt>
Deformálódott kapcsolótengely
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "F8T40A9204012" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Diagnostic des symptômes de dysfonctionnement des feux stop
</title>
<diag type = "condition" id = "F8T40A9204012001">
<condition>
<ptxt>
Les feux stop ne s&apos;allument pas
</ptxt>
</condition>
<ps>
<ptxt>
Ampoules(s) grillée(s)
</ptxt>
</ps>
<action>
<ptxt>
Remplacer la ou les ampoules.
</ptxt>
</action>
<ps>
<ptxt>
Fusible du circuit grillé
</ptxt>
</ps>
<action>
<ptxt>
Remplacer le fusible et rechercher les courts-circuits éventuels.
</ptxt>
</action>
<ps>
<ptxt>
Contacteur de feux stop défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier le contacteur de feux stop en se reportant au point 
<intxref refid = "F8T40A9206011">Vérification du contacteur de feux stop</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Câblage ou mise à la masse défectueux
</ptxt>
</ps>
<action>
<ptxt>
Réparer le circuit.
</ptxt>
</action>
<condition>
<ptxt>
Les feux stop restent allumés
</ptxt>
</condition>
<ps>
<ptxt>
Contacteur de feux stop défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier ou régler le contacteur de feux stop en se reportant aux points 
<intxref refid = "F8T40A9206011">Vérification du contacteur de feux stop</intxref>
 ou 
<intxref refid = "F8T40A4106006">Réglage du contacteur de feux stop</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

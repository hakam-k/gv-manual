<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "F8T40A1614001" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Diagnostic des symptômes de refroidissement du moteur
</title>
<diag type = "condition" id = "F8T40A1614001001">
<condition>
<ptxt>
Surchauffe du moteur (le ventilateur du radiateur fonctionne)
</ptxt>
</condition>
<ps>
<ptxt>
Courroie de la pompe à eau détendue ou cassée
</ptxt>
</ps>
<action>
<ptxt>
Régler ou remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Manque de liquide de refroidissement
</ptxt>
</ps>
<action>
<ptxt>
Vérifier le niveau de liquide de refroidissement et en ajouter au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Thermostat défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Pompe à eau défectueuse
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Ailettes du radiateur sales ou pliées
</ptxt>
</ps>
<action>
<ptxt>
Nettoyer ou corriger.
</ptxt>
</action>
<ps>
<ptxt>
Fuite de liquide du système de refroidissement
</ptxt>
</ps>
<action>
<ptxt>
Réparer.
</ptxt>
</action>
<ps>
<ptxt>
Radiateur obstrué
</ptxt>
</ps>
<action>
<ptxt>
Vérifier le radiateur et le remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Bouchon de radiateur défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Avance à l&apos;allumage incorrecte
</ptxt>
</ps>
<action>
<ptxt>
Régler.
</ptxt>
</action>
<ps>
<ptxt>
Frottement des freins
</ptxt>
</ps>
<action>
<ptxt>
Régler le frein.
</ptxt>
</action>
<ps>
<ptxt>
Patinage de l&apos;embrayage
</ptxt>
</ps>
<action>
<ptxt>
Régler ou remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Faible charge de la batterie
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Mauvais rendement de l&apos;alternateur
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et réparer.
</ptxt>
</action>
<ps>
<ptxt>
Capteur ECT défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Relais de ventilateur de refroidissement du radiateur n° 2 et&#x002F;ou n° 3 défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Moteur du ventilateur de radiateur défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
ECM défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Câblage ou mise à la masse défectueux
</ptxt>
</ps>
<action>
<ptxt>
Réparer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Trop de dispositifs causant une charge électrique excessive
</ptxt>
</ps>
<action>
<ptxt>
Déposer.
</ptxt>
</action>
<condition>
<ptxt>
Surchauffe du moteur (le ventilateur du radiateur ne fonctionne pas)
</ptxt>
</condition>
<ps>
<ptxt>
Fusible grillé
</ptxt>
</ps>
<action>
<ptxt>
Vérifier le fusible de 15 A de la boîte à fusibles&#x002F;du boîtier de relais et rechercher un éventuel court-circuit à la masse.
</ptxt>
</action>
<ps>
<ptxt>
Relais du ventilateur de refroidissement du radiateur n° 1 défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Capteur ECT défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Moteur du ventilateur de refroidissement du radiateur défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
Câblage ou mise à la masse défectueux
</ptxt>
</ps>
<action>
<ptxt>
Réparer au besoin.
</ptxt>
</action>
<ps>
<ptxt>
ECM défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier et remplacer au besoin.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

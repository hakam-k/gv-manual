<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "F8T40A1814A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Consommation accrue de carburant ou faibles performances du moteur
</title>
<diag type = "condition">
<condition>
<ptxt>
Consommation accrue de carburant ou faibles performances du moteur
</ptxt>
</condition>
<ps>
<ptxt>
Avance à l&apos;allumage incorrecte
</ptxt>
</ps>
<action>
<ptxt>
Vérifier les capteurs concernés et la plaque de capteur CKP.
</ptxt>
</action>
<ps>
<ptxt>
Bougie(s) d&apos;allumage ou câble(s) haute tension défectueux (pour moteur M16A)
</ptxt>
</ps>
<action>
<ptxt>
Régler, nettoyer ou remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Ensemble de bobine d&apos;allumage défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Capteur CKP ou plaque de capteur CKP défectueux
</ptxt>
</ps>
<action>
<ptxt>
Nettoyer, serrer ou remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Capteur CMP ou dent de rotor de capteur de l&apos;arbre à cames défectueux
</ptxt>
</ps>
<action>
<ptxt>
Nettoyer, serrer ou remplacer.
</ptxt>
</action>
<ps>
<ptxt>
Capteur de cliquetis défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
<ps>
<ptxt>
ECM défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

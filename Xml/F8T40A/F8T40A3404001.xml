<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3D">
<servcat sctnbr = "03" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "F8T40A3404001" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Diagnostic des symptômes de l&apos;arbre de transmission
</title>
<diag type = "condition" id = "F8T40A3404001001">
<condition>
<ptxt>
Bruit anormal
</ptxt>
</condition>
<ps>
<ptxt>
Ecrou et boulon de flasque d&apos;arbre de transmission desserrés
</ptxt>
</ps>
<action>
<ptxt>
Serrer l&apos;écrou et le boulon de flasque d&apos;arbre de transmission. 
<intxreftitleoff1 refid = "F8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Roulement de croisillon usé ou coincé
</ptxt>
</ps>
<action>
<ptxt>
Remplacer. 
<intxreftitleoff1 refid = "F8T40A3406003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Croisillon usé
</ptxt>
</ps>
<action>
<ptxt>
Remplacer l&apos;arbre de transmission. 
<intxreftitleoff1 refid = "F8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<condition>
<ptxt>
Vibrations
</ptxt>
</condition>
<ps>
<ptxt>
Arbre de transmission déformé
</ptxt>
</ps>
<action>
<ptxt>
Remplacer. 
<intxreftitleoff1 refid = "F8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "F5JB0B1406077" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "0" modify = "0">
<title>
Dépose et pose de la pompe à vide
</title>
<servinfosub id = "F5JB0B1406077001" page = "2">
<title>
Dépose
</title>
<s1>
<ptxt>
Débrancher le câble négatif (&#8211;) de la batterie.
</ptxt>
</s1>
<s1>
<ptxt>
Déposer la tôle du moteur.
</ptxt>
</s1>
<s1>
<ptxt>
Déposer le tuyau d&#8217;admission d&#8217;air en se référant à 
<intxref refid = "F5JB0B1406074">Dépose et pose du tuyau d&#8217;admission d&#8217;air</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Débrancher la durite à dépression (1) de la pompe à vide (2).
</ptxt>
</s1>
<s1>
<ptxt>
Déposer la pompe à vide de la culasse.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B120002.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "161" gh = "127"/>
</figure>
</s1>
</servinfosub>
<c-change/>
<servinfosub id = "F5JB0B1406077002" page = "2">
<title>
Pose
</title>
<s1>
<ptxt>
Fixer la pompe à vide (1) sur la culasse avec un nouveau joint.
</ptxt>
<torque>
<torqueitem id = "T1D009">
<t-part>
Boulon de pompe à vide
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
23
</t-value1>
<t-value2>
2,3
</t-value2>
<t-value3>
17,0
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Brancher le tuyau à dépression (2) sur la pompe à vide.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B120003.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "161" gh = "127"/>
</figure>
</s1>
<s1>
<ptxt>
Poser le tuyau d&#8217;admission d&#8217;air en se référant à 
<intxref refid = "F5JB0B1406074">Dépose et pose du tuyau d&#8217;admission d&#8217;air</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Poser la tôle du moteur.
</ptxt>
</s1>
<s1>
<ptxt>
Raccorder le câble négatif (&#8211;) à la batterie.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

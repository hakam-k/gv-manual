<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "F5JB0B9204012" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "0" modify = "0">
<title>
Diagnostic des symptômes des feux stop
</title>
<diag type = "condition" id = "F5JB0B9204012001">
<condition>
<ptxt>
Les feux stop ne s&apos;allument pas
</ptxt>
</condition>
<ps>
<ptxt>
Ampoule(s) grillée(s)
</ptxt>
</ps>
<action>
<ptxt>
Remplacer la ou les ampoule(s).
</ptxt>
</action>
<ps>
<ptxt>
Fusible du circuit grillé
</ptxt>
</ps>
<action>
<ptxt>
Remplacer le fusible et rechercher les courts-circuits éventuels.
</ptxt>
</action>
<ps>
<ptxt>
Contacteur des feux stop défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier le contacteur des feux stop en se reportant au point 
<intxref refid = "F5JB0B9206010">Vérification du contacteur des feux stop</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Câblage défectueux ou mise à la masse défectueuse
</ptxt>
</ps>
<action>
<ptxt>
Réparer le circuit.
</ptxt>
</action>
<condition>
<ptxt>
Les feux stop restent allumés
</ptxt>
</condition>
<ps>
<ptxt>
Contacteur des feux stop défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier ou régler le contacteur des feux stop en se reportant au point 
<intxref refid = "F5JB0B9206010">Vérification du contacteur des feux stop</intxref>
 ou 
<intxref refid = "F5JB0B4106006">Réglage du contacteur de feux stop</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

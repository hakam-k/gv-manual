<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "F5JB0B9304004" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "0" modify = "0">
<title>
Diagnostique des symptômes de l&#8217;indicateur de niveau de carburant
</title>
<diag type = "condition" id = "F5JB0B9304004001">
<condition>
<ptxt>
L&#8217;indicateur de niveau du carburant ne fonctionne pas ou fonctionne mal
</ptxt>
</condition>
<ps>
<ptxt>
Fusible du circuit grillé
</ptxt>
</ps>
<action>
<ptxt>
Remplacer le fusible et rechercher un court-circuit.
</ptxt>
</action>
<ps>
<ptxt>
Capteur de niveau de carburant défectueux
</ptxt>
</ps>
<action>
<ptxt>
Vérifier le niveau du carburant en se référant à 
<intxref refid = "F5JB0B9306005">Examen du capteur de niveau de carburant</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Câblage ou mise à la masse défectueux
</ptxt>
</ps>
<action>
<ptxt>
Réparer le circuit.
</ptxt>
</action>
<ps>
<ptxt>
Combiné d&#8217;instruments défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer le combiné d&#8217;instruments.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

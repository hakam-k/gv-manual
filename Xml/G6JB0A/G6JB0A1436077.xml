<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "G6JB0A1436077" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Aus- und Einbau der Unterdruckpumpe
</title>
<servinfosub id = "G6JB0A1436077001" page = "2">
<title>
Ausbau
</title>
<s1>
<ptxt>
Das Massekabel (&#8211;) von der Batterie abklemmen.
</ptxt>
</s1>
<s1>
<ptxt>
Die Motorabdeckung abmontieren.
</ptxt>
</s1>
<s1>
<ptxt>
Die Luftansaugleitung (1) gemäß 
<intxref refid = "G6JB0A1436074">Ausbau und Einbau des Luftansaugrohrs:Für F9Q-Motor</intxref>
 ausbauen.
</ptxt>
</s1>
<s1>
<ptxt>
Den Unterdruckschlauch (1) von der Unterdruckpumpe (2) lösen.
</ptxt>
</s1>
<s1>
<ptxt>
Die Unterdruckpumpe vom Zylinderkopf demontieren.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B120002.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "161" gh = "127"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "G6JB0A1436077002" page = "2">
<title>
Einbau
</title>
<s1>
<ptxt>
Die Unterdruckpumpe (1) samt neuer Dichtung am Zylinderkopf montieren.
</ptxt>
<torque>
<torqueitem id = "T1D367">
<t-part>
Unterdruckpumpenschraube
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
23
</t-value1>
<t-value2>
2,3
</t-value2>
<t-value3>
17,0
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Den Unterdruckschlauch (2) an die Unterdruckpumpe anschließen.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B120003.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "161" gh = "127"/>
</figure>
</s1>
<s1>
<ptxt>
Die Luftansaugleitung (1) gemäß 
<intxref refid = "G6JB0A1436074">Ausbau und Einbau des Luftansaugrohrs:Für F9Q-Motor</intxref>
 einbauen.
</ptxt>
</s1>
<s1>
<ptxt>
Die Motorabdeckung anmontieren.
</ptxt>
</s1>
<s1>
<ptxt>
Das Massekabel (&#8211;) an die Batterie anschließen.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

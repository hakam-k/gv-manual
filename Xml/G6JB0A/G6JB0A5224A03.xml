<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G6JB0A5224A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Schaltung schwergängig
</title>
<diag type = "condition">
<condition>
<ptxt>
Schaltung schwergängig
</ptxt>
</condition>
<ps>
<ptxt>
Kupplungspedalspiel falsch eingestellt
</ptxt>
</ps>
<action>
<ptxt>
Pedalarm und&#x002F;oder Kupplungsgeberzylinder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Kupplungsscheibe verzogen oder beschädigt
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Kupplungsdruckplatte beschädigt
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsdeckel austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Luft im Kupplungshydrauliksystem
</ptxt>
</ps>
<action>
<ptxt>
Entlüften.
</ptxt>
</action>
<ps>
<ptxt>
Flüssigkeitsaustritt an Kupplungsleitung
</ptxt>
</ps>
<action>
<ptxt>
Leckstelle lokalisieren und reparieren.
</ptxt>
</action>
<ps>
<ptxt>
Synchronring verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Angefaste Zähne an Schaltmuffe oder Gangrad verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Schaltmuffe und Gangrad austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Schaltwelle verzogen
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

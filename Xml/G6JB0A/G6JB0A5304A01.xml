<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G6JB0A5304A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Schlupf
</title>
<diag type = "condition">
<condition>
<ptxt>
Schlupf
</ptxt>
</condition>
<ps>
<ptxt>
Kupplungspedalspiel falsch eingestellt
</ptxt>
</ps>
<action>
<ptxt>
Entlüften oder Geberzylinder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Belag der Kupplungsscheibe verölt oder verschlissen.
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsscheibe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Kupplungsscheibe, Druckplatte oder Schwungrad verzogen
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsscheibe, Kupplungsdeckel bzw. Schwungrad austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Membranfeder ermüdet
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsdeckel austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Geberzylinderkolben bzw. Topfdichtung kehrt nicht in Ausgangsstellung zurück
</ptxt>
</ps>
<action>
<ptxt>
Hauptzylinder reparieren.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

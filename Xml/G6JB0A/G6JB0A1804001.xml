<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G6JB0A1804001" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Symptomdiagnose für das Zündsystem
</title>
<diag type = "condition" id = "G6JB0A1804001001">
<condition>
<ptxt>
Anlasser dreht ordnungsgemäß, aber Motor startet nicht oder nur schwer (kein Zündfunken)
</ptxt>
</condition>
<ps>
<ptxt>
Zündspulensicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Signalkabel oder Zündkabel gelöst, oder schlechter Kontakt (für M16 Motor)
</ptxt>
</ps>
<action>
<ptxt>
Fest anschließen.
</ptxt>
</action>
<ps>
<ptxt>
Zündkabel defekt (für M16-Motor)
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Zündkerze(n) defekt
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Zündspule defekt
</ptxt>
</ps>
<action>
<ptxt>
Zündspulen-Baugruppe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
CKP-Sensor oder CKP-Sensorplatte defekt
</ptxt>
</ps>
<action>
<ptxt>
Reinigen, festziehen oder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
CMP-Fühler oder Impulsgeberzahn der Nockenwelle defekt
</ptxt>
</ps>
<action>
<ptxt>
Reinigen, festziehen oder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
ECM defekt
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<condition>
<ptxt>
Übermäßiger Verbrauch oder mangelhafte Motorleistung
</ptxt>
</condition>
<ps>
<ptxt>
Zündzeitpunkt verstellt
</ptxt>
</ps>
<action>
<ptxt>
Betroffene Sensoren und CKP-Sensorplatte überprüfen.
</ptxt>
</action>
<ps>
<ptxt>
Zündkerze(n) oder Zündkabel defekt (bei M16-Motor)
</ptxt>
</ps>
<action>
<ptxt>
Elektrodenabstand korrigieren, reinigen oder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Zündspulen-Baugruppe defekt
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
CKP-Sensor oder CKP-Sensorplatte defekt
</ptxt>
</ps>
<action>
<ptxt>
Reinigen, festziehen oder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
CMP-Fühler oder Impulsgeberzahn der Nockenwelle defekt
</ptxt>
</ps>
<action>
<ptxt>
Reinigen, festziehen oder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Klopfsensor defekt
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
ECM defekt
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1E">
<servcat sctnbr = "01" scnbr = "05">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "G6JB0A1536006" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Ausbau und Einbau des Wärmetauschers
</title>
<servinfosub id = "G6JB0A1536006001">
<title>
Ausbau
</title>
<s1>
<ptxt>
Den Ölfilter gemäß 
<intxref refid = "G6JB0A0206043">Wechseln von Motoröl und Filter (Dieselmotor-Modell)</intxref>
 ausbauen.
</ptxt>
</s1>
<s1>
<ptxt>
Die Wasserschläuche (1) vom Wärmetauscher (2) lösen.
</ptxt>
</s1>
<s1>
<ptxt>
Den Wärmetauscher (2) ausbauen.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B150041.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "179" gh = "124"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "G6JB0A1536006002">
<title>
Einbau
</title>
<s1>
<ptxt>
Den Wärmetauscher (1) mit einer neuen Dichtung einbauen.
</ptxt>
<torque>
<torqueitem id = "T1E333">
<t-part>
Ölfilterhalterschraube
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
15
</t-value1>
<t-value2>
1,5
</t-value2>
<t-value3>
11,0
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Die Wasserschläuche an den Wärmetauscher (2) anschließen.
</ptxt>
</s1>
<s1>
<ptxt>
Den Ölfilter gemäß 
<intxref refid = "G6JB0A0206043">Wechseln von Motoröl und Filter (Dieselmotor-Modell)</intxref>
 einbauen.
</ptxt>
<figure>
<graphic graphicname = "I5JB0B150042.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "179" gh = "124"/>
</figure>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G6JB0A9304017" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Symptomdiagnose der Hupe
</title>
<diag type = "condition" id = "G6JB0A9304017001">
<condition>
<ptxt>
Hupe funktioniert nicht.
</ptxt>
</condition>
<ps>
<ptxt>
Schaltkreissicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Sicherung austauschen und auf Kurzschluss prüfen.
</ptxt>
</action>
<ps>
<ptxt>
Hupenschalter defekt
</ptxt>
</ps>
<action>
<ptxt>
Hupenschalter überprüfen.
</ptxt>
</action>
<ps>
<ptxt>
Hupenrelais defekt
</ptxt>
</ps>
<action>
<ptxt>
Hupenrelais überprüfen. Siehe dazu 
<intxref refid = "G6JB0A9306023">Überprüfung des Hupenrelais</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Schaltkreis reparieren.
</ptxt>
</action>
<ps>
<ptxt>
Hupe defekt
</ptxt>
</ps>
<action>
<ptxt>
Hupe überprüfen. Siehe dazu 
<intxref refid = "G6JB0A9306022">Überprüfung der Hupe</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

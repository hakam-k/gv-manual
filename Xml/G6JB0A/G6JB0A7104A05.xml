<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7A">
<servcat sctnbr = "07" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G6JB0A7104A05" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Heizungsleistung ist mangelhaft (für Zusatzheizung)
</title>
<diag type = "condition">
<condition>
<ptxt>
Heizungsleistung ist mangelhaft (für Zusatzheizung)
</ptxt>
</condition>
<ps>
<ptxt>
Sicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Die betreffenden Sicherungen kontrollieren und auf einen Kurzschluss an Masse überprüfen.
</ptxt>
</action>
<ps>
<ptxt>
Zusatzheizungsrelais defekt
</ptxt>
</ps>
<action>
<ptxt>
Zusatzheizungsrelais gemäß 
<intxref refid = "G6JB0A7106024">Überprüfung Zusatzheizungssystem (Dieselmotor-Modell)</intxref>
 prüfen.
</ptxt>
</action>
<ps>
<ptxt>
Zusatzheizungsbaugruppe defekt
</ptxt>
</ps>
<action>
<ptxt>
Zusatzheizung gemäß 
<intxref refid = "G6JB0A7106025">Überprüfung des Zusatzheizungssystems im Fahrzeug (Dieselmotor-Modell)</intxref>
 prüfen.
</ptxt>
</action>
<ps>
<ptxt>
ECM defekt
</ptxt>
</ps>
<action>
<ptxt>
ECM überprüfen.
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Bei Bedarf reparieren.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

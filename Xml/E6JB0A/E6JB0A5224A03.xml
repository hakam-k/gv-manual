<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E6JB0A5224A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Dureza del cambio
</title>
<diag type = "condition">
<condition>
<ptxt>
Dureza del cambio
</ptxt>
</condition>
<ps>
<ptxt>
Recorrido libre del pedal de embrague incorrecto
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el brazo del pedal y&#x002F;o el cilindro maestro del embrague.
</ptxt>
</action>
<ps>
<ptxt>
Disco del embrague deformado o roto
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Plato de presión del embrague da&#x00F1;ado
</ptxt>
</ps>
<action>
<ptxt>
Reemplace la tapa del embrague.
</ptxt>
</action>
<ps>
<ptxt>
Aire en el sistema hidráulico del embrague
</ptxt>
</ps>
<action>
<ptxt>
Purgue el aire.
</ptxt>
</action>
<ps>
<ptxt>
Fuga en la tubería de fluido del embrague
</ptxt>
</ps>
<action>
<ptxt>
Localice el punto de fuga y repare.
</ptxt>
</action>
<ps>
<ptxt>
Anillo sincronizador desgastado
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Diente biselado del manguito o del engranaje desgastado
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el manguito y el engranaje.
</ptxt>
</action>
<ps>
<ptxt>
Eje de cambio deformado
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

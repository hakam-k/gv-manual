<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "4A">
<servcat sctnbr = "04" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E6JB0A4104A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Fuerza de frenado insuficiente
</title>
<diag type = "condition">
<condition>
<ptxt>
Fuerza de frenado insuficiente
</ptxt>
</condition>
<ps>
<ptxt>
Fugas de líquido de freno
</ptxt>
</ps>
<action>
<ptxt>
Localice el punto de fuga y repare.
</ptxt>
</action>
<ps>
<ptxt>
Disco o pastillas de freno manchados de líquido
</ptxt>
</ps>
<action>
<ptxt>
Limpie o reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Recalentamiento de los frenos
</ptxt>
</ps>
<action>
<ptxt>
Determine la causa y repare.
</ptxt>
</action>
<ps>
<ptxt>
Contacto inapropiado de las zapatas del freno en el tambor del freno
</ptxt>
</ps>
<action>
<ptxt>
Repare para obtener un contacto correcto.
</ptxt>
</action>
<ps>
<ptxt>
Zapatas de freno manchadas de líquido o mojadas con agua
</ptxt>
</ps>
<action>
<ptxt>
Limpie o reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Forros de zapatas de freno desgastados en exceso
</ptxt>
</ps>
<action>
<ptxt>
Reemplace las zapatas de freno.
</ptxt>
</action>
<ps>
<ptxt>
Cilindros de las ruedas defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Repare o reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Anomalía en el conjunto de pinza del freno
</ptxt>
</ps>
<action>
<ptxt>
Repare o reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Presencia de aire en el sistema
</ptxt>
</ps>
<action>
<ptxt>
Purgue el sistema.
</ptxt>
</action>
<ps>
<ptxt>
Avería del ABS (ESP&#174;)
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el sistema y sustituya si fuera necesario.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

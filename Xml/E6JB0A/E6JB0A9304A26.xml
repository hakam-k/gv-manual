<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E6JB0A9304A26" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
El claxon no funciona
</title>
<diag type = "condition">
<condition>
<ptxt>
El claxon no funciona
</ptxt>
</condition>
<ps>
<ptxt>
Fusible del circuito fundido
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el fusible y verifique si se ha producido un cortocircuito.
</ptxt>
</action>
<ps>
<ptxt>
Interruptor del claxon defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el interruptor del claxon.
</ptxt>
</action>
<ps>
<ptxt>
Relé del claxon defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el relé del claxon como se indica en 
<intxref refid = "E6JB0A9306023">Inspección del relé de la bocina</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Cableado o conexión a masa defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Repare el circuito.
</ptxt>
</action>
<ps>
<ptxt>
Claxon defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el claxon como se indica en 
<intxref refid = "E6JB0A9306022">Inspección de la bocina</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

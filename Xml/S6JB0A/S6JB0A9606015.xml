<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9F">
<servcat sctnbr = "09" scnbr = "06">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0A9606015" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Keyless Entry Receiver Removal and Installation (If Equipped)
</title>
<servinfosub id = "S6JB0A9606015001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove console box from vehicle body referring to 
<intxref refid = "S6JB0A9806003">Console Box Components</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect keyless entry receiver coupler.
</ptxt>
</s1>
<s1>
<ptxt>
Remove keyless entry receiver (1).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A960020.swf" width = "88.000mm" height = "47.628mm" ver = "01" gw = "129" gh = "118"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S6JB0A9606015002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure.
</ptxt>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

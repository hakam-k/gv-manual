<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0A1614A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Engine overheats (Radiator fan does not operate)
</title>
<diag type = "condition">
<condition>
<ptxt>
Engine overheats (Radiator fan does not operate)
</ptxt>
</condition>
<ps>
<ptxt>
Fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Check 15 A fuse of relay&#x002F;fuse box and check for short circuit to ground.
</ptxt>
</action>
<ps>
<ptxt>
Radiator cooling fan relay No.1 faulty
</ptxt>
</ps>
<action>
<ptxt>
Check and replace as necessary.
</ptxt>
</action>
<ps>
<ptxt>
ECT sensor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check and replace as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Radiator cooling fan motor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check and replace as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair as necessary.
</ptxt>
</action>
<ps>
<ptxt>
ECM faulty
</ptxt>
</ps>
<action>
<ptxt>
Check and replace as necessary.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

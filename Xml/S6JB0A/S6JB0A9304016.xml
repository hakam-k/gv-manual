<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0A9304016" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Cigarette Lighter Symptom Diagnosis (If Equipped)
</title>
<diag type = "condition" id = "S6JB0A9304016001">
<condition>
<ptxt>
Cigarette lighter shows no operation
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
ACC relay faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace relay
</ptxt>
</action>
<ps>
<ptxt>
Cigarette lighter faulty
</ptxt>
</ps>
<action>
<ptxt>
Check cigarette lighter.
</ptxt>
</action>
<ps>
<ptxt>
Ignition switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check ignition switch referring to 
<intxref refid = "S6JB0A9306002">Ignition Switch Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

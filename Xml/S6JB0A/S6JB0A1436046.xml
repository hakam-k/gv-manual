<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0A1436046" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Intercooler Removal and Installation
</title>
<ptxt>
<intxrefpoff refid = "S6JB0A1436045">Intercooler and Air Intake Pipe Components:For F9Q Engine</intxrefpoff>
</ptxt>
<servinfosub id = "S6JB0A1436046001" page = "2">
<title>
Removal 
</title>
<s1>
<ptxt>
Remove front bumper referring to 
<intxref refid = "S6JB0A9B06001">Front Bumper Components</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Remove resonator (1).
</ptxt>
<figure>
<graphic graphicname = "I5JB0B140007.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "147" gh = "127"/>
</figure>
</s1>
<s1>
<ptxt>
Disconnect intercooler inlet hose (1) and outlet hose (2) from intercooler (4).
</ptxt>
</s1>
<s1>
<ptxt>
Remove intercooler brackets (3) and intercooler (4).
</ptxt>
<figure>
<graphic graphicname = "I5JB0B140008.swf" width = "88.000mm" height = "48.333mm" ver = "01" gw = "225" gh = "120"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S6JB0A1436046002" page = "2">
<title>
Installation
</title>
<s1>
<ptxt>
Install intercooler brackets (1) and intercooler (2).
</ptxt>
</s1>
<s1>
<ptxt>
Connect intercooler inlet hose (3) and outlet hose (4).
</ptxt>
<torque>
<torqueitem id = "T1D359">
<t-part>
Intercooler inlet hose clamp
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
4.5
</t-value1>
<t-value2>
0.45
</t-value2>
<t-value3>
3.5
</t-value3>
</torqueitem>
<torqueitem id = "T1D360">
<t-part>
Intercooler outlet hose clamp
</t-part>
<t-part1>
b
</t-part1>
<t-value1>
4.5
</t-value1>
<t-value2>
0.45
</t-value2>
<t-value3>
3.5
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I5JB0B140009.swf" width = "88.000mm" height = "45.511mm" ver = "02" gw = "230" gh = "112"/>
</figure>
</s1>
<c-change/>
<s1>
<ptxt>
Install resonator (1).
</ptxt>
<figure>
<graphic graphicname = "I5JB0B140007.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "147" gh = "127"/>
</figure>
</s1>
<s1>
<ptxt>
Install front bumper referring to 
<intxref refid = "S6JB0A9B06001">Front Bumper Components</intxref>
.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3A">
<servcat sctnbr = "03" scnbr = "01">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0A3114A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Abnormal noise: When making turns
</title>
<diag type = "condition">
<condition>
<ptxt>
Abnormal noise: When making turns
</ptxt>
</condition>
<ps>
<ptxt>
Grease leakage from boot
</ptxt>
</ps>
<action>
<ptxt>
Replace boot and apply grease referring to 
<intxref refid = "S6JB0A3116004">Front Drive Shaft Disassembly and Assembly:Front</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Worn or broken drive shaft joint
</ptxt>
</ps>
<action>
<ptxt>
Replace drive shaft joint referring to 
<intxref refid = "S6JB0A3116004">Front Drive Shaft Disassembly and Assembly:Front</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

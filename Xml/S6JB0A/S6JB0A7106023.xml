<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7A">
<servcat sctnbr = "07" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0A7106023" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Rear Duct Removal and Installation
</title>
<ptxt>
<intxrefpoff refid = "S6JB0A7106022">Rear Duct Components</intxrefpoff>
</ptxt>
<servinfosub id = "S6JB0A7106023001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative (&#8211;) cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove front seats.
</ptxt>
</s1>
<s1>
<ptxt>
Remove console box.
</ptxt>
</s1>
<s1>
<ptxt>
Take off carpet till rear ducts is totally exposed.
</ptxt>
</s1>
<s1>
<ptxt>
Remove rear ducts (1).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A710026.swf" width = "88.000mm" height = "45.158mm" ver = "01" gw = "164" gh = "111"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S6JB0A7106023002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Insert boss (1) of duct into hole of floor member (2).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A710027.swf" width = "88.000mm" height = "51.156mm" ver = "01" gw = "166" gh = "128"/>
</figure>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

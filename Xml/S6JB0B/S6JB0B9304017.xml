<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0B9304017" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Horn Symptom Diagnosis
</title>
<diag type = "condition" id = "S6JB0B9304017001">
<condition>
<ptxt>
Horn does not operate
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Horn switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check horn switch.
</ptxt>
</action>
<ps>
<ptxt>
Horn relay faulty
</ptxt>
</ps>
<action>
<ptxt>
Check horn relay referring to 
<intxref refid = "S6JB0B9306023">Horn Relay Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
<ps>
<ptxt>
Horn faulty
</ptxt>
</ps>
<action>
<ptxt>
Check horn referring to 
<intxref refid = "S6JB0B9306022">Horn Inspection</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

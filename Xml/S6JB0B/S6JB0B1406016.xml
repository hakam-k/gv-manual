</servicetool>
<torque>
<torqueitem id = "T1D006">
<t-part>
Crankshaft pulley bolt
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
150
</t-value1>
<t-value2>
15
</t-value2>
<t-value3>
108.5
</t-value3>
</torqueitem>
</torque>

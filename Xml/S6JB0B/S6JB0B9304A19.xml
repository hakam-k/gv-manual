<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0B9304A19" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
All A&#x002F;T shift position indicator does not light up
</title>
<diag type = "condition">
<condition>
<ptxt>
All A&#x002F;T shift position indicator does not light up
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Data (information) can not be received by CAN communication
</ptxt>
</ps>
<action>
<ptxt>
Check TCM for DTC referring to 
<intxref refid = "S6JB0B5104004">DTC Check</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Transmission range sensor (shift switch) faulty
</ptxt>
</ps>
<action>
<ptxt>
Check transmission range sensor referring to 
<intxref refid = "S6JB0B5106014">Transmission Range Sensor Inspection and Adjustment</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or ground faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
<ps>
<ptxt>
Combination meter faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace combination meter.
</ptxt>
</action>
<ps>
<ptxt>
TCM faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace after making sure that none of above parts is faulty.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0B1306018" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Crankshaft Position (CKP) Sensor Removal and Installation
</title>
<servinfosub id = "S6JB0B1306018001">
<title>
Removal
</title>
<s1>
<ptxt>
Remove battery.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect connector from CKP sensor (1).
</ptxt>
</s1>
<s1>
<ptxt>
Remove CKP sensor from transmission case (2).
</ptxt>
<figure>
<graphic graphicname = "I6JB01130021.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "169" gh = "127"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S6JB0B1306018002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Tighten CKP sensor bolt to specified torque.
</ptxt>
<torque>
<torqueitem id = "T1C009">
<t-part>
CKP sensor bolt
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
11
</t-value1>
<t-value2>
1.1
</t-value2>
<t-value3>
8.0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I6JB01130022.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "169" gh = "127"/>
</figure>
</item>
<item>
<ptxt>
Connect connector to CKP sensor securely.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

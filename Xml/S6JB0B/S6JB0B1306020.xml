<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0B1306020" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Knock Sensor Removal and Installation
</title>
<servinfosub id = "S6JB0B1306020001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove intake manifold from cylinder head referring to 
<intxref refid = "S6JB0B1406009">Intake Collector and Intake Manifold Removal and Installation</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect knock sensor connector (1).
</ptxt>
</s1>
<s1>
<ptxt>
Remove knock sensor (2) from cylinder block.
</ptxt>
<figure>
<graphic graphicname = "I6JB01130025.swf" width = "88.000mm" height = "52.920mm" ver = "01" gw = "169" gh = "133"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S6JB0B1306020002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure for installation.
</ptxt>
<torque>
<torqueitem id = "T1C010">
<t-part>
Knock sensor
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
22
</t-value1>
<t-value2>
2.2
</t-value2>
<t-value3>
16.0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I6JB01130026.swf" width = "88.000mm" height = "50.803mm" ver = "01" gw = "169" gh = "127"/>
</figure>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

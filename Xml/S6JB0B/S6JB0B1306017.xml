<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0B1306017" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Camshaft Position (CMP) Sensor Removal and Installation
</title>
<servinfosub id = "S6JB0B1306017001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect connector from CMP sensor.
</ptxt>
</s1>
<s1>
<ptxt>
Remove CMP sensor (1) from timing chain cover.
</ptxt>
<figure>
<graphic graphicname = "I6JB01130019.swf" width = "88.000mm" height = "47.628mm" ver = "01" gw = "156" gh = "118"/>
<callout>
<table frame = "all" colsep = "1" rowsep = "1">
<tgroup cols = "2" colsep = "1" rowsep = "1">
<colspec colnum = "1" colname = "1" colwidth = "0.315in"/>
<colspec colnum = "2" colname = "2" colwidth = "3.150in"/>
<tbody>
<row rowsep = "0">
<entry colname = "1" border-top = "1" border-bottom = "1" border-left = "1" border-right = "0" align = "right">
<ptxt>
2.
</ptxt>
</entry>
<entry colname = "2" border-top = "1" border-bottom = "1" border-left = "0" border-right = "1" align = "left">
<ptxt>
Coolant reservoir tank
</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</callout>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S6JB0B1306017002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure noting the following.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Tighten CMP sensor bolt to specified torque.
</ptxt>
<torque>
<torqueitem id = "T1C008">
<t-part>
CMP sensor bolt
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
11
</t-value1>
<t-value2>
1.1
</t-value2>
<t-value3>
8.0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I6JB01130020.swf" width = "88.000mm" height = "47.628mm" ver = "01" gw = "156" gh = "118"/>
</figure>
</item>
<item>
<ptxt>
Connect CMP sensor connector securely.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

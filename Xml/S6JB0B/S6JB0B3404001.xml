<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3D">
<servcat sctnbr = "03" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0B3404001" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Propeller Shaft Symptom Diagnosis
</title>
<diag type = "condition" id = "S6JB0B3404001001">
<condition>
<ptxt>
Abnormal noise
</ptxt>
</condition>
<ps>
<ptxt>
Loose propeller shaft flange bolt and nut
</ptxt>
</ps>
<action>
<ptxt>
Tighten propeller shaft flange bolt and nut.
</ptxt>
</action>
<ps>
<ptxt>
Spider bearing worn out or stuck
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
<ps>
<ptxt>
Wear spider
</ptxt>
</ps>
<action>
<ptxt>
Replace propeller shaft.
</ptxt>
</action>
<condition>
<ptxt>
Vibration
</ptxt>
</condition>
<ps>
<ptxt>
Deformed propeller shaft
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

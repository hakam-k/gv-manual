<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0B1806004" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Spark Plug Removal and Installation
</title>
<servinfosub id = "S6JB0B1806004001">
<title>
Removal
</title>
<s1>
<ptxt>
Remove ignition coil assembly (1) referring to 
<intxref refid = "S6JB0B1806002">Ignition Coil Assembly (Igniter and Ignition Coil) Removal and Installation</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Remove spark plug (2).
</ptxt>
<figure>
<graphic graphicname = "I4JA01180002.swf" width = "88.000mm" height = "45.581mm" ver = "01" gw = "113" gh = "112"/>
</figure>
</s1>
</servinfosub>
<c-change/>
<servinfosub id = "S6JB0B1806004002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S6JB0B1806005">Spark Plug Inspection</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Install spark plug (1) and tighten them to specified torque.
</ptxt>
<torque>
<torqueitem id = "T1H002">
<t-part>
Spark plug
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
25
</t-value1>
<t-value2>
2.5
</t-value2>
<t-value3>
18.0
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Install ignition coil assembly (2) referring to 
<intxref refid = "S6JB0B1806002">Ignition Coil Assembly (Igniter and Ignition Coil) Removal and Installation</intxref>
.
</ptxt>
<figure>
<graphic graphicname = "I4JA01180003.swf" width = "88.000mm" height = "50.323mm" ver = "01" gw = "116" gh = "126"/>
</figure>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

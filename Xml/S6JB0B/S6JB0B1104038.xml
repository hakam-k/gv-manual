<servinfosub id = "S6JB0B1104038002">
<title>
DTC Detecting Condition and Trouble Area
</title>
<table frame = "all" colsep = "1" rowsep = "1" pgwide = "1">
<tgroup cols = "2" colsep = "1" rowsep = "1">
<colspec colnum = "1" colname = "1" colwidth = "4.089in"/>

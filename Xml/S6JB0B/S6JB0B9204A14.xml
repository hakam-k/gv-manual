<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0B9204A14" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Optical axis of only one headlight does not change
</title>
<diag type = "condition">
<condition>
<ptxt>
Optical axis of only one headlight does not change
</ptxt>
</condition>
<ps>
<ptxt>
Headlight levering actuator faulty
</ptxt>
</ps>
<action>
<ptxt>
Check actuator referring to 
<intxref refid = "S6JB0B9206022">Headlight Leveling Actuator Inspection (If Equipped)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Headlight housing deformed
</ptxt>
</ps>
<action>
<ptxt>
Replace headlight housing.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or ground faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1B">
<servcat sctnbr = "01" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0B1206003" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
EVAP Canister Removal and Installation
</title>
<servinfosub id = "S6JB0B1206003001">
<title>
Removal
</title>
<s1>
<ptxt>
Hoist vehicle.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect hoses (1) from EVAP canister (2).
</ptxt>
</s1>
<s1>
<ptxt>
Remove EVAP canister with its bracket (3).
</ptxt>
<figure>
<graphic graphicname = "I5JB0C120003.swf" width = "88.000mm" height = "61.386mm" ver = "01" gw = "228" gh = "157"/>
</figure>
</s1>
<s1>
<ptxt>
Remove EVAP canister from its bracket.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect hoses from EVAP canister.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

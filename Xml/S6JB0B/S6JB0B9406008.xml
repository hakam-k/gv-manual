<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0B9406008" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "1">
<title>
Windshield Wiper and Washer Switch Removal and Installation
</title>
<servinfosub id = "S6JB0B9406008001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative (&#8211;) cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove steering column hole cover (1).
</ptxt>
</s1>
<s1>
<ptxt>
Remove steering column covers (2).
</ptxt>
<ptxt>
Turn steering wheel to access steering column cover rear end screws (3).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A940020.swf" width = "88.000mm" height = "53.978mm" ver = "02" gw = "176" gh = "136"/>
</figure>
</s1>
<s1>
<ptxt>
Remove windshield wiper and washer switch (1) from combination switch assembly (2) and disconnect its coupler (3).
</ptxt>
<figure>
<graphic graphicname = "I4RS0B940008.swf" width = "88.000mm" height = "49.039mm" ver = "01" gw = "193" gh = "122"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S6JB0B9406008002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S6JB0B9406009">Windshield Wiper and Washer Switch Inspection</intxrefpoff>
</ptxt>
<ptxt>
Reverse removal procedure.
</ptxt>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0B9304001" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Speedometer Symptom Diagnosis
</title>
<diag type = "condition" id = "S6JB0B9304001001">
<condition>
<ptxt>
Speedometer shows no operation or incorrect operation
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Data (information) can not be received by CAN communication
</ptxt>
</ps>
<action>
<ptxt>
Check ECM for DTC referring to 
<intxref refid = "S6JB0B1104007">DTC Check</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Rear wheel speed sensor or sensor ring faulty
</ptxt>
</ps>
<action>
<ptxt>
Check rear wheel speed sensor or sensor ring referring to 
<intxref refid = "S6JB0B4506008">Rear Wheel Speed Sensor On-Vehicle Inspection</intxref>
 or 
<intxref refid = "S6JB0B4506013">Rear Wheel Encoder On-Vehicle Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or ground faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
<ps>
<ptxt>
Combination meter faulty 
</ptxt>
</ps>
<action>
<ptxt>
Replace combination meter.
</ptxt>
</action>
<ps>
<ptxt>
ECM faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace after making sure that none of above parts is faulty.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

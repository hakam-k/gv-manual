</table>
</spec>
<figure>
<graphic graphicname = "IYSQ01141097.swf" width = "88.000mm" height = "37.750mm" ver = "01" gw = "113" gh = "90"/>
<callout>
<table frame = "all" colsep = "1" rowsep = "1">
<tgroup cols = "4" colsep = "1" rowsep = "1" align = "left">
<colspec colnum = "1" colname = "1" colwidth = "0.236in"/>
<colspec colnum = "2" colname = "2" colwidth = "1.142in"/>

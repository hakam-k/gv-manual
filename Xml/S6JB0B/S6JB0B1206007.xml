<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1B">
<servcat sctnbr = "01" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S6JB0B1206007" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
PCV Valve Removal and Installation
</title>
<servinfosub id = "S6JB0B1206007001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect PCV hose from PCV valve.
</ptxt>
</s1>
<s1>
<ptxt>
Remove PCV valve from cylinder head cover.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S6JB0B1206007002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S6JB0B1206008">PCV Valve Inspection</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Apply sealant to thread part of PCV valve (1).
</ptxt>
<materials>
<materialitem name = "SUZUKI Bond No.1207F" id = "M1B001" uid = "41">
<m-part>
&#8220;A&#8221;
</m-part>
<m-desc>
Water tight sealant
</m-desc>
<m-number>
99000&#8211;31250
</m-number>
<m-name>
SUZUKI Bond No.1207F
</m-name>
</materialitem>
</materials>
</s1>
<s1>
<ptxt>
Install PCV valve (1) onto cylinder head cover.
</ptxt>
<torque>
<torqueitem id = "T1B001">
<t-part>
PCV valve
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
27
</t-value1>
<t-value2>
2.7
</t-value2>
<t-value3>
19.5
</t-value3>
</torqueitem>
</torque>
</s1>
<s1>
<ptxt>
Connect PCV hose (2) to PCV valve (1).
</ptxt>
<figure>
<graphic graphicname = "I4JA01121011.swf" width = "88.000mm" height = "45.864mm" ver = "01" gw = "189" gh = "113"/>
</figure>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

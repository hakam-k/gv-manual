<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S6JB0B7204A05" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Cool air does not come out only intermittently
</title>
<diag type = "condition">
<condition>
<ptxt>
Cool air does not come out only intermittently
</ptxt>
</condition>
<ps>
<ptxt>
Wiring connection faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair as necessary.
</ptxt>
</action>
<ps>
<ptxt>
Expansion valve faulty
</ptxt>
</ps>
<action>
<ptxt>
Check expansion valve referring to 
<intxref refid = "S6JB0B7206013">Expansion Valve On-Vehicle Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Excessive moisture in A&#x002F;C system
</ptxt>
</ps>
<action>
<ptxt>
Replace desiccant, and then perform evacuation and charge referring to 
<intxref refid = "S6JB0B7206001">Operation Procedure for Charging A&#x002F;C with Refrigerant</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Magnet clutch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check magnet clutch referring to 
<intxref refid = "S6JB0B7206036">Magnet Clutch Operating Check</intxref>
 or 
<intxref refid = "S6JB0B7206036">Magnet Clutch Operating Check</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Excessive amount of refrigerant
</ptxt>
</ps>
<action>
<ptxt>
Check the amount of refrigerant.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

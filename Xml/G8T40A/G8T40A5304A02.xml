<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G8T40A5304A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Kupplung schleift
</title>
<diag type = "condition">
<condition>
<ptxt>
Kupplung schleift
</ptxt>
</condition>
<ps>
<ptxt>
Kupplungspedalspiel falsch eingestellt
</ptxt>
</ps>
<action>
<ptxt>
Entlüften oder Geberzylinder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Membranfeder ermüdet oder Federspitzen verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsdeckel austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Zahnkranz der Antriebswelle rostig
</ptxt>
</ps>
<action>
<ptxt>
Schmieren.
</ptxt>
</action>
<ps>
<ptxt>
Zahnräder der Antriebswelle beschädigt oder verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Antriebswelle austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Übermäßiger Schlag der Kupplungsscheibe
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsscheibe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Belag der Kupplungsscheibe riefig oder verölt
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsscheibe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Flüssigkeitsaustritt
</ptxt>
</ps>
<action>
<ptxt>
Reparieren oder austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<ptxt>
Birne defekt
</ptxt>
</ps>
<action>
<ptxt>
Glühlampe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Zünder defekt
</ptxt>
</ps>
<action>
<ptxt>

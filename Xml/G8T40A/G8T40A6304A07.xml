<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "6C">
<servcat sctnbr = "06" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G8T40A6304A07" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Ungewöhnliche Geräusche
</title>
<diag type = "condition">
<condition>
<ptxt>
Ungewöhnliche Geräusche
</ptxt>
</condition>
<ps>
<ptxt>
Lufteintritt in das System aufgrund von Flüssigkeitsmangel
</ptxt>
</ps>
<action>
<ptxt>
Lenkungsflüssigkeit auffüllen und System entlüften.
</ptxt>
</action>
<ps>
<ptxt>
Eindringen von Luft in die Flüssigkeitsleitungen oder -schläuche
</ptxt>
</ps>
<action>
<ptxt>
Leitungen bzw. Schläuche austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Rutschender (lockerer) Zusatzantriebsriemen (P&#x002F;S-Antriebsriemen)
</ptxt>
</ps>
<action>
<paragroup>
<ptxt>
- M16A-Modell: Riemenspannung einstellen.
</ptxt>
<ptxt>
- J20A-, J24B-, N32A- und F9Q-Modell: Antriebsriemen und&#x002F;oder Riemenspanner bei Bedarf austauschen.
</ptxt>
</paragroup>
</action>
<ps>
<ptxt>
Verschlissener Zusatzantriebsriemen (P&#x002F;S-Antriebsriemen)
</ptxt>
</ps>
<action>
<ptxt>
Riemen austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Befestigungsschraube des Getriebegehäuses locker
</ptxt>
</ps>
<action>
<ptxt>
Schrauben nachziehen.
</ptxt>
</action>
<ps>
<ptxt>
Gestänge oder Gelenke locker
</ptxt>
</ps>
<action>
<ptxt>
Nachziehen.
</ptxt>
</action>
<ps>
<ptxt>
Leitungen oder Schläuche scheuern an der Karosserie
</ptxt>
</ps>
<action>
<ptxt>
Leitungen und Schläuche korrekt einbauen.
</ptxt>
</action>
<ps>
<ptxt>
Flügel der P&#x002F;S-Pumpe defekt
</ptxt>
</ps>
<action>
<ptxt>
Defekte Teile austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Störung des Regelventils im Lenkgetriebegehäuse
</ptxt>
</ps>
<action>
<ptxt>
Lenkgetriebegehäuse austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Lager der P&#x002F;S-Pumpenwelle beschädigt
</ptxt>
</ps>
<action>
<ptxt>
Lager austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

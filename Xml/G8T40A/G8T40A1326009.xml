<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "2">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "G8T40A1326009" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Aus- und Einbau des ECT-Sensors (Motorkühlmittel-Temperatursensor)
</title>
<servinfosub id = "G8T40A1326009001">
<title>
Ausbau
</title>
<s1>
<ptxt>
Das Minuskabel von der Batterie lösen.
</ptxt>
</s1>
<s1>
<ptxt>
Kühlmittel ablassen. 
<intxreftitleoff1 refid = "G8T40A1626004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Den Steckverbinder vom ECT-Sensor abziehen.
</ptxt>
</s1>
<s1>
<ptxt>
Den ECT-Sensor (1) entfernen.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "G8T40A1326009002">
<title>
Einbau
</title>
<ptxt>
<intxrefpoff refid = "G8T40A1326010">Überprüfung des ECT-Sensors (Motorkühlmittel-Temperatursensor):J24B</intxrefpoff>
</ptxt>
<ptxt>
Den Ausbauvorgang unter Beachtung des Folgenden umkehren.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Den O-Ring auf Schäden prüfen und bei Mängeln austauschen.
</ptxt>
</item>
<item>
<ptxt>
Die Schrauben des ECT-Sensors mit dem angegebenen Drehmoment festziehen.
</ptxt>
<torque>
<torqueitem id = "T1C213">
<t-part>
ECT-Sensor
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
12
</t-value1>
<t-value2>
1,2
</t-value2>
<t-value3>
9,0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I8T401132012.swf" width = "88.000mm" height = "45.511mm" ver = "01" gw = "143" gh = "112"/>
</figure>
</item>
<item>
<ptxt>
Den Steckverbinder an den ECT-Sensor anschließen.
</ptxt>
</item>
<item>
<ptxt>
Das Kühlsystem wieder auffüllen. 
<intxreftitleoff1 refid = "G8T40A1626005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
<item>
<ptxt>
Auf Kühlmittellecks prüfen. 
<intxreftitleoff1 refid = "G8T40A1626003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G8T40A9404001" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Symptomdiagnose für Windschutzscheibenwischer- und waschanlage
</title>
<diag type = "condition" id = "G8T40A9404001001">
<condition>
<ptxt>
Störung des Wischers
</ptxt>
</condition>
<ps>
<ptxt>
Schaltkreissicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Sicherung austauschen und auf Kurzschluss prüfen.
</ptxt>
</action>
<ps>
<ptxt>
Wischermotor defekt
</ptxt>
</ps>
<action>
<ptxt>
Wischermotor überprüfen. Siehe dazu 
<intxref refid = "G8T40A9406005">Überprüfung des Wischermotors für die Windschutzscheibe</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Kombinationsschalter (Wischerschalter) defekt
</ptxt>
</ps>
<action>
<ptxt>
Wischerschalter überprüfen. Siehe dazu 
<intxref refid = "G8T40A9406009">Überprüfung des Scheibenwischer- und Waschanlagenschalters für die Windschutzscheibe</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Schaltkreis reparieren.
</ptxt>
</action>
<condition>
<ptxt>
Störung der Waschanlage
</ptxt>
</condition>
<ps>
<ptxt>
Schlauch bzw. Düse der Waschanlage verstopft
</ptxt>
</ps>
<action>
<ptxt>
Schlauch bzw. Düse der Waschanlage reinigen oder reparieren.
</ptxt>
</action>
<ps>
<ptxt>
Schaltkreissicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Sicherung austauschen und auf Kurzschluss prüfen.
</ptxt>
</action>
<ps>
<ptxt>
Waschanlagenmotor defekt
</ptxt>
</ps>
<action>
<ptxt>
Waschanlagenmotor überprüfen. Siehe dazu 
<intxref refid = "G8T40A9406003">Überprüfung der Waschwasserpumpe</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Kombinationsschalter (Waschanlagenschalter) defekt
</ptxt>
</ps>
<action>
<ptxt>
Waschanlagenschalter überprüfen. Siehe dazu 
<intxref refid = "G8T40A9406009">Überprüfung des Scheibenwischer- und Waschanlagenschalters für die Windschutzscheibe</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Schaltkreis reparieren.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

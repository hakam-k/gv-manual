<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "3">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G8T40A1634A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Überhitzung des Motors (Kühlerlüfter in Betrieb) 
</title>
<diag type = "condition">
<condition>
<ptxt>
Überhitzung des Motors (Kühlerlüfter in Betrieb) 
</ptxt>
</condition>
<ps>
<ptxt>
Zusatzantriebsriemen lose oder gerissen
</ptxt>
</ps>
<action>
<ptxt>
Den Zusatzantriebsriemen einstellen oder austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26008"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Nicht genügend Kühlmittel
</ptxt>
</ps>
<action>
<ptxt>
Kühlmittelstand überprüfen und bei Bedarf nachfüllen. 
<intxreftitleoff1 refid = "G8T40A1636002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Thermostat ist defekt
</ptxt>
</ps>
<action>
<ptxt>
Thermostatbaugruppe austauschen. 
<intxreftitleoff1 refid = "G8T40A1636008"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Kühlmittelpumpe ist defekt
</ptxt>
</ps>
<action>
<ptxt>
Kühlmittelpumpe austauschen. 
<intxreftitleoff1 refid = "G8T40A1636016"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Verschmutzte oder verbogene Kühlerrippen
</ptxt>
</ps>
<action>
<ptxt>
Reinigen oder reparieren. 
<intxreftitleoff1 refid = "G8T40A1636013"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Kühlmittelaustritt am Kühlsystem
</ptxt>
</ps>
<action>
<ptxt>
Kühlsystem instand setzen 
<intxreftitleoff1 refid = "G8T40A1636003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Verstopfter Kühler
</ptxt>
</ps>
<action>
<ptxt>
Kühler überprüfen und bei Bedarf austauschen. 
<intxreftitleoff1 refid = "G8T40A1636014"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Kühlerdeckel ist defekt
</ptxt>
</ps>
<action>
<ptxt>
Kühlerdeckel prüfen. 
<intxreftitleoff1 refid = "G8T40A1636003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Bremsen schleifen
</ptxt>
</ps>
<action>
<ptxt>
Bremsen einstellen. 
<intxreftitleoff1 refid = "G8T40A4104002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Batterieladung unzureichend
</ptxt>
</ps>
<action>
<ptxt>
Batterie prüfen und bei Bedarf austauschen. 
<intxreftitleoff1 refid = "G8T40A1A24001"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Generator erzeugt zu wenig Strom
</ptxt>
</ps>
<action>
<ptxt>
Generator prüfen und bei Bedarf instand setzen. 
<intxreftitleoff1 refid = "G8T40A1A24002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
ECT-Sensor defekt
</ptxt>
</ps>
<action>
<ptxt>
ECT-Sensor prüfen und bei Bedarf austauschen. 
<intxreftitleoff1 refid = "G8T40A1336010"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Kühlerlüftermotor defekt
</ptxt>
</ps>
<action>
<ptxt>
Kühlerlüftermotor prüfen und bei Bedarf austauschen. 
<intxreftitleoff1 refid = "G8T40A1636009"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
ECM defekt
</ptxt>
</ps>
<action>
<ptxt>
ECM prüfen und bei Bedarf austauschen. 
<intxreftitleoff1 refid = "G8T40A1134063"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Bei Bedarf reparieren.
</ptxt>
</action>
<ps>
<ptxt>
Zu viele zusätzliche elektrische Verbraucher
</ptxt>
</ps>
<action>
<ptxt>
Bei Bedarf Verbraucher ausbauen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

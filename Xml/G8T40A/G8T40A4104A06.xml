<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "4A">
<servcat sctnbr = "04" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G8T40A4104A06" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Pedal pulsiert
</title>
<diag type = "condition">
<condition>
<ptxt>
Pedal pulsiert
</ptxt>
<ptxt>
(Pedal pulsiert bei Bremsbetätigung)
</ptxt>
</condition>
<ps>
<ptxt>
Radlager locker oder beschädigt
</ptxt>
</ps>
<action>
<ptxt>
Radnabe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Verzogener Achsschenkel der Vorder- oder der Hinterradaufhängung
</ptxt>
</ps>
<action>
<ptxt>
Achsschenkel austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Übermäßige laterale Rundlaufabweichung der Bremsscheibe
</ptxt>
</ps>
<action>
<ptxt>
Gemäß Anweisung überprüfen. Bei Überschreitung des Grenzwerts die Bremsscheibe abdrehen oder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Hinterrad-Bremstrommeln unrund
</ptxt>
</ps>
<action>
<ptxt>
Rundlaufabweichung überprüfen. Bei Bedarf reparieren oder Bremstrommel austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Bremssattel klemmt
</ptxt>
</ps>
<action>
<ptxt>
Gleitzapfen des Bremssattels überprüfen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<section sectnbr = "3D">
<servcat sctnbr = "03" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G8T40A3404001" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Symptomdiagnose für Kardanwelle
</title>
<diag type = "condition" id = "G8T40A3404001001">
<condition>
<ptxt>
Ungewöhnliche Geräusche
</ptxt>
</condition>
<ps>
<ptxt>
Flanschschraube und Mutter der Kardanwelle locker
</ptxt>
</ps>
<action>
<ptxt>
Flanschschraube und Mutter der Kardanwelle festziehen. 
<intxreftitleoff1 refid = "G8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Gelenkkreuzlager abgenutzt oder festgefressen
</ptxt>
</ps>
<action>
<ptxt>
Austauschen. 
<intxreftitleoff1 refid = "G8T40A3406003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Gelenkkreuz verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Kardanwelle austauschen. 
<intxreftitleoff1 refid = "G8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<condition>
<ptxt>
Vibration
</ptxt>
</condition>
<ps>
<ptxt>
Kardanwelle verformt
</ptxt>
</ps>
<action>
<ptxt>
Austauschen. 
<intxreftitleoff1 refid = "G8T40A3406002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>

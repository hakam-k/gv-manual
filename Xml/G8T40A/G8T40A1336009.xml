<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "G8T40A1336009" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Aus- und Einbau des Motorkühlmittel-Temperatursensors (ECT-Sensor)
</title>
<servinfosub id = "G8T40A1336009001" page = "2">
<title>
Ausbau
</title>
<s1 page = "2">
<ptxt>
Das Minuskabel von der Batterie lösen.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Kühlmittel ablassen. 
<intxreftitleoff1 refid = "G8T40A1636004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Die Motorabdeckung entfernen.
</ptxt>
</s1>
<s1>
<ptxt>
Den Ölmessstab und seine Führung ausbauen.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Den Steckverbinder (1) des ECT-Sensors lösen.
</ptxt>
<figure page = "2">
<graphic graphicname = "I8T401131012.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "170" gh = "124"/>
</figure>
</s1>
<s1 page = "2">
<ptxt>
Den ECT-Sensor vom Zylinderkopf abnehmen.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "G8T40A1336009002" page = "2">
<title>
Einbau
</title>
<ptxt>
Den Ausbauvorgang unter Beachtung des Folgenden umkehren.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Den neuen ECT-Sensor mit dem angegebenen Drehmoment festziehen.
</ptxt>
<torque>
<torqueitem id = "T1C321">
<t-part>
ECT-Sensor
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
22
</t-value1>
<t-value2>
2,2
</t-value2>
<t-value3>
16,5
</t-value3>
</torqueitem>
</torque>
<figure page = "2">
<graphic graphicname = "I8T401131013.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "172" gh = "124"/>
</figure>
</item>
<item>
<ptxt>
Das Kühlsystem wieder auffüllen. 
<intxreftitleoff1 refid = "G8T40A1636017"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
<item>
<ptxt>
Den ECT-Sensor bei laufendem Motor auf Kühlmittellecks überprüfen.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

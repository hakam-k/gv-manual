<entry colname = "1" border-top = "1" border-bottom = "1" border-left = "1" border-right = "0" align = "right">
<ptxt>
&#x005B;A&#x005D;:
</ptxt>
</entry>
<entry colname = "2" border-top = "1" border-bottom = "1" border-left = "0" border-right = "1" align = "left">
<ptxt>
Steckverbinder der beheizten Lambdasonde Nr.1 (sensorseitig)
</ptxt>
</entry>
</row>
<row rowsep = "0">
<entry colname = "1" border-top = "1" border-bottom = "1" border-left = "1" border-right = "0" align = "right">
<ptxt>
&#x005B;B&#x005D;:
</ptxt>
</entry>
<entry colname = "2" border-top = "1" border-bottom = "1" border-left = "0" border-right = "1" align = "left">
<ptxt>
Steckverbinder der beheizten Lambdasonde Nr.2 (sensorseitig)
</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</callout>
</figure>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>

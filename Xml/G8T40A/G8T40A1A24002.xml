<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1J">
<servcat sctnbr = "01" scnbr = "10">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G8T40A1A24002" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Symptomdiagnose für Generator
</title>
<ptxt>
Fehler im Ladesystem werden durch eines oder mehrere der folgenden Symptome angezeigt:
</ptxt>
<servinfosub id = "G8T40A1A24002001" page = "1">
<title>
Funktionsstörung der Ladekontrollleuchte
</title>
<diag type = "condition" id = "G8T40A1A24002002">
<condition>
<ptxt>
Bei Zündschalterstellung ON und ausgeschaltetem Motor leuchtet die Ladekontrollleuchte nicht
</ptxt>
</condition>
<ps>
<ptxt>
Sicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Sicherung austauschen.
</ptxt>
</action>
<ps>
<ptxt>
CAN-Kommunikationsleitung defekt
</ptxt>
</ps>
<action>
<paragroup>
<ptxt>
Die CAN-Kommunikationsleitung reparieren oder austauschen. 
<intxreftitleoff1 refid = "G8T40AA824002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</paragroup>
</action>
<ps>
<ptxt>
Kombiinstrument defekt
</ptxt>
</ps>
<action>
<ptxt>
Das Kombiinstrument austauschen. 
<intxreftitleoff1 refid = "G8T40A9306003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
BCM defekt
</ptxt>
</ps>
<action>
<ptxt>
BCM überprüfen. 
<intxreftitleoff1 refid = "G8T40AA204007"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung defekt
</ptxt>
</ps>
<action>
<ptxt>
Die Verkabelung reparieren.
</ptxt>
</action>
<ps>
<ptxt>
IC-Regler oder Feldspule defekt
</ptxt>
</ps>
<action>
<ptxt>
Generator reparieren oder austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26006"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<condition>
<ptxt>
Die Ladekontrollleuchte erlischt nicht, wenn der Motor läuft (häufiges Aufladen der Batterie erforderlich).
</ptxt>
</condition>
<ps>
<ptxt>
Zusatzantriebsriemen locker oder verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Den Zusatzantriebsriemen austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
IC-Regler oder Generator defekt
</ptxt>
</ps>
<action>
<ptxt>
Generator austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung defekt
</ptxt>
</ps>
<action>
<ptxt>
Die Verkabelung reparieren.
</ptxt>
</action>
</diag>
</servinfosub>
<servinfosub id = "G8T40A1A24002003">
<title>
Batterieladefehler
</title>
<diag type = "condition" id = "G8T40A1A24002004">
<condition>
<ptxt>
Batterie nicht ausreichend geladen
</ptxt>
</condition>
<ps>
<ptxt>
Zusatzantriebsriemen locker oder verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Den Zusatzantriebsriemen austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Batteriepolklemmen locker oder defekt 
</ptxt>
</ps>
<action>
<ptxt>
Die Batteriepolklemmen anziehen oder die Batterie austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Gleichrichter, Rotor oder IC-Regler defekt
</ptxt>
</ps>
<action>
<ptxt>
Generator reparieren oder austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<condition>
<ptxt>
Batterie überladen
</ptxt>
</condition>
<ps>
<ptxt>
IC-Regler defekt
</ptxt>
</ps>
<action>
<ptxt>
Generator reparieren oder austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfosub>
<servinfosub id = "G8T40A1A24002005">
<title>
Geräusche
</title>
<diag type = "condition" id = "G8T40A1A24002006">
<condition>
<ptxt>
Generator macht Lärm
</ptxt>
</condition>
<ps>
<ptxt>
Befestigungsschrauben lose
</ptxt>
</ps>
<action>
<ptxt>
Befestigungsschrauben festziehen. 
</ptxt>
</action>
<ps>
<ptxt>
Lager verschmutzt, abgenutzt oder defekt
</ptxt>
</ps>
<action>
<ptxt>
Generator reparieren oder austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Rotor, Diode oder Stator defekt
</ptxt>
</ps>
<action>
<ptxt>
Generator reparieren oder austauschen. 
<intxreftitleoff1 refid = "G8T40A1A26005"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

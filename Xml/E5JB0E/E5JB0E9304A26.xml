<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0E9304A26" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
El encendedor de cigarrillos no funciona
</title>
<diag type = "condition">
<condition>
<ptxt>
El encendedor de cigarrillos no funciona
</ptxt>
</condition>
<ps>
<ptxt>
Fusible del circuito fundido
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el fusible y compruebe si se ha producido un cortocircuito.
</ptxt>
</action>
<ps>
<ptxt>
Relé ACC defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el relé.
</ptxt>
</action>
<ps>
<ptxt>
Encendedor de cigarrillos defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el encendedor de cigarrillos.
</ptxt>
</action>
<ps>
<ptxt>
Interruptor de encendido defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el interruptor de encendido. Para ello consulte 
<intxref refid = "E5JB0E9306002">Inspección del interruptor de encendido</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Cableado o conexión a masa defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Repare el circuito.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

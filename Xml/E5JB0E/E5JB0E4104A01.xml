<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "4A">
<servcat sctnbr = "04" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0E4104A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Insuficiente fuerza de frenado
</title>
<diag type = "condition">
<condition>
<ptxt>
Insuficiente fuerza de frenado
</ptxt>
</condition>
<ps>
<ptxt>
Fuga de líquido de frenos en las tuberías de freno
</ptxt>
</ps>
<action>
<ptxt>
Localice el lugar de la fuga y repárela.
</ptxt>
</action>
<ps>
<ptxt>
Disco o pastillas de freno con manchas de líquido
</ptxt>
</ps>
<action>
<ptxt>
Limpie o recambie.
</ptxt>
</action>
<ps>
<ptxt>
Frenos sobrecalentados
</ptxt>
</ps>
<action>
<ptxt>
Determine la causa y repare.
</ptxt>
</action>
<ps>
<ptxt>
Contacto deficiente de las zapatas en el tambor del freno
</ptxt>
</ps>
<action>
<ptxt>
Repare para obtener el contacto adecuado.
</ptxt>
</action>
<ps>
<ptxt>
Forros de las zapatas de freno con manchas de líquido o empapados de agua
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Forros de las zapatas de freno excesivamente desgastados
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Cilindros de rueda, defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Repare o reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Avería del conjunto de la pinza
</ptxt>
</ps>
<action>
<ptxt>
Repare o reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Aire en el sistema
</ptxt>
</ps>
<action>
<ptxt>
Purgue el sistema.
</ptxt>
</action>
<ps>
<ptxt>
Avería del ABS (ESP&#174;), si está equipado
</ptxt>
</ps>
<action>
<ptxt>
Revise el sistema y reemplácelo si es necesario.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

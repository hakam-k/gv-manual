<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3A">
<servcat sctnbr = "03" scnbr = "01">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0E3114A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Ruido anómalo Al arrancar
</title>
<diag type = "condition">
<condition>
<ptxt>
Ruido anómalo Al arrancar
</ptxt>
</condition>
<ps>
<ptxt>
Tuercas de rueda flojas
</ptxt>
</ps>
<action>
<ptxt>
Apriete las tuercas de rueda (consulte 
<intxref refid = "E5JB0E2406003">Extracción e instalación de la rueda</intxref>
).
</ptxt>
</action>
<ps>
<ptxt>
Pernos de brida del eje propulsor flojos
</ptxt>
</ps>
<action>
<ptxt>
Apriete los pernos de brida del eje propulsor (consulte 
<intxref refid = "E5JB0E3116003">Extracción e instalación del conjunto de eje propulsor delantero:Parte delantera</intxref>
).
</ptxt>
</action>
<ps>
<ptxt>
Cojinete de rueda roto o dañado
</ptxt>
</ps>
<action>
<ptxt>
Sustitúyalo (consulte 
<intxref refid = "E5JB0E2206009">Revisión de cojinete, tuerca, disco y cubo de rueda delantero</intxref>
).
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

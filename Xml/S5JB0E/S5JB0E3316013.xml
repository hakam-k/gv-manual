<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3C">
<servcat sctnbr = "03" scnbr = "03">
<configtype typenbr = "1">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S5JB0E3316013" svcinfotypenbr = "08" svcinfoqualnbr = "02" page = "2" supplyoff = "1" modify = "0">
<title>
Counter Gear Assembly Disassembly and Reassembly
</title>
<ptxt>
<intxrefpoff refid = "S5JB0E3316009">Transfer Assembly Disassembly and Reassembly:Motor-Shift Type (Transfer with Shift Actuator)</intxrefpoff>
</ptxt>
<servinfosub id = "S5JB0E3316013001" page = "2">
<title>
Disassembly
</title>
<s1 page = "2">
<ptxt>
Remove snap ring (1) from counter gear (2).
</ptxt>
<figure page = "2">
<graphic graphicname = "I5JB0A331068.swf" width = "88.000mm" height = "44.100mm" ver = "01" gw = "71" gh = "108"/>
</figure>
</s1>
<s1 page = "2">
<ptxt>
Remove bearing (1) from counter gear (2) using special tool, bearing puller (3) and press (4).
</ptxt>
<servicetool>
<servicetoolitem id = "L3C116">
<s-part>
(A)
</s-part>
<s-number num = "09913-80113">
09913&#8211;80113
</s-number>
<ptxt>
</ptxt>
</servicetoolitem>
</servicetool>
<figure page = "2">
<graphic graphicname = "I5JB0A331069.swf" width = "88.000mm" height = "48.333mm" ver = "02" gw = "160" gh = "120"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S5JB0E3316013002" page = "2">
<title>
Reassembly
</title>
<s1 page = "2">
<ptxt>
Clean all components thoroughly, inspect them for any abnormality and replace with new ones as necessary.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Press-fit bearing (1) to counter gear using special tool and press.
</ptxt>
<servicetool>
<servicetoolitem id = "L3C117">
<s-part>
(A)
</s-part>
<s-number num = "09913-70123">
09913&#8211;70123
</s-number>
</servicetoolitem>
</servicetool>
<figure page = "2">
<graphic graphicname = "I5JB0A331070.swf" width = "88.000mm" height = "46.922mm" ver = "01" gw = "57" gh = "116"/>
</figure>
</s1>
<c-change/>
<s1 page = "2">
<ptxt>
Install snap ring (1) to counter gear (2).
</ptxt>
<figure page = "2">
<graphic graphicname = "I5JB0A331068.swf" width = "88.000mm" height = "44.100mm" ver = "01" gw = "71" gh = "108"/>
</figure>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

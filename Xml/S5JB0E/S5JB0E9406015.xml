<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S5JB0E9406015" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "0" modify = "0">
<title>
Headlight Washer Pump Removal and Installation (If Equipped)
</title>
<servinfosub id = "S5JB0E9406015001">
<title>
Removal
</title>
<s1>
<ptxt>
Remove front bumper referring to 
<intxref refid = "S5JB0E9B06001">Front Bumper Components</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect headlight washer hose (1) and washer pump connector (2) from headlight washer pump (3).
</ptxt>
</s1>
<s1>
<ptxt>
Remove headlight washer pump from washer tank.
</ptxt>
<figure>
<graphic graphicname = "I5JB0A940024.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "195" gh = "124"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S5JB0E9406015002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S5JB0E9406003">Washer Pump Inspection</intxrefpoff>
</ptxt>
<ptxt>
Reverse removal procedure for installation.
</ptxt>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0E7204A16" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
A large rattling noise is heard at idle or sudden acceleration
</title>
<servinfosub id = "S5JB0E7204011009">
<title>
Abnormal Noise from Crankshaft Pulley
</title>
<diag type = "condition">
<condition>
<ptxt>
A large rattling noise is heard at idle or sudden acceleration
</ptxt>
</condition>
<ps>
<ptxt>
Loosen crankshaft pulley bolt
</ptxt>
</ps>
<action>
<ptxt>
Retighten bolt.
</ptxt>
</action>
</diag>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

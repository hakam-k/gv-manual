<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0E5304A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Clutch vibration
</title>
<diag type = "condition">
<condition>
<ptxt>
Clutch vibration
</ptxt>
</condition>
<ps>
<ptxt>
Glazed (glass-like) clutch facings
</ptxt>
</ps>
<action>
<ptxt>
Repair or replace disc.
</ptxt>
</action>
<ps>
<ptxt>
Clutch facings dirty with oil
</ptxt>
</ps>
<action>
<ptxt>
Replace disc.
</ptxt>
</action>
<ps>
<ptxt>
Release bearing slides unsmoothly
</ptxt>
</ps>
<action>
<ptxt>
Replace clutch operating cylinder assembly.
</ptxt>
</action>
<ps>
<ptxt>
Wobbly clutch disc, or poor facing contact
</ptxt>
</ps>
<action>
<ptxt>
Replace disc.
</ptxt>
</action>
<ps>
<ptxt>
Weakened torsion springs in clutch disc
</ptxt>
</ps>
<action>
<ptxt>
Replace disc.
</ptxt>
</action>
<ps>
<ptxt>
Clutch disc rivets loose
</ptxt>
</ps>
<action>
<ptxt>
Replace disc.
</ptxt>
</action>
<ps>
<ptxt>
Distorted pressure plate or flywheel surface
</ptxt>
</ps>
<action>
<ptxt>
Replace clutch cover or flywheel.
</ptxt>
</action>
<ps>
<ptxt>
Weakened or loosened engine mounting bolt or nut
</ptxt>
</ps>
<action>
<ptxt>
Retighten or replace mounting.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

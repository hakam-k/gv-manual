<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0E1804A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Engine cranks, but will not start or hard to start (No spark)
</title>
<diag type = "condition">
<condition>
<ptxt>
Engine cranks, but will not start or hard to start (No spark)
</ptxt>
</condition>
<ps>
<ptxt>
Blown fuse for ignition coil
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
<ps>
<ptxt>
Loose connection or disconnection of high-tension cord(s) (for M16 engine) or lead wire
</ptxt>
</ps>
<action>
<ptxt>
Connect securely.
</ptxt>
</action>
<ps>
<ptxt>
Faulty high-tension cord(s) (for M16 engine)
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
<ps>
<ptxt>
Faulty spark plug(s)
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
<ps>
<ptxt>
Faulty ignition coil
</ptxt>
</ps>
<action>
<ptxt>
Replace ignition coil assembly.
</ptxt>
</action>
<ps>
<ptxt>
Faulty CKP sensor or CKP sensor plate
</ptxt>
</ps>
<action>
<ptxt>
Clean, tighten or replace.
</ptxt>
</action>
<ps>
<ptxt>
Faulty CMP sensor or sensor rotor tooth of camshaft
</ptxt>
</ps>
<action>
<ptxt>
Clean, tighten or replace.
</ptxt>
</action>
<ps>
<ptxt>
Faulty ECM
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

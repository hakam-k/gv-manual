<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S5JB0E1306017" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "0" modify = "0">
<title>
Knock Sensor Removal and Installation
</title>
<servinfosub id = "S5JB0E1306017001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Hoist vehicle.
</ptxt>
</s1>
<s1>
<ptxt>
For M16 engine, remove starting motor referring to 
<intxref refid = "S5JB0E1906001">Starting Motor Dismounting and Remounting</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect knock sensor connector (1).
</ptxt>
</s1>
<s1>
<ptxt>
Remove knock sensor (2) from cylinder block.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S5JB0E1306017002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure for installation.
</ptxt>
<torque>
<torqueitem id = "T1C009">
<t-part>
Knock sensor
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
22
</t-value1>
<t-value2>
2.2
</t-value2>
<t-value3>
16.0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I5JB0A130029.swf" width = "88.000mm" height = "48.686mm" ver = "02" gw = "174" gh = "121"/>
</figure>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<servinfo servinfosgmlid = "S5JB0E9304017" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "0" modify = "0">
<title>
Horn Symptom Diagnosis
</title>
<diag type = "condition" id = "S5JB0E9304017001">
<condition>
<ptxt>
Horn does not operate
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Horn switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check horn switch.
</ptxt>
</action>
<ps>
<ptxt>
Horn relay faulty
</ptxt>
</ps>
<action>
<ptxt>
Check horn relay.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
<ps>
<ptxt>
Horn faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace horn.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0E5204A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Hard shifting
</title>
<diag type = "condition">
<condition>
<ptxt>
Hard shifting
</ptxt>
</condition>
<ps>
<ptxt>
Improper clutch pedal free travel
</ptxt>
</ps>
<action>
<ptxt>
Replace pedal arm and&#x002F;or clutch master cylinder.
</ptxt>
</action>
<ps>
<ptxt>
Distorted or broken clutch disc
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
<ps>
<ptxt>
Damaged clutch pressure plate
</ptxt>
</ps>
<action>
<ptxt>
Replace clutch cover.
</ptxt>
</action>
<ps>
<ptxt>
Air in clutch hydraulic system
</ptxt>
</ps>
<action>
<ptxt>
Bleed air.
</ptxt>
</action>
<ps>
<ptxt>
Fluid leakage from clutch fluid line
</ptxt>
</ps>
<action>
<ptxt>
Locate leaking point and repair.
</ptxt>
</action>
<ps>
<ptxt>
Worn synchronizer ring
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
<ps>
<ptxt>
Worn chamfered tooth on sleeve or gear
</ptxt>
</ps>
<action>
<ptxt>
Replace sleeve and gear.
</ptxt>
</action>
<ps>
<ptxt>
Distorted shift shaft
</ptxt>
</ps>
<action>
<ptxt>
Replace.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

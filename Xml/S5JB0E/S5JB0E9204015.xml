<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0E9204015" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "1" modify = "0">
<title>
Illumination Cancel System Symptom Diagnosis (If Equipped)
</title>
<diag type = "condition" id = "S5JB0E9204015001">
<condition>
<ptxt>
Illumination cancel do not normal operation
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Illumination cancel switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check illumination cancel switch referring to 
<intxref refid = "S5JB0E9206028">Illumination Cancel Switch Inspection (If Equipped)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Combination meter and&#x002F;or information display (clock) faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace combination meter and&#x002F;or information display (clock).
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

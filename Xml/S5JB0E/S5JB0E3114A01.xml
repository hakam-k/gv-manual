<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3A">
<servcat sctnbr = "03" scnbr = "01">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0E3114A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Abnormal noise: When starting
</title>
<diag type = "condition">
<condition>
<ptxt>
Abnormal noise: When starting
</ptxt>
</condition>
<ps>
<ptxt>
Loose wheel nut(s)
</ptxt>
</ps>
<action>
<ptxt>
Tighten wheel nut(s) referring to 
<intxref refid = "S5JB0E2406003">Wheel Removal and Installation</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Loose drive shaft flange bolt(s)
</ptxt>
</ps>
<action>
<ptxt>
Tighten drive shaft flange bolt(s) referring to 
<intxref refid = "S5JB0E3116003">Front Drive Shaft Assembly Removal and Installation:Front</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Broken or damaged wheel bearing
</ptxt>
</ps>
<action>
<ptxt>
Replace referring to 
<intxref refid = "S5JB0E2206009">Front Wheel Hub, Disc, Nut and Bearing Check</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

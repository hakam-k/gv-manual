<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "E6JB0B1306018" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Extracción e instalación del sensor de posición del cigüeñal (CKP)
</title>
<servinfosub id = "E6JB0B1306018001">
<title>
Extracción
</title>
<s1>
<ptxt>
Extraiga la batería.
</ptxt>
</s1>
<s1>
<ptxt>
Desenchufe el conector del sensor de CKP (1).
</ptxt>
</s1>
<s1>
<ptxt>
Extraiga el sensor de CKP de la caja de transmisión (2).
</ptxt>
<figure>
<graphic graphicname = "I6JB01130021." width = "88.000mm" height = "50.803mm" ver = "01" gw = "169" gh = "127"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "E6JB0B1306018002">
<title>
Instalación
</title>
<ptxt>
Invierta el procedimiento de extracción, teniendo en cuenta lo siguiente.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Apriete el perno del sensor de CKP al par especificado.
</ptxt>
<torque>
<torqueitem id = "T1C009">
<t-part>
Perno del sensor de CKP
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
11
</t-value1>
<t-value2>
1,1
</t-value2>
<t-value3>
8,0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I6JB01130022." width = "88.000mm" height = "50.803mm" ver = "01" gw = "169" gh = "127"/>
</figure>
</item>
<item>
<ptxt>
Conecte firmemente el conector al sensor de CKP.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

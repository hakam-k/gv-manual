<s-part>
(A)
</s-part>
<s-number num = "09915-77412">
09915&#8211;77412
</s-number>
</servicetoolitem>
<servicetoolitem id = "L6C002">
<s-part>
(B)
</s-part>
<s-number num = "09915-77420">
09915&#8211;77420
</s-number>
</servicetoolitem>
</servicetool>

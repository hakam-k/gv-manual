<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0B9304004" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "0" modify = "0">
<title>
Kraftstoffanzeigen-Symptomdiagnose
</title>
<diag type = "condition" id = "G5JB0B9304004001">
<condition>
<ptxt>
Kraftstoffanzeige funktioniert nicht oder mangelhaft
</ptxt>
</condition>
<ps>
<ptxt>
Schaltkreissicherung durchgebrannt
</ptxt>
</ps>
<action>
<ptxt>
Sicherung austauschen und Schaltkreis auf Kurzschluss untersuchen.
</ptxt>
</action>
<ps>
<ptxt>
Kraftstoffstandgeber defekt
</ptxt>
</ps>
<action>
<ptxt>
Den Kraftstoffstandgeber gemäß 
<intxref refid = "G5JB0B9306005">Überprüfung Kraftstoffstandgeber</intxref>
 prüfen.
</ptxt>
</action>
<ps>
<ptxt>
Verkabelung oder Masseanschluss defekt
</ptxt>
</ps>
<action>
<ptxt>
Schaltkreis reparieren.
</ptxt>
</action>
<ps>
<ptxt>
Kombinationsinstrument defekt
</ptxt>
</ps>
<action>
<ptxt>
Kombinationsinstrument austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

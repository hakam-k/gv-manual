<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0B5204A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Schaltung schwergängig
</title>
<diag type = "condition">
<condition>
<ptxt>
Schaltung schwergängig
</ptxt>
</condition>
<ps>
<ptxt>
Kupplungspedalspiel falsch eingestellt
</ptxt>
</ps>
<action>
<ptxt>
Pedalarm und&#x002F;oder Kupplungshauptzylinder austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Verzogene oder gebrochene Kupplungsscheibe
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Beschädigte Kupplungsdruckplatte
</ptxt>
</ps>
<action>
<ptxt>
Kupplungsdeckel austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Luft in der Kupplungshydraulik
</ptxt>
</ps>
<action>
<ptxt>
Entlüften.
</ptxt>
</action>
<ps>
<ptxt>
Flüssigkeitsleitung undicht
</ptxt>
</ps>
<action>
<ptxt>
Leckstelle ausfindig machen und reparieren.
</ptxt>
</action>
<ps>
<ptxt>
Synchronring verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Angefaste Zähne an Schaltmuffe oder Zahnrad verschlissen
</ptxt>
</ps>
<action>
<ptxt>
Schaltmuffe und Zahnrad austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Schaltwelle verbogen
</ptxt>
</ps>
<action>
<ptxt>
Austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

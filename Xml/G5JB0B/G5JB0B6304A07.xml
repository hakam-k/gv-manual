<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "6C">
<servcat sctnbr = "06" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0B6304A07" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Abnormales Geräusch
</title>
<diag type = "condition">
<condition>
<ptxt>
Abnormales Geräusch
</ptxt>
</condition>
<ps>
<ptxt>
Lufteinritt wegen Flüssigkeitsmangel
</ptxt>
</ps>
<action>
<ptxt>
Flüssigkeit nachfüllen und System entlüften.
</ptxt>
</action>
<ps>
<ptxt>
Flüssigkeit enthält Luft aus Leitungen oder Schläuchen
</ptxt>
</ps>
<action>
<ptxt>
Leitungen oder Schläuche austauschen.
</ptxt>
</action>
<ps>
<ptxt>
P&#x002F;S-Antriebsriemen rutscht (locker).
</ptxt>
</ps>
<action>
<ptxt>
Spanner ggf. austauschen.
</ptxt>
</action>
<ps>
<ptxt>
P&#x002F;S-Antriebsriemen verschlissen.
</ptxt>
</ps>
<action>
<ptxt>
Den Riemen austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Getriebegehäuseschraube locker.
</ptxt>
</ps>
<action>
<ptxt>
Die Schrauben fest anziehen.
</ptxt>
</action>
<ps>
<ptxt>
Gestänge oder Gelenke locker
</ptxt>
</ps>
<action>
<ptxt>
Befestigen.
</ptxt>
</action>
<ps>
<ptxt>
Leitungen oder Schläuche berühren ein Fahrzeugteil
</ptxt>
</ps>
<action>
<ptxt>
Leitungen und Schläuche korrekt montieren.
</ptxt>
</action>
<ps>
<ptxt>
Flügel der P&#x002F;S-Pumpe defekt
</ptxt>
</ps>
<action>
<ptxt>
Defektes Bauteil oder P&#x002F;S-Pumpe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Steuerventil im Getriebegehäuse defekt
</ptxt>
</ps>
<action>
<ptxt>
Getriebegehäuse oder P&#x002F;S-Pumpe austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Lager der P&#x002F;S-Pumpenwelle defekt
</ptxt>
</ps>
<action>
<ptxt>
Lager oder P&#x002F;S-Pumpe austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

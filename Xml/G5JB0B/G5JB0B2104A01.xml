<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "2A">
<servcat sctnbr = "02" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "G5JB0B2104A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Fahrzeug zieht (drängt) auf eine Seite
</title>
<diag type = "condition">
<condition>
<ptxt>
Fahrzeug zieht (drängt) auf eine Seite
</ptxt>
</condition>
<ps>
<ptxt>
Falsch aufgezogene oder ungleiche Reifen
</ptxt>
</ps>
<action>
<ptxt>
Reifen austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Ungeeigneter Reifendruck
</ptxt>
</ps>
<action>
<ptxt>
Den Reifendruck entsprechend korrigieren.
</ptxt>
</action>
<ps>
<ptxt>
Gebrochene oder ermüdete Schraubenfedern
</ptxt>
</ps>
<action>
<ptxt>
Schraubenfedern austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Seitendrang von Radialreifen
</ptxt>
</ps>
<action>
<ptxt>
Reifen austauschen.
</ptxt>
</action>
<ps>
<ptxt>
Radspur falsch eingestellt
</ptxt>
</ps>
<action>
<ptxt>
Radspur überprüfen und einstellen.
</ptxt>
</action>
<ps>
<ptxt>
Einseitiges Schleifen einer Bremse
</ptxt>
</ps>
<action>
<ptxt>
Bremse reparieren.
</ptxt>
</action>
<ps>
<ptxt>
Lockere, verbogene oder gebrochene Teile der Vorder- oder Hinterradaufhängung
</ptxt>
</ps>
<action>
<ptxt>
Radaufhängungsteile festziehen bzw. austauschen.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

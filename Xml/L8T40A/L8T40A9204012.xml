<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A9204012" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Diagnosi Sintomi Luci Freni
</title>
<diag type = "condition" id = "L8T40A9204012001">
<condition>
<ptxt>
Le luci dei freni non si accendono
</ptxt>
</condition>
<ps>
<ptxt>
Lampadina(e) bruciata(e)
</ptxt>
</ps>
<action>
<ptxt>
Sostituire la(e) lampadina(e).
</ptxt>
</action>
<ps>
<ptxt>
Fusibile del circuito bruciato
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il fusibile e verificare se ci sono cortocircuiti.
</ptxt>
</action>
<ps>
<ptxt>
Interruttore luci freni difettoso
</ptxt>
</ps>
<action>
<ptxt>
Verificare l&apos;interruttore della luce del freno facendo riferimento a 
<intxref refid = "L8T40A9206011">Ispezione interruttore luce freni</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Cablaggio o massa difettosi
</ptxt>
</ps>
<action>
<ptxt>
Riparare il circuito filo
</ptxt>
</action>
<condition>
<ptxt>
Le luci dei freni rimangono accese
</ptxt>
</condition>
<ps>
<ptxt>
Interruttore luci freni difettoso
</ptxt>
</ps>
<action>
<ptxt>
Verificare o regolare l&apos;interruttore della luce del freno facendo riferimento a 
<intxref refid = "L8T40A9206011">Ispezione interruttore luce freni</intxref>
 o 
<intxref refid = "L8T40A4106006">Regolazione interruttore luce freni</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

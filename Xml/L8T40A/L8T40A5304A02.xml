<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A5304A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Frizione che &#8220;attacca&#8221;
</title>
<diag type = "condition">
<condition>
<ptxt>
Frizione che &#8220;attacca&#8221;
</ptxt>
</condition>
<ps>
<ptxt>
Corsa libera non corretta del pedale della frizione
</ptxt>
</ps>
<action>
<ptxt>
Spurgare l&apos;aria o sostituire il cilindro principale.
</ptxt>
</action>
<ps>
<ptxt>
Molla del diaframma allentata o punta della molla usurata.
</ptxt>
</ps>
<action>
<ptxt>
Sostituire la campana della frizione
</ptxt>
</action>
<ps>
<ptxt>
Scanalature albero primario arrugginite
</ptxt>
</ps>
<action>
<ptxt>
Lubrificare
</ptxt>
</action>
<ps>
<ptxt>
Scanalature dell&#8217;albero di trasmissione danneggiate o usurate
</ptxt>
</ps>
<action>
<ptxt>
Sostituire l&#8217;albero
</ptxt>
</action>
<ps>
<ptxt>
Disco della frizione con troppo gioco
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il disco
</ptxt>
</action>
<ps>
<ptxt>
Parte esterna della frizione rotta o sporca d&#8217;olio
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il disco
</ptxt>
</action>
<ps>
<ptxt>
Perdita di liquido
</ptxt>
</ps>
<action>
<ptxt>
Riparare o sostituire.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

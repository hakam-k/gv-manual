<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A5304A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Slittamento
</title>
<diag type = "condition">
<condition>
<ptxt>
Slittamento
</ptxt>
</condition>
<ps>
<ptxt>
Corsa libera non corretta del pedale della frizione
</ptxt>
</ps>
<action>
<ptxt>
Spurgare l&apos;aria o sostituire il cilindro principale.
</ptxt>
</action>
<ps>
<ptxt>
Superficie del disco della frizione usurata o sporca d&#8217;olio
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il disco
</ptxt>
</action>
<ps>
<ptxt>
Disco, disco condotto o volano deformati
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il disco, il coperchio della frizione o il volano.
</ptxt>
</action>
<ps>
<ptxt>
Molla del diaframma allentata.
</ptxt>
</ps>
<action>
<ptxt>
Sostituire la campana della frizione
</ptxt>
</action>
<ps>
<ptxt>
Pistone del cilindro mastro o coppetta di tenuta non ritorna
</ptxt>
</ps>
<action>
<ptxt>
Riparare il cilindro principale
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

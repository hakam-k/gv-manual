<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "2A">
<servcat sctnbr = "02" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A2104A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Usura pneumatici anomala o eccessiva
</title>
<diag type = "condition">
<condition>
<ptxt>
Usura pneumatici anomala o eccessiva
</ptxt>
</condition>
<ps>
<ptxt>
Molla a spirale deformata o rotta
</ptxt>
</ps>
<action>
<ptxt>
Sostituire la molla elicoidale.
</ptxt>
</action>
<ps>
<ptxt>
Pneumatico fuori equilibrio
</ptxt>
</ps>
<action>
<ptxt>
Regolare l&#8217;equilibratura o sostituire il pneumatico.
</ptxt>
</action>
<ps>
<ptxt>
Allineamento ruote alterato
</ptxt>
</ps>
<action>
<ptxt>
Controllare e regolare l&#8217;allineamento delle ruote.
</ptxt>
</action>
<ps>
<ptxt>
Montante (ammortizzatore) difettoso
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il montante (Ammortizzatore).
</ptxt>
</action>
<ps>
<ptxt>
Guida difficile
</ptxt>
</ps>
<action>
<ptxt>
Sostituire gli pneumatici.
</ptxt>
</action>
<ps>
<ptxt>
Veicolo sovraccarico
</ptxt>
</ps>
<action>
<ptxt>
Sostituire gli pneumatici.
</ptxt>
</action>
<ps>
<ptxt>
Pneumatici che non sono stati sottoposti a rotazione
</ptxt>
</ps>
<action>
<ptxt>
Sostituire o ruotare gli pneumatici.
</ptxt>
</action>
<ps>
<ptxt>
Cuscinetto ruota usurato o debole
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il cuscinetto ruota.
</ptxt>
</action>
<ps>
<ptxt>
Ruota o pneumatico malfermi
</ptxt>
</ps>
<action>
<ptxt>
Sostituire ruota o pneumatico.
</ptxt>
</action>
<ps>
<ptxt>
I pneumatici non sono gonfiati a dovere
</ptxt>
</ps>
<action>
<ptxt>
Regolare la pressione dei pneumatici.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

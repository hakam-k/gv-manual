<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A9204A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
L&apos;anabbagliante non si accende
</title>
<diag type = "condition">
<condition>
<ptxt>
L&apos;anabbagliante non si accende
</ptxt>
</condition>
<ps>
<ptxt>
Fusibili circuiti fusi
</ptxt>
</ps>
<action>
<ptxt>
Sostituire i fusibili e verificare se ci sono corto-circuiti.
</ptxt>
</action>
<ps>
<ptxt>
Relè anabbagliante difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare il relè del faro anteriore facendo riferimento a 
<intxref refid = "L8T40A9206008">Ispezione relè luci di coda, relè proiettori</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Interruttore luci o smorzatore difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare l&apos;interruttore di illuminazione e dimmer facendo riferimento a 
<intxref refid = "L8T40A9206007">Ispezione interruttore proiettore (in interruttore di illuminazione)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Lampadine guaste
</ptxt>
</ps>
<action>
<ptxt>
Sostituire le lampadine.
</ptxt>
</action>
<ps>
<ptxt>
Accenditori guasti
</ptxt>
</ps>
<action>
<ptxt>
Sostituire gli accenditori.
</ptxt>
</action>
<ps>
<ptxt>
Resistenza guaste
</ptxt>
</ps>
<action>
<ptxt>
Sostituire le resistenze.
</ptxt>
</action>
<ps>
<ptxt>
Cablaggio o massa difettosi
</ptxt>
</ps>
<action>
<ptxt>
Riparare il circuito filo
</ptxt>
</action>
<ps>
<ptxt>
BMC difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare il funzionamento del BCM facendo riferimento a 
<intxref refid = "L8T40AA204019">Ispezione del BCM e dei suoi circuiti</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

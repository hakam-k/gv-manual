<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "3">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "L8T40A1336009" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Rimozione e installazione del sensore temperatura del refrigerante motore (ECT)
</title>
<servinfosub id = "L8T40A1336009001" page = "2">
<title>
Rimozione
</title>
<s1 page = "2">
<ptxt>
Scollegare il cavo negativo dalla batteria.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Scaricare il fluido di raffreddamento. 
<intxreftitleoff1 refid = "L8T40A1636004"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Rimuovere il carter del motore.
</ptxt>
</s1>
<s1>
<ptxt>
Rimuovere l&apos;astina di livello dell&apos;olio e la sua guida.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Scollegare il connettore del sensore ECT (1).
</ptxt>
<figure page = "2">
<graphic graphicname = "I8T401131012.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "170" gh = "124"/>
</figure>
</s1>
<s1 page = "2">
<ptxt>
Rimuovere il sensore ECT dalla testata del cilindro.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "L8T40A1336009002" page = "2">
<title>
Installazione
</title>
<ptxt>
Seguire la procedura inversa, ricordando quanto segue.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Stringere il sensore ECT alla coppia specificata.
</ptxt>
<torque>
<torqueitem id = "T1C321">
<t-part>
Sensore ECT
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
22
</t-value1>
<t-value2>
2,2
</t-value2>
<t-value3>
16,5
</t-value3>
</torqueitem>
</torque>
<figure page = "2">
<graphic graphicname = "I8T401131013.swf" width = "88.000mm" height = "49.745mm" ver = "01" gw = "172" gh = "124"/>
</figure>
</item>
<item>
<ptxt>
Riempire l&apos;impianto raffreddamento 
<intxreftitleoff1 refid = "L8T40A1636017"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</item>
<item>
<ptxt>
Controllare eventuali perdite di refrigerante dal sensore ECT con il motore in funzione.
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

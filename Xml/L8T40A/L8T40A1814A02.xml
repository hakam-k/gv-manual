<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A1814A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Prestazioni del motore o economia di carburante insufficienti
</title>
<diag type = "condition">
<condition>
<ptxt>
Prestazioni del motore o economia di carburante insufficienti
</ptxt>
</condition>
<ps>
<ptxt>
Messa in fase dell&apos;accensione non corretta
</ptxt>
</ps>
<action>
<ptxt>
Controllare i sensori collegati e la piastra del sensore CKP.
</ptxt>
</action>
<ps>
<ptxt>
Candela(e) di accensione guasta(e) o cavo(i) ad alta tensione (per motore M16A)
</ptxt>
</ps>
<action>
<ptxt>
Regolare, pulire o sostituire.
</ptxt>
</action>
<ps>
<ptxt>
Gruppo bobina di accensione guasto
</ptxt>
</ps>
<action>
<ptxt>
Sostituire.
</ptxt>
</action>
<ps>
<ptxt>
Sensore CKP e piastra sensore CKP guasti
</ptxt>
</ps>
<action>
<ptxt>
Pulire, serrare o sostituire.
</ptxt>
</action>
<ps>
<ptxt>
Sensore CMP o dente del rotore del sensore dell&apos;albero a camme guasti
</ptxt>
</ps>
<action>
<ptxt>
Pulire, serrare o sostituire.
</ptxt>
</action>
<ps>
<ptxt>
Sensore di detonazione difettoso
</ptxt>
</ps>
<action>
<ptxt>
Sostituire.
</ptxt>
</action>
<ps>
<ptxt>
ECM guasto
</ptxt>
</ps>
<action>
<ptxt>
Sostituire.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

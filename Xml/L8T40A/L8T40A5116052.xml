</t-part>
<t-part1>
a
</t-part1>
<t-value1>
35
</t-value1>
<t-value2>
3,5
</t-value2>
<t-value3>
25,5
</t-value3>
</torqueitem>
<torqueitem id = "T5A113">
<t-part>

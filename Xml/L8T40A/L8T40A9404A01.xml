<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A9404A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Cattivo funzionamento tergicristallo
</title>
<diag type = "condition">
<condition>
<ptxt>
Cattivo funzionamento tergicristallo
</ptxt>
</condition>
<ps>
<ptxt>
Fusibile del circuito bruciato
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il fusibile e verificare se ci sono cortocircuiti.
</ptxt>
</action>
<ps>
<ptxt>
Motorino tergicristallo difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare il motorino del tergicristallo facendo riferimento a 
<intxref refid = "L8T40A9406005">Ispezione motorino Lava-tergicristalli</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Interruttore multifunzione (interruttore tergicristallo) difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare l&apos;interruttore del tergicristallo facendo riferimento a 
<intxref refid = "L8T40A9406009">Ispezione dell&#8217;interruttore del lava-tergicristalli</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Cablaggio o massa difettosi
</ptxt>
</ps>
<action>
<ptxt>
Riparare il circuito filo
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

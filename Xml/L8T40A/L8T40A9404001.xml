<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9D">
<servcat sctnbr = "09" scnbr = "04">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A9404001" svcinfotypenbr = "06" svcinfoqualnbr = "04" page = "1" supplyoff = "1" modify = "0">
<title>
Diagnosi sintomatica del lava-tergicristalli anteriore
</title>
<diag type = "condition" id = "L8T40A9404001001">
<condition>
<ptxt>
Cattivo funzionamento tergicristallo
</ptxt>
</condition>
<ps>
<ptxt>
Fusibile del circuito bruciato
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il fusibile e verificare se ci sono cortocircuiti.
</ptxt>
</action>
<ps>
<ptxt>
Motorino tergicristallo difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare il motorino del tergicristallo facendo riferimento a 
<intxref refid = "L8T40A9406005">Ispezione motorino Lava-tergicristalli</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Interruttore multifunzione (interruttore tergicristallo) difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare l&apos;interruttore del tergicristallo facendo riferimento a 
<intxref refid = "L8T40A9406009">Ispezione dell&#8217;interruttore del lava-tergicristalli</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Cablaggio o massa difettosi
</ptxt>
</ps>
<action>
<ptxt>
Riparare il circuito filo
</ptxt>
</action>
<condition>
<ptxt>
Cattivo funzionamento spruzzatore
</ptxt>
</condition>
<ps>
<ptxt>
Tubo o ugello del dispositivo di lavaggio ostruiti.
</ptxt>
</ps>
<action>
<ptxt>
Pulire o riparare il tubo e ugello ostruiti.
</ptxt>
</action>
<ps>
<ptxt>
Fusibile del circuito bruciato
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il fusibile e verificare se ci sono cortocircuiti.
</ptxt>
</action>
<ps>
<ptxt>
Motore lavaggio difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare il motorino di lavaggio facendo riferimento a 
<intxref refid = "L8T40A9406003">Ispezione della pompa del lavacristalli</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Interruttore multifunzione (interruttore lavacristalli) difettoso
</ptxt>
</ps>
<action>
<ptxt>
Controllare interruttore del lavafari facendo riferimento a 
<intxref refid = "L8T40A9406009">Ispezione dell&#8217;interruttore del lava-tergicristalli</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Cablaggio o massa difettosi
</ptxt>
</ps>
<action>
<ptxt>
Riparare il circuito filo
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1D">
<servcat sctnbr = "01" scnbr = "04">
<configtype typenbr = "4">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "L8T40A1446004" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Rimozione e installazione del gruppo filtro dell&#8217;aria
</title>
<servinfosub id = "L8T40A1446004001" page = "2">
<title>
Rimozione
</title>
<s1 page = "2">
<ptxt>
Scollegare il cavo negativo dalla batteria.
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Scollegare il connettore del sensore MAF (1) dal gruppo sensore MAF (2).
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Scollegare il flessibile di uscita del filtro dell&#8217;aria (3) dal gruppo filtro dell&#8217;aria (4).
</ptxt>
</s1>
<s1 page = "2">
<ptxt>
Rimuovere il gruppo filtro dell&#8217;aria.
</ptxt>
<figure>
<graphic graphicname = "I9T401141005.swf" width = "88.000mm" height = "51.156mm" ver = "01" gw = "183" gh = "128"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "L8T40A1446004002" page = "2">
<title>
Installazione
</title>
<ptxt>
Seguire la procedura inversa rispetto a quella di rimozione, ricordando quanto segue.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Stringere il bullone del sensore MAF alla coppia specificata.
</ptxt>
</item>
</list1>
<torque>
<torqueitem id = "T1D484">
<t-part>
Bullone sensore MAF 
</t-part>
<t-value1>
3
</t-value1>
<t-value2>
0,3
</t-value2>
<t-value3>
2,5
</t-value3>
</torqueitem>
</torque>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5B">
<servcat sctnbr = "05" scnbr = "02">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "L8T40A5214A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Marce dure
</title>
<diag type = "condition">
<condition>
<ptxt>
Marce dure
</ptxt>
</condition>
<ps>
<ptxt>
Corsa libera non corretta del pedale della frizione
</ptxt>
</ps>
<action>
<ptxt>
Sostituire il braccio del pedale e&#x002F;o cilindro mastro della frizione.
</ptxt>
</action>
<ps>
<ptxt>
Disco della frizione deformato o rotto
</ptxt>
</ps>
<action>
<ptxt>
Sostituire.
</ptxt>
</action>
<ps>
<ptxt>
Piastra pressione frizione danneggiato
</ptxt>
</ps>
<action>
<ptxt>
Sostituire la campana della frizione
</ptxt>
</action>
<ps>
<ptxt>
Aria nel sistema idraulico della frizione
</ptxt>
</ps>
<action>
<ptxt>
Spurgare l&apos;aria.
</ptxt>
</action>
<ps>
<ptxt>
Perdita di liquido dalla linea del liquido della frizione
</ptxt>
</ps>
<action>
<ptxt>
Localizzare il punto di perdita e riparare.
</ptxt>
</action>
<ps>
<ptxt>
Anello sincronizzatore usurato
</ptxt>
</ps>
<action>
<ptxt>
Sostituire.
</ptxt>
</action>
<ps>
<ptxt>
Dente smussato usurato su manicotto o ingranaggio
</ptxt>
</ps>
<action>
<ptxt>
Sostituire manicotto e ingranaggio.
</ptxt>
</action>
<ps>
<ptxt>
Albero cambio deformato
</ptxt>
</ps>
<action>
<ptxt>
Sostituire.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

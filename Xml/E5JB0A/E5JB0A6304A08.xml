<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "6C">
<servcat sctnbr = "06" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0A6304A08" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Ruido anómalo
</title>
<diag type = "condition">
<condition>
<ptxt>
Ruido anómalo
</ptxt>
</condition>
<ps>
<ptxt>
Penetra aire a causa de una cantidad de líquido insuficiente
</ptxt>
</ps>
<action>
<ptxt>
Añada líquido y purgue el aire.
</ptxt>
</action>
<ps>
<ptxt>
Aire mezclado con el líquido de los tubos o las mangueras
</ptxt>
</ps>
<action>
<ptxt>
Reemplace los tubos o mangueras.
</ptxt>
</action>
<ps>
<ptxt>
La correa de P&#x002F;S patina (floja)
</ptxt>
</ps>
<action>
<ptxt>
Ajuste la tensión de la correa.
</ptxt>
</action>
<ps>
<ptxt>
Correa de P&#x002F;S desgastada
</ptxt>
</ps>
<action>
<ptxt>
Reemplace la correa.
</ptxt>
</action>
<ps>
<ptxt>
Perno de fijación de la caja de engranajes flojo
</ptxt>
</ps>
<action>
<ptxt>
Apriete los pernos.
</ptxt>
</action>
<ps>
<ptxt>
Varillaje o juntas sueltos
</ptxt>
</ps>
<action>
<ptxt>
Vuelva a apretar.
</ptxt>
</action>
<ps>
<ptxt>
Tuberías o mangueras en contacto con una parte de la carrocería del vehículo
</ptxt>
</ps>
<action>
<ptxt>
Instale correctamente las tuberías y las mangueras.
</ptxt>
</action>
<ps>
<ptxt>
Paletas de la bomba de P&#x002F;S defectuosas
</ptxt>
</ps>
<action>
<ptxt>
Reemplace la pieza defectuosa.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

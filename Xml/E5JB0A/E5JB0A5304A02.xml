<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0A5304A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Arrastre del embrague
</title>
<diag type = "condition">
<condition>
<ptxt>
Arrastre del embrague
</ptxt>
</condition>
<ps>
<ptxt>
Carrera libre del pedal del embrague incorrecta
</ptxt>
</ps>
<action>
<ptxt>
Purgue el aire o reemplace el cilindro maestro.
</ptxt>
</action>
<ps>
<ptxt>
Muelle del diafragma debilitado o punta del muelle desgastada
</ptxt>
</ps>
<action>
<ptxt>
Sustituya la tapa del embrague.
</ptxt>
</action>
<ps>
<ptxt>
Estrías del eje de entrada oxidadas
</ptxt>
</ps>
<action>
<ptxt>
Lubrique.
</ptxt>
</action>
<ps>
<ptxt>
Estrías del eje de entrada de la transmisión desgastadas o dañadas.
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el eje de entrada.
</ptxt>
</action>
<ps>
<ptxt>
Disco de embrague excesivamente tembloroso
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el disco.
</ptxt>
</action>
<ps>
<ptxt>
Forros del disco de embrague aceitosos o rotos.
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el disco.
</ptxt>
</action>
<ps>
<ptxt>
Pérdida de líquido
</ptxt>
</ps>
<action>
<ptxt>
Repare o sustituya.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

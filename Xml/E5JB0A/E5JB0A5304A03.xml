<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0A5304A03" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Vibración del embrague
</title>
<diag type = "condition">
<condition>
<ptxt>
Vibración del embrague
</ptxt>
</condition>
<ps>
<ptxt>
Superficies del embrague cristalizadas
</ptxt>
</ps>
<action>
<ptxt>
Repare o sustituya el disco.
</ptxt>
</action>
<ps>
<ptxt>
Forros del embrague aceitosos.
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el disco.
</ptxt>
</action>
<ps>
<ptxt>
El cojinete de desembrague no se desliza suavemente
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el conjunto del cilindro de accionamiento del embrague.
</ptxt>
</action>
<ps>
<ptxt>
Disco de embrague bamboleante o poco contacto de la superficie
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el disco.
</ptxt>
</action>
<ps>
<ptxt>
Muelles de torsión debilitados en el disco de embrague
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el disco.
</ptxt>
</action>
<ps>
<ptxt>
Remaches del disco de embrague sueltos
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el disco.
</ptxt>
</action>
<ps>
<ptxt>
Superficie de la placa de presión o del volante de inercia deformada
</ptxt>
</ps>
<action>
<ptxt>
Sustituya la tapa del embrague o el volante.
</ptxt>
</action>
<ps>
<ptxt>
Perno o tuerca del soporte del motor debilitado o flojo
</ptxt>
</ps>
<action>
<ptxt>
Apriete o sustituya el soporte.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

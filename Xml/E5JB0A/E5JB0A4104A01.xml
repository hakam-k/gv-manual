<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "4A">
<servcat sctnbr = "04" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E5JB0A4104A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Fuerza de frenado insuficiente
</title>
<diag type = "condition">
<condition>
<ptxt>
Fuerza de frenado insuficiente
</ptxt>
</condition>
<ps>
<ptxt>
Fuga de líquido de frenos de los latiguillos de freno
</ptxt>
</ps>
<action>
<ptxt>
Localice el punto de fuga y repare.
</ptxt>
</action>
<ps>
<ptxt>
Disco o pastillas de freno manchados de líquido
</ptxt>
</ps>
<action>
<ptxt>
Limpie o sustituya.
</ptxt>
</action>
<ps>
<ptxt>
Recalentamiento de los frenos
</ptxt>
</ps>
<action>
<ptxt>
Determine la causa y repare.
</ptxt>
</action>
<ps>
<ptxt>
Mal contacto de las zapatas en el tambor del freno
</ptxt>
</ps>
<action>
<ptxt>
Repare para obtener un contacto correcto.
</ptxt>
</action>
<ps>
<ptxt>
Forros de zapatas de freno, manchados de líquido o mojados con agua
</ptxt>
</ps>
<action>
<ptxt>
Sustituya.
</ptxt>
</action>
<ps>
<ptxt>
Forros de zapatas de freno desgastados en exceso
</ptxt>
</ps>
<action>
<ptxt>
Sustituya.
</ptxt>
</action>
<ps>
<ptxt>
Cilindros de las ruedas defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Repare o sustituya.
</ptxt>
</action>
<ps>
<ptxt>
Mal funcionamiento del conjunto de pinza de freno
</ptxt>
</ps>
<action>
<ptxt>
Repare o sustituya.
</ptxt>
</action>
<ps>
<ptxt>
Presencia de aire en el sistema
</ptxt>
</ps>
<action>
<ptxt>
Purgue el sistema de frenos.
</ptxt>
</action>
<ps>
<ptxt>
Anomalía del ABS (sistema de frenos antibloqueo), si estuviera instalado
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el sistema y reemplace si es necesario.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

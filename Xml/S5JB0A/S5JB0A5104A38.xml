<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5A">
<servcat sctnbr = "05" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0A5104A38" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Poor 1 &#8594; 2 shift
</title>
<servinfosub id = "S5JB0A5104019004">
<title>
Trouble Diagnosis 2
</title>
<topic id = "S5JB0A5104019005">
<title>
On-vehicle repair
</title>
<diag type = "condition">
<condition>
<ptxt>
Poor 1 &#8594; 2 shift
</ptxt>
</condition>
<ps>
<ptxt>
Malfunction of shift solenoid valve No.2
</ptxt>
</ps>
<action>
<ptxt>
Inspect. If NG, replace.
</ptxt>
</action>
<ps>
<ptxt>
Malfunction of output shaft speed sensor
</ptxt>
</ps>
<action>
<ptxt>
Inspect. If NG, replace.
</ptxt>
</action>
<ps>
<ptxt>
Malfunction of transmission range sensor
</ptxt>
</ps>
<action>
<ptxt>
Inspect. If NG, replace.
</ptxt>
</action>
<ps>
<ptxt>
Malfunction of accelerator pedal position sensor
</ptxt>
</ps>
<action>
<ptxt>
Inspect referring to 
<intxref refid = "S5JB0A1104073">DTC P0122: Throttle Position Sensor (Main) Circuit Low</intxref>
, 
<intxref refid = "S5JB0A1104074">DTC P0123: Throttle Position Sensor (Main) Circuit High</intxref>
, 
<intxref refid = "S5JB0A1104075">DTC P0222: Throttle Position Sensor (Sub) Circuit Low</intxref>
 and 
<intxref refid = "S5JB0A1104076">DTC P0223: Throttle Position Sensor (Sub) Circuit High</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Faulty valve body component
</ptxt>
</ps>
<action>
<ptxt>
Replace valve body assembly.
</ptxt>
</action>
</diag>
</topic>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

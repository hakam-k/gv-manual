<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0A9204A15" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Optical axes of both headlights do not change
</title>
<diag type = "condition">
<condition>
<ptxt>
Optical axes of both headlights do not change
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Headlight manual leveling switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check headlight manual leveling switch referring to 
<intxref refid = "S5JB0A9206019">Headlight Manual Leveling Switch (If Equipped) Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Supply voltage too low
</ptxt>
</ps>
<action>
<ptxt>
Check charging system referring to 
<intxref refid = "S5JB0A1A04002">Generator Test (Undercharged Battery Check)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

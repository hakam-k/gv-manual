<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S5JB0A9306015" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Outside Air Temperature Sensor Removal and Installation (If Equipped)
</title>
<servinfosub id = "S5JB0A9306015001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative (&#8211;) cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove front bumper referring to 
<intxref refid = "S5JB0A9B06001">Front Bumper Components</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect connector (1) from outside air temperature sensor (2).
</ptxt>
</s1>
<s1>
<ptxt>
Remove outside air temperature sensor (2) from front bumper (3).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A930010.swf" width = "88.000mm" height = "65.620mm" ver = "01" gw = "140" gh = "169"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S5JB0A9306015002">
<title>
Installation
</title>
<ptxt>
<intxrefpoff refid = "S5JB0A9306016">Outside Air Temperature Sensor Inspection (If Equipped)</intxrefpoff>
</ptxt>
<ptxt>
Reverse removal procedure for installation.
</ptxt>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

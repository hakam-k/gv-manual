<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1C">
<servcat sctnbr = "01" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S5JB0A1306013" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Camshaft Position (CMP) Sensor Removal and Installation
</title>
<servinfosub id = "S5JB0A1306013001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect connector from CMP sensor.
</ptxt>
</s1>
<s1>
<ptxt>
Remove camshaft position sensor from cylinder head cover (for J20 engine) or cylinder head (for M16 engine).
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S5JB0A1306013002">
<title>
Installation
</title>
<s1>
<ptxt>
Install camshaft position sensor to cylinder head cover (for J20 engine) or cylinder head (for M16 engine).
</ptxt>
<torque>
<torqueitem id = "T1C006">
<t-part>
CMP sensor bolt 
</t-part>
<t-part1>
a
</t-part1>
<t-value1>
11
</t-value1>
<t-value2>
1.1
</t-value2>
<t-value3>
8.0
</t-value3>
</torqueitem>
</torque>
<figure>
<graphic graphicname = "I5JB0A130026.swf" width = "88.000mm" height = "97.722mm" ver = "03" gw = "179" gh = "260"/>
<callout>
<table frame = "all" colsep = "1" rowsep = "1">
<tgroup cols = "2" colsep = "1" rowsep = "1">
<colspec colnum = "1" colname = "1" colwidth = "0.315in"/>
<colspec colnum = "2" colname = "2" colwidth = "1.141in"/>
<tbody>
<row rowsep = "1">
<entry colname = "1" border-top = "1" border-bottom = "1" border-left = "1" border-right = "0" align = "right">
<ptxt>
&#x005B;A&#x005D;:
</ptxt>
</entry>
<entry colname = "2" border-top = "1" border-bottom = "1" border-left = "0" border-right = "1" align = "left">
<ptxt>
For J20 engine
</ptxt>
</entry>
</row>
<row rowsep = "0">
<entry colname = "1" border-top = "1" border-bottom = "1" border-left = "1" border-right = "0" align = "right">
<ptxt>
&#x005B;B&#x005D;:
</ptxt>
</entry>
<entry colname = "2" border-top = "1" border-bottom = "1" border-left = "0" border-right = "1" align = "left">
<ptxt>
For M16 engine
</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</callout>
</figure>
</s1>
<s1>
<ptxt>
Connect connector to CMP sensor securely.
</ptxt>
</s1>
<s1>
<ptxt>
Connect negative cable to battery.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

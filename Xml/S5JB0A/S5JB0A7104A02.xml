<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7A">
<servcat sctnbr = "07" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0A7104A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Air temperature is not changed even when temperature selector is changed
</title>
<diag type = "condition">
<condition>
<ptxt>
Air temperature is not changed even when temperature selector is changed
</ptxt>
</condition>
<ps>
<ptxt>
Temperature control door broken
</ptxt>
</ps>
<action>
<ptxt>
Repair Temperature control door.
</ptxt>
</action>
<ps>
<ptxt>
Linkage broken
</ptxt>
</ps>
<action>
<ptxt>
Check actuator linkage referring to 
<intxref refid = "S5JB0A7106019">Actuator Linkage Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Heater hoses leaking or clogged
</ptxt>
</ps>
<action>
<ptxt>
Replace hoses.
</ptxt>
</action>
<ps>
<ptxt>
Heater core leaking or clogged
</ptxt>
</ps>
<action>
<ptxt>
Replace heater core referring to 
<intxref refid = "S5JB0A7106010">Heater Core Removal and Installation</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Temperature control actuator faulty
</ptxt>
</ps>
<action>
<ptxt>
Check temperature control actuator referring to 
<intxref refid = "S5JB0A7106016">Temperature Control Actuator Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Temperature selector faulty
</ptxt>
</ps>
<action>
<ptxt>
Check HVAC control module referring to 
<intxref refid = "S5JB0A7106012">HVAC Control Module and Its Circuits Inspection (for Vehicle without A&#x002F;C)</intxref>
 or 
<intxref refid = "S5JB0A7204052">HVAC Control Module and Its Circuits Inspection</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

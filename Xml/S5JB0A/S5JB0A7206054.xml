<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S5JB0A7206054" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Inside Air Temperature Sensor Removal and Installation
</title>
<servinfosub id = "S5JB0A7206054001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable (&#8211;) at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove steering column hole cover.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect inside air temperature sensor connector (1) and aspirator hose (2).
</ptxt>
</s1>
<s1>
<ptxt>
Remove inside air temperature sensor (3) from vehicle.
</ptxt>
<figure>
<graphic graphicname = "I5JB0A720054.swf" width = "88.000mm" height = "51.156mm" ver = "01" gw = "125" gh = "128"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S5JB0A7206054002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure.
</ptxt>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

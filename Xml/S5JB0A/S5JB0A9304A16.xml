<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0A9304A16" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Headlight auto leveling indicator stays ON
</title>
<diag type = "condition">
<condition>
<ptxt>
Headlight auto leveling indicator stays ON
</ptxt>
</condition>
<ps>
<ptxt>
Headlight auto leveling control module faulty
</ptxt>
</ps>
<action>
<ptxt>
Check headlight auto leveling control module referring to 
<intxref refid = "S5JB0A9204021">Inspection of Headlight Leveling Control Module and Its Circuit (Vehicle Equipped With Auto Leveling Headlight System)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or ground faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
<ps>
<ptxt>
Combination meter faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace combination meter.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

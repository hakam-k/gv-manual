<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5A">
<servcat sctnbr = "05" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S5JB0A5106044" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Manual Selector Assembly Removal and Installation
</title>
<ptxt>
<intxrefpoff refid = "S5JB0A5106043">Manual Selector Assembly Components</intxrefpoff>
</ptxt>
<servinfosub id = "S5JB0A5106044001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove front console box.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect shift lever switch connector.
</ptxt>
</s1>
<s1>
<ptxt>
Remove manual selector assembly mounting bolts.
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect select cable (1) from manual selector assembly (2).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A510034.swf" width = "88.000mm" height = "44.100mm" ver = "02" gw = "113" gh = "108"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "S5JB0A5106044002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure to install manual selector assembly noting the following instructions.
</ptxt>
<list1 type = "unordered">
<item>
<ptxt>
Tighten manual selector assembly mounting bolts to specified torque.
</ptxt>
<torque>
<torqueitem id = "T5A003">
<t-part>
Manual selector assembly mounting bolt
</t-part>
<t-value1>
18
</t-value1>
<t-value2>
1.8
</t-value2>
<t-value3>
13.0
</t-value3>
</torqueitem>
</torque>
</item>
<item>
<ptxt>
Adjust select cable referring to 
<intxref refid = "S5JB0A5106048">Select Cable Adjustment</intxref>
</ptxt>
</item>
</list1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

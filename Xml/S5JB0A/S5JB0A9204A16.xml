<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9B">
<servcat sctnbr = "09" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0A9204A16" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Optical axis of only one headlight does not change
</title>
<diag type = "condition">
<condition>
<ptxt>
Optical axis of only one headlight does not change
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Headlight leveling actuator faulty
</ptxt>
</ps>
<action>
<ptxt>
Check actuator referring to 
<intxref refid = "S5JB0A9206026">Headlight Leveling Actuator Inspection (If Equipped)</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Headlight housing deformed
</ptxt>
</ps>
<action>
<ptxt>
Replace headlight housing.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0A9304A14" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Illumination indicator stays ON
</title>
<diag type = "condition">
<condition>
<ptxt>
Illumination indicator stays ON
</ptxt>
</condition>
<ps>
<ptxt>
Lighting switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check lighting switch referring to 
<intxref refid = "S5JB0A9206005">Headlight Switch (in Lighting Switch) Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Data (information) can not be received by CAN communication
</ptxt>
</ps>
<action>
<ptxt>
Check BCM for DTC referring to 
<intxref refid = "S5JB0AA204005">DTC Check</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or ground faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
<ps>
<ptxt>
Combination meter faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace combination meter.
</ptxt>
</action>
<ps>
<ptxt>
BCM faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace after making sure that none of above parts is faulty.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

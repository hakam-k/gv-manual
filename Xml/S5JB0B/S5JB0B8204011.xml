<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "8B">
<servcat sctnbr = "08" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B8204011" svcinfotypenbr = "06" svcinfoqualnbr = "02" supplyoff = "0" modify = "0">
<title>
DTC B1013: SDM fault
</title>
<servinfosub id = "S5JB0B8204011001">
<title>
DTC Will Set when
</title>
<ptxt>
An internal SDM fault is detected by SDM.
</ptxt>
<attention3>
<ptxt>
DTC B1013 can never be cleared once it has been set.
</ptxt>
</attention3>
</servinfosub>
<servinfosub id = "S5JB0B8204011002">
<title>
DTC Troubleshooting
</title>
<s1>
<ptxt>
Turn ignition switch OFF.
</ptxt>
</s1>
<s1>
<ptxt>
Replace SDM.
</ptxt>
</s1>
<s1>
<ptxt>
Repeat 
<intxref refid = "S5JB0B8204001">Air Bag Diagnostic System Check</intxref>
.
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "2A">
<servcat sctnbr = "02" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B2104A10" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Cupped tires
</title>
<diag type = "condition">
<condition>
<ptxt>
Cupped tires
</ptxt>
</condition>
<ps>
<ptxt>
Front struts defective
</ptxt>
</ps>
<action>
<ptxt>
Replace struts.
</ptxt>
</action>
<ps>
<ptxt>
Worn wheel bearings
</ptxt>
</ps>
<action>
<ptxt>
Replace wheel bearings.
</ptxt>
</action>
<ps>
<ptxt>
Excessive tire or wheel run-out
</ptxt>
</ps>
<action>
<ptxt>
Replace tire and&#x002F;or wheel.
</ptxt>
</action>
<ps>
<ptxt>
Worn ball joints
</ptxt>
</ps>
<action>
<ptxt>
Replace front suspension control arm.
</ptxt>
</action>
<ps>
<ptxt>
Tire out of balance
</ptxt>
</ps>
<action>
<ptxt>
Adjust tire balance.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

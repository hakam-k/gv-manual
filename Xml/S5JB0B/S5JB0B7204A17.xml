<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B7204A17" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Clattering noise is heard from pulley
</title>
<servinfosub id = "S5JB0B7204011011">
<title>
Abnormal Noise from Tension Pulley
</title>
<diag type = "condition">
<condition>
<ptxt>
Clattering noise is heard from pulley
</ptxt>
</condition>
<ps>
<ptxt>
Worn or damaged bearing
</ptxt>
</ps>
<action>
<ptxt>
Replace tension pulley.
</ptxt>
</action>
</diag>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

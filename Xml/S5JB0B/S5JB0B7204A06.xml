<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7B">
<servcat sctnbr = "07" scnbr = "02">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B7204A06" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Cool air comes out only at high speed
</title>
<diag type = "condition">
<condition>
<ptxt>
Cool air comes out only at high speed
</ptxt>
</condition>
<ps>
<ptxt>
Condenser clogged
</ptxt>
</ps>
<action>
<ptxt>
Check condenser referring to 
<intxref refid = "S5JB0B7206002">A&#x002F;C Condenser Assembly On-Vehicle Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Insufficient charge of refrigerant
</ptxt>
</ps>
<action>
<ptxt>
Check the amount of refrigerant and system for leaks.
</ptxt>
</action>
<ps>
<ptxt>
Air in A&#x002F;C system
</ptxt>
</ps>
<action>
<ptxt>
Replace desiccant, and then perform evacuation and charge referring to 
<intxref refid = "S5JB0B7206001">Operation Procedure for Charging A&#x002F;C with Refrigerant</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Compressor drive belt loosened or broken
</ptxt>
</ps>
<action>
<ptxt>
Adjust or replace drive belt.
</ptxt>
</action>
<ps>
<ptxt>
Compressor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check compressor.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

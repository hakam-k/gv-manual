<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "2A">
<servcat sctnbr = "02" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B2104A09" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Body leans or sways in corners
</title>
<diag type = "condition">
<condition>
<ptxt>
Body leans or sways in corners
</ptxt>
</condition>
<ps>
<ptxt>
Loose stabilizer bar
</ptxt>
</ps>
<action>
<ptxt>
Tighten stabilizer bar bolts or nuts, or replace bushes.
</ptxt>
</action>
<ps>
<ptxt>
Faulty strut (shock absorber) or mounting
</ptxt>
</ps>
<action>
<ptxt>
Replace strut (shock absorber) or tighten mounting.
</ptxt>
</action>
<ps>
<ptxt>
Broken or sagging coil springs
</ptxt>
</ps>
<action>
<ptxt>
Replace coil springs.
</ptxt>
</action>
<ps>
<ptxt>
Overloaded
</ptxt>
</ps>
<action>
<ptxt>
Check loading.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B9304002" svcinfotypenbr = "06" svcinfoqualnbr = "04" supplyoff = "0" modify = "0">
<title>
Tacho meter Symptom Diagnosis
</title>
<diag type = "condition" id = "S5JB0B9304002001">
<condition>
<ptxt>
Tacho meter shows no operation or incorrect operation
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse and check for short circuit.
</ptxt>
</action>
<ps>
<ptxt>
Data (information) can not be received by CAN communication
</ptxt>
</ps>
<action>
<ptxt>
Check ECM for DTC referring to 
<intxref refid = "S5JB0B1104005">DTC Check</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or ground faulty
</ptxt>
</ps>
<action>
<ptxt>
Repair circuit.
</ptxt>
</action>
<ps>
<ptxt>
Combination meter faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace combination meter.
</ptxt>
</action>
<ps>
<ptxt>
ECM faulty
</ptxt>
</ps>
<action>
<ptxt>
Replace after making sure that none of above parts is faulty.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

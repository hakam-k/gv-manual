<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9I">
<servcat sctnbr = "09" scnbr = "09">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B9904A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Sliding roof does not operate (sliding roof motor does not run)
</title>
<diag type = "condition">
<condition>
<ptxt>
Sliding roof does not operate (sliding roof motor does not run)
</ptxt>
</condition>
<ps>
<ptxt>
Circuit fuse blown
</ptxt>
</ps>
<action>
<ptxt>
Replace fuse to check for short.
</ptxt>
</action>
<ps>
<ptxt>
Sliding roof switch faulty
</ptxt>
</ps>
<action>
<ptxt>
Check sliding roof switch referring to 
<intxref refid = "S5JB0B9906004">Sliding (Sun) Roof Switch Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Wiring or grounding faulty
</ptxt>
</ps>
<action>
<ptxt>
Check circuit referring to 
<intxref refid = "S5JB0B9906006">Sliding (Sun) Roof System Circuit and Motor Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Sliding roof motor faulty
</ptxt>
</ps>
<action>
<ptxt>
Check sliding roof motor referring to 
<intxref refid = "S5JB0B9906006">Sliding (Sun) Roof System Circuit and Motor Inspection</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "4A">
<servcat sctnbr = "04" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B4104A07" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Braking noise
</title>
<diag type = "condition">
<condition>
<ptxt>
Braking noise
</ptxt>
</condition>
<ps>
<ptxt>
Glazed shoe linings, or foreign matters stuck to linings
</ptxt>
</ps>
<action>
<ptxt>
Repair or replace shoe lining.
</ptxt>
</action>
<ps>
<ptxt>
Worn or distorted shoe linings
</ptxt>
</ps>
<action>
<ptxt>
Replace shoe lining (or pad).
</ptxt>
</action>
<ps>
<ptxt>
Loose front wheel bearings
</ptxt>
</ps>
<action>
<ptxt>
Replace wheel bearing.
</ptxt>
</action>
<ps>
<ptxt>
Distorted backing plates or loose mounting bolts
</ptxt>
</ps>
<action>
<ptxt>
Replace or retighten securing bolts.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

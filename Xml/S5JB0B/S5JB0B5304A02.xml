<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B5304A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Dragging clutch
</title>
<diag type = "condition">
<condition>
<ptxt>
Dragging clutch
</ptxt>
</condition>
<ps>
<ptxt>
Improper clutch pedal free travel
</ptxt>
</ps>
<action>
<ptxt>
Bleed air or replace master cylinder.
</ptxt>
</action>
<ps>
<ptxt>
Weakened diaphragm spring, or worn spring tip
</ptxt>
</ps>
<action>
<ptxt>
Replace clutch cover.
</ptxt>
</action>
<ps>
<ptxt>
Rusted input shaft splines
</ptxt>
</ps>
<action>
<ptxt>
Lubricate.
</ptxt>
</action>
<ps>
<ptxt>
Damaged or worn splines of transmission input shaft
</ptxt>
</ps>
<action>
<ptxt>
Replace input shaft.
</ptxt>
</action>
<ps>
<ptxt>
Excessively wobbly clutch disc
</ptxt>
</ps>
<action>
<ptxt>
Replace disc.
</ptxt>
</action>
<ps>
<ptxt>
Clutch facings broken or dirty with oil
</ptxt>
</ps>
<action>
<ptxt>
Replace disc.
</ptxt>
</action>
<ps>
<ptxt>
Fluid leakage
</ptxt>
</ps>
<action>
<ptxt>
Repair or replace.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "3C">
<servcat sctnbr = "03" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "S5JB0B3304A04" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Noise
</title>
<ptxt>
Diagnose transfer assembly after performing the following inspections.
</ptxt>
<s1>
<ptxt>
Perform 4WD control system check referring to 
<intxref refid = "S5JB0B3304001">4WD Control System Check</intxref>
.
</ptxt>
</s1>
<s1>
<ptxt>
Confirm 4WD control system operation referring to 
<intxref refid = "S5JB0B3301005">4WD Control System Operation</intxref>
</ptxt>
</s1>
<diag type = "condition">
<condition>
<ptxt>
Noise
</ptxt>
</condition>
<ps>
<ptxt>
Damaged or worn bearing(s)
</ptxt>
</ps>
<action>
<ptxt>
Refer to 
<intxref refid = "S5JB0B3306010">Transfer Assembly Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Damaged or worn gear(s)
</ptxt>
</ps>
<action>
<ptxt>
Refer to 
<intxref refid = "S5JB0B3306010">Transfer Assembly Inspection</intxref>
.
</ptxt>
</action>
<ps>
<ptxt>
Damaged or worn chamfered tooth on sleeve or gear
</ptxt>
</ps>
<action>
<ptxt>
Refer to 
<intxref refid = "S5JB0B3306010">Transfer Assembly Inspection</intxref>
.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

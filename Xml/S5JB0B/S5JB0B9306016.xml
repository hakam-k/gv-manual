<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "9C">
<servcat sctnbr = "09" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "S5JB0B9306016" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "0" modify = "0">
<title>
Audio Unit Removal and Installation
</title>
<servinfosub id = "S5JB0B9306016001">
<title>
Removal
</title>
<s1>
<ptxt>
Disconnect negative (&#8211;) cable at battery.
</ptxt>
</s1>
<s1>
<ptxt>
Remove instrument panel center garnish trims (1).
</ptxt>
<figure>
<graphic graphicname = "I5JB0A930018.swf" width = "88.000mm" height = "50.803mm" ver = "03" gw = "169" gh = "127"/>
</figure>
</s1>
<s1>
<ptxt>
Remove 6 mounting screws (1).
</ptxt>
</s1>
<s1>
<ptxt>
Disconnect electrical connectors from audio unit and HVAC control module.
</ptxt>
</s1>
<s1>
<ptxt>
Remove audio unit (2) with HVAC control module from instrument panel.
</ptxt>
<figure>
<graphic graphicname = "I5JB0A930019.swf" width = "88.000mm" height = "47.275mm" ver = "04" gw = "154" gh = "117"/>
</figure>
</s1>
<s1>
<ptxt>
Remove audio unit from HVAC control module.
</ptxt>
</s1>
</servinfosub>
<servinfosub id = "S5JB0B9306016002">
<title>
Installation
</title>
<ptxt>
Reverse removal procedure.
</ptxt>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

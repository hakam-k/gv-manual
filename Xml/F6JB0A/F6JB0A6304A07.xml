<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "6C">
<servcat sctnbr = "06" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "F6JB0A6304A07" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Bruit anormal
</title>
<diag type = "condition">
<condition>
<ptxt>
Bruit anormal
</ptxt>
</condition>
<ps>
<ptxt>
Aspiration d&apos;air causée par un manque de liquide
</ptxt>
</ps>
<action>
<ptxt>
Ajouter du liquide et purger l&apos;air.
</ptxt>
</action>
<ps>
<ptxt>
Air mélangé au liquide des tuyaux ou durites
</ptxt>
</ps>
<action>
<ptxt>
Remplacer les durites ou tuyaux.
</ptxt>
</action>
<ps>
<ptxt>
Patinage de la courroie P&#x002F;S (desserrée)
</ptxt>
</ps>
<action>
<ptxt>
Régler la tension de la courroie
</ptxt>
</action>
<ps>
<ptxt>
Courroie P&#x002F;S usée
</ptxt>
</ps>
<action>
<ptxt>
Remplacer la courroie.
</ptxt>
</action>
<ps>
<ptxt>
Boulon de fixation du carter de mécanisme desserré
</ptxt>
</ps>
<action>
<ptxt>
Resserrer les boulons.
</ptxt>
</action>
<ps>
<ptxt>
Timonerie ou joints desserrés
</ptxt>
</ps>
<action>
<ptxt>
Resserrer.
</ptxt>
</action>
<ps>
<ptxt>
Tuyaux ou durites en contact avec des pièces de la carrosserie du véhicule
</ptxt>
</ps>
<action>
<ptxt>
Poser les tuyaux et les durites correctement.
</ptxt>
</action>
<ps>
<ptxt>
Palettes de la pompe P&#x002F;S défectueuses
</ptxt>
</ps>
<action>
<ptxt>
Remplacer la pièce défectueuse.
</ptxt>
</action>
<ps>
<ptxt>
Mauvais fonctionnement de la soupape de commande du carter de mécanisme
</ptxt>
</ps>
<action>
<ptxt>
Remplacer le carter de mécanisme.
</ptxt>
</action>
<ps>
<ptxt>
Roulement de l&apos;arbre de la pompe P&#x002F;S défectueux
</ptxt>
</ps>
<action>
<ptxt>
Remplacer le roulement.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

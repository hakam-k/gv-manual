<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "5C">
<servcat sctnbr = "05" scnbr = "03">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "H5JB0A5304A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Súrlódó tengelykapcsoló
</title>
<diag type = "condition">
<condition>
<ptxt>
Súrlódó tengelykapcsoló
</ptxt>
</condition>
<ps>
<ptxt>
Nem megfelelő a tengelykapcsoló pedál holtjátéka
</ptxt>
</ps>
<action>
<ptxt>
Légtelenítse vagy cserélje ki a főhengert.
</ptxt>
</action>
<ps>
<ptxt>
Meggyengült hasított tányérrugó vagy kopottak a rugólamellák végei
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a tengelykapcsoló-fedelet.
</ptxt>
</action>
<ps>
<ptxt>
A behajtótengely bordái rozsdásak
</ptxt>
</ps>
<action>
<ptxt>
Kenje meg.
</ptxt>
</action>
<ps>
<ptxt>
A sebességváltó behajtótengelyének bordái sérültek vagy kopottak
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a behajtótengelyt.
</ptxt>
</action>
<ps>
<ptxt>
Erősen imbolygó tengelykapcsoló-tárcsa
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a tengelykapcsoló-tárcsát.
</ptxt>
</action>
<ps>
<ptxt>
A tengelykapcsoló-tárcsa felületei repedtek vagy olajosak
</ptxt>
</ps>
<action>
<ptxt>
Cserélje ki a tengelykapcsoló-tárcsát.
</ptxt>
</action>
<ps>
<ptxt>
Folyadékszivárgás
</ptxt>
</ps>
<action>
<ptxt>
Javítsa meg vagy cserélje ki.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1H">
<servcat sctnbr = "01" scnbr = "08">
<configtype typenbr = "1">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E8T40A1814A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Consumo de combustible o rendimiento del motor deficientes
</title>
<diag type = "condition">
<condition>
<ptxt>
Consumo de combustible o rendimiento del motor deficientes
</ptxt>
</condition>
<ps>
<ptxt>
Calado de encendido incorrecto
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione los sensores relacionados y la placa del sensor CKP.
</ptxt>
</action>
<ps>
<ptxt>
Bujías o cables de alta tensión defectuosos (para el motor M16)
</ptxt>
</ps>
<action>
<ptxt>
Ajuste, limpie o sustituya.
</ptxt>
</action>
<ps>
<ptxt>
Conjunto de bobina de encendido defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
<ps>
<ptxt>
Sensor CKP o placa del sensor CKP defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Limpie, apriete o sustituya.
</ptxt>
</action>
<ps>
<ptxt>
Sensor CMP o diente de rotor del sensor del árbol de levas defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Limpie, apriete o sustituya.
</ptxt>
</action>
<ps>
<ptxt>
Sensor de detonación defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
<ps>
<ptxt>
ECM defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Reemplace.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

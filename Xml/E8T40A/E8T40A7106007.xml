<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "7A">
<servcat sctnbr = "07" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "6">
<servinfo servinfosgmlid = "E8T40A7106007" svcinfotypenbr = "08" svcinfoqualnbr = "01" page = "2" supplyoff = "1" modify = "0">
<title>
Extracción e instalación del controlador del motor del soplador
</title>
<ptxt>
<intxrefpoff refid = "E8T40A7106001">Componentes de la unidad del soplador y la unidad del calentador</intxrefpoff>
</ptxt>
<servinfosub id = "E8T40A7106007001">
<title>
Extracción
</title>
<s1>
<ptxt>
Desconecte el cable negativo (-) de la batería.
</ptxt>
</s1>
<s1>
<ptxt>
Desactive el sistema de airbag. 
<intxreftitleoff1 refid = "E8T40A8206001"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Saque la guantera.
</ptxt>
</s1>
<s1>
<ptxt>
Extraiga la tapa inferior del panel de instrumentos del lado del pasajero. 
<intxreftitleoff1 refid = "E8T40A9306014"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
<s1>
<ptxt>
Desconecte el acoplador (3) del controlador del motor del soplador.
</ptxt>
</s1>
<s1>
<ptxt>
Extraiga el controlador (1) del motor del soplador aflojando sus tornillos (2).
</ptxt>
<figure>
<graphic graphicname = "I9T401710004.swf" width = "88.000mm" height = "47.981mm" ver = "01" gw = "152" gh = "119"/>
</figure>
</s1>
</servinfosub>
<servinfosub id = "E8T40A7106007002">
<title>
Instalación
</title>
<ptxt>
<intxrefpoff refid = "E8T40A7106008">Inspección del controlador del motor del soplador</intxrefpoff>
</ptxt>
<s1>
<ptxt>
Invierta el procedimiento de extracción.
</ptxt>
</s1>
<s1>
<ptxt>
Active el sistema de airbag. 
<intxreftitleoff1 refid = "E8T40A8206002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</s1>
</servinfosub>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "2A">
<servcat sctnbr = "02" scnbr = "01">
<configtype typenbr = "0">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E8T40A2104A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
Desgaste anormal o excesivo de los neumáticos
</title>
<diag type = "condition">
<condition>
<ptxt>
Desgaste anormal o excesivo de los neumáticos
</ptxt>
</condition>
<ps>
<ptxt>
Muelle espiral roto o hundido
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el muelle espiral.
</ptxt>
</action>
<ps>
<ptxt>
Neumático desequilibrado
</ptxt>
</ps>
<action>
<ptxt>
Ajuste el equilibrado o reemplace el neumático.
</ptxt>
</action>
<ps>
<ptxt>
Alineación de las ruedas desajustada
</ptxt>
</ps>
<action>
<ptxt>
Compruebe y ajuste la alineación de las ruedas.
</ptxt>
</action>
<ps>
<ptxt>
Montante defectuoso (amortiguador)
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el montante (amortiguador).
</ptxt>
</action>
<ps>
<ptxt>
Conducción difícil
</ptxt>
</ps>
<action>
<ptxt>
Reemplace los neumáticos.
</ptxt>
</action>
<ps>
<ptxt>
Vehículo sobrecargado
</ptxt>
</ps>
<action>
<ptxt>
Reemplace los neumáticos.
</ptxt>
</action>
<ps>
<ptxt>
Neumáticos sin rotar
</ptxt>
</ps>
<action>
<ptxt>
Reemplace o gire los neumáticos.
</ptxt>
</action>
<ps>
<ptxt>
Cojinete de rueda desgastado o flojo
</ptxt>
</ps>
<action>
<ptxt>
Reemplace el cojinete de la rueda.
</ptxt>
</action>
<ps>
<ptxt>
Neumático o llanta inestable
</ptxt>
</ps>
<action>
<ptxt>
Reemplace la rueda o el neumático.
</ptxt>
</action>
<ps>
<ptxt>
Los neumáticos no están inflados correctamente
</ptxt>
</ps>
<action>
<ptxt>
Ajuste la presión de inflado de los neumáticos.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

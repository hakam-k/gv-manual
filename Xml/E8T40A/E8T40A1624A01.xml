<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E8T40A1624A01" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
El motor se recalienta (el ventilador del radiador funciona) 
</title>
<diag type = "condition">
<condition>
<ptxt>
El motor se recalienta (el ventilador del radiador funciona) 
</ptxt>
</condition>
<ps>
<ptxt>
Correa de transmisión para accesorios floja o rota
</ptxt>
</ps>
<action>
<ptxt>
Revise la correa de transmisión para accesorios. 
<intxreftitleoff1 refid = "E8T40A1A26008"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
No hay suficiente refrigerante
</ptxt>
</ps>
<action>
<ptxt>
Compruebe el nivel del refrigerante y añada más si es necesario. 
<intxreftitleoff1 refid = "E8T40A1626002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Termostato defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Revise el termostato. 
<intxreftitleoff1 refid = "E8T40A1626020"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Bomba de agua defectuosa
</ptxt>
</ps>
<action>
<ptxt>
Verifique la bomba de agua. 
<intxreftitleoff1 refid = "E8T40A1626016"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Aletas del radiador sucias o dobladas
</ptxt>
</ps>
<action>
<ptxt>
Limpie o enderece. 
<intxreftitleoff1 refid = "E8T40A1626013"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Fugas de refrigerante en el sistema de refrigeración
</ptxt>
</ps>
<action>
<ptxt>
Verifique si el sistema de refrigeración tiene fugas. 
<intxreftitleoff1 refid = "E8T40A1626003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Radiador obstruido
</ptxt>
</ps>
<action>
<ptxt>
Sustituya el radiador. 
<intxreftitleoff1 refid = "E8T40A1626014"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Tapón del radiador defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Revise el tapón del radiador. 
<intxreftitleoff1 refid = "E8T40A1626003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Arrastre de los frenos
</ptxt>
</ps>
<action>
<ptxt>
Compruebe los frenos. 
<intxreftitleoff1 refid = "E8T40A4104002"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Embrague resbaladizo
</ptxt>
</ps>
<action>
<ptxt>
Verifique el embrague. 
<intxreftitleoff1 refid = "E8T40A5304001"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Batería descargada
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione la batería. 
<intxreftitleoff1 refid = "E8T40A1A24001"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Generación deficiente de electricidad por parte del generador
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el generador. 
<intxreftitleoff1 refid = "E8T40A1A24003"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Sensor ECT defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el sensor ECT. 
<intxreftitleoff1 refid = "E8T40A1326010"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Motor del ventilador de refrigeración del radiador defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Revise el motor del ventilador de refrigeración del radiador. 
<intxreftitleoff1 refid = "E8T40A1626009"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
ECM defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Verifique el ECM y su circuito. 
<intxreftitleoff1 refid = "E8T40A1134063"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Cableado o conexión a masa defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Repare el cableado.
</ptxt>
</action>
<ps>
<ptxt>
Equipado con demasiadas piezas eléctricas opcionales
</ptxt>
</ps>
<action>
<ptxt>
Desmóntelas.
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>

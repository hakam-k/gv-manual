<?xml version="1.0" encoding="utf-8" ?><?xml-stylesheet type="text/xsl" href="..\config\suzuki.xsl" ?><!DOCTYPE suzuki [<!ENTITY % sdatacharaents SYSTEM "..\config\sdatacharaents.txt">%sdatacharaents;]><suzuki><manual>
<section sectnbr = "1F">
<servcat sctnbr = "01" scnbr = "06">
<configtype typenbr = "2">
<servinfotype sitcode = "4">
<servinfo servinfosgmlid = "E8T40A1624A02" svcinfotypenbr = "06" svcinfoqualnbr = "03">
<title>
El motor se recalienta (el ventilador del radiador no funciona)
</title>
<diag type = "condition">
<condition>
<ptxt>
El motor se recalienta (el ventilador del radiador no funciona)
</ptxt>
</condition>
<ps>
<ptxt>
Fusible fundido
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el fusible. 
</ptxt>
</action>
<ps>
<ptxt>
Relé del ventilador de refrigeración del radiador defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Revise el relé del ventilador de refrigeración del radiador. 
<intxreftitleoff1 refid = "E8T40A1626021"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Sensor ECT defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Inspeccione el sensor ECT. 
<intxreftitleoff1 refid = "E8T40A1326010"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Motor del ventilador de refrigeración del radiador defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Revise el motor del ventilador de refrigeración del radiador. 
<intxreftitleoff1 refid = "E8T40A1626009"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
<ps>
<ptxt>
Cableado o conexión a masa defectuosos
</ptxt>
</ps>
<action>
<ptxt>
Repare según sea necesario.
</ptxt>
</action>
<ps>
<ptxt>
ECM defectuoso
</ptxt>
</ps>
<action>
<ptxt>
Verifique el ECM y su circuito. 
<intxreftitleoff1 refid = "E8T40A1124063"><img src = "..\icon\intxreftitleoff.gif"/></intxreftitleoff1>
</ptxt>
</action>
</diag>
</servinfo>
</servinfotype>
</configtype>
</servcat>
</section></manual></suzuki>
